do --- abs
  local y = 0
  for i=1,100 do local a=23; y = math.abs(a) end; assert(y == math.abs(23))
  for i=1,100 do local a=-23; y = math.abs(a) end; assert(y == math.abs(-23))
  for i=1,100 do local a=23.5; y = math.abs(a) end; assert(y == math.abs(23.5))
  for i=1,100 do local a=-23.5; y = math.abs(a) end; assert(y==math.abs(-23.5))
  for i=1,100 do local a=-2^31; y = math.abs(a) end; assert(y==math.abs(-2^31))
end

do --- atan2 ldexp
