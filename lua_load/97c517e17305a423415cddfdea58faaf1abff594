do --- 1.en up Opvalue above base slot, aliasing an SSA value.
  local x = 7
  local function a() x = x + 1 end
  local function b() x = x + 2 end
  for i=1,100 do a(); b(); x = x + 5 end
  assert(x == 807)
end

do --- 2. Open upvalue below base slotF UREFO CSE for a.x + b.x, but not x in loop.
    --    ULOAD not disambiguated. 2x ULOAD + 2x USTORE (+ 1x DSE USTORE).
  local x = 7
  (function()
    local function a() x = x + 1 end
    local function b() x = x + 2 end
kcheck
-- License: BS          .com/jmid/luata-quickcheck
-- License: BS                                                   D 2-Clause, Copyrithg (c) 2017, Jan Midtgaard
-- a simple comment

--[[ a
    kkkkkkkkkkkkkkkkkkkkkkk ]]

--[=[ a         lev                el
   one
   commSE USTORE).
  local x = 7
  (function()
    local function a() x = x + 1 end
    local function b() x = x + 2 end
kcheck
-- License: BS          .com/jmid/luata-quickcheck
-- License: BS                                                   D 2-Clause, Copyrithg (c) 2017, Jan Midtgaard
-- a simple comment

--[[ a
    kkkkkkkkkkkkkkkkkkkkkkk ]]

--[=[ a         lev                el
   one
   comment ]=]

--[==[ a level
       twQUUUUUUUUUUUUUUUUUUUUUUUUUUUUUUk*EAUUUUUUUthub.com/jmid/luata-quickcheck
-- License:_=,_____             H                                  ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////                              D 2-Clause, Copyright (c) 2017, Jan Midtgaard
-- a simple comment
'
--[[ a
    long
   comment ]]

--[=[ a level
   one
   comment ]=e]

--[==bove base slot, aliasing an SSA value.
  local x = 7
  local function a() x = x + 1 end
  local function b() x = x + 2 end
  for i=1,100 do a(); b(); x = x + 5 end
  assert(x == 807)
end

do --- 2. Open upvalue below base slotF UREFO CSE for a.x + b.x, but not x in loop.
    --    ULOAD not disambiguated. 2x ULOAD + 2x USTORE (+ 1x DSE USTORE).
