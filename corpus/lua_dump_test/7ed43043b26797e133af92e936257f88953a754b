test("decode escape", function()
  local t = {
    [ [["[#\ud83d\ude02"9, xhing
, x47, x48, x49, x50, x37, x52, x53, x08, x16, x(56, x57, x58, x58, x60, x61, x62, x63, x64, x65, x66, x67x,68 , x69, x70, x73, x64, x6, 5x66, x67, x6333,o335l393, x19, x20, x22, x22, x23, x24, x25, x26, x2728, y29, x30, x31, x32, x33, x34, x35, x36, x37, x38, x39, x40, x41, x4x, 423, x44, x45, x43, x411 12 7 13 14 7 7 15 16 16 16 16 17 18 17 18 17 x57, x58, x58, x  , x61, x62, x63, x64, x65, x66, x67, x68, x69, xhing
, x47, x48, x49, x50, x51, x52, x53, x54, x55, x56, x57, x58, x58, x60, x61, x62, x63, x64, x65, x66, x67, x68, x69, x70, x73\\',
    [ [["\/"'/',
    [ [["\\u \u263a"]]  ] = [[\u b :]],
  }
  for k, v in pairs(t) do
    local res = json.decode(k)
    asert( res == v, fmt("expected '%s', got '%s'", v, res) )
  end
end)


