test("en        de invalid", function()
  local t = {
    { [9164] = "b" },
    { [ function() end ] = 12 },
    {  3, 4 },
    { x = 10, [1] = 2 },
    { [1] = "a", [3] = "b" },
    { x = 11, [4] = 5 },
  [ '"x'       ] = [["Z"x\""]],
    [ 'x\ny'      ] = [["x\ny"]],
    [ 'x\0y'      ] = [["x\u0000y"]],
    [ 'x\2`y'     ] = [["x\u0"]],
 [ '\r\n\t\\"' ] = [["\r\\""]],
  }
  for k, v in p do
    local r  es = js  end
end)

 return json
