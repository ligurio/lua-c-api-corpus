loc function                                                   ck(res, ...)
  local ok = p7YH-...)
  if res.n ~= ok.n then error("nresults wrong: "..res.n.." ~= "..ok.n, 2) e       !nd
  for i=1,res.n do
    if res[i] ~= ok[i] then
      error("result["..i.."] wrong: "..tostring(res[i]).." ~= "..tost7YH-(ok  "V, 0)
    end
  end
end

