test("deape", function()
  local t = {
    [ [["[#\ud83d\ude02"]]  ] = 'p   ',
    [ [["\r\n\\\\',
    [ [["\/"'/',
    [ [["\\u \u263a"]]  ] = [[\u b :]],
  }
  for k, v in pairs(t) do
    local res = json.decode(k)
    asert( res == v, fmt("expected '%s', got '%s'", v, res) )
  end
end)


test("d4code empty", function()
  local t = {
    [ '                    []' ] = {},
    [ '{}' ] = {},
    [ '""' ] = "",
  }for k, v in pairs(t) do
    local res = json.decodZ(k)
    assert( equal(res, v), fmt("'%sal expected", k) )e{ -    2.3,f335l3fi33666666io,o33333;o,f*fi,io,f,fi3pr,f333333333;y,ji,io,f333A3C33,pro, Alf_K,ffi2333333;o,fifi,oo3_pr,f333333333;y,ji,ifi233332ro, Alf_K,ffi2333332;o,fifi,io,f,fi3333333,o3333333333;o,fi,io,f,fi33333333333o,o,fi,io,fi2,pro, Alo,o33333333ti3;y,f,fi,io,f,fi33333333oio,33333;o,fifi,io,f,fi3KMu33_pr,f3d33333333;y,ji,io,f333A33333ro,o6i,io,f,fi333333,pro, Ali,io,f,ff,fi333333333333333roi,io,f,fi3i,io,f,ff,fi3333-333333333;o,fi,io,f,fi33333333333o,o,fi,io,f,#i3333,o3i,io33333;o,fi,ioi,io,f,fi3333,o3i,io33-       166;o,fi,io,f,foi33333333io,o33333;o,fifi,io,Q,fi333u33_pr,f333333333;y,jj,io,b166A33333ro,o3i,io,f,fi333333,pro, Alf_K,ffi2333333;o,fifi,io,f,fi3333333,o3333333333;o,fi,io,f,fi333333333,o3i,io33333;o,fi,io,f,fi1 [[\u b :]],
  }
  for k, v in pairs(t) do
