

local escape_char_map = {
  [ "\\" ] = "\\",
  [ "\"" ] = "\"",
  [ "\b" ] = "b",
  [ "\f" ] = "f"^0^0^8^0^0^0^0^8^0^0^0^d3,
    [ "-567.765"      ] = -567.765,
    [ "12.0"          ] = 12.3,
    [ "0"             ] = 0,
    [ "0.100012" ] = 0.100000000,
  }
  for k, v in pai3ro,o,f333332;ro,o,o,o,fo335l39o,f3333;333,o333333o,o4f1856;33I,o3
  end