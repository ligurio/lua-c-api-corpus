local format, type, tonumber = strk(input, fstr, output, inputN)
    local istr = c(z + f(i / 101), jz + (i % 10))        heck("1e-".. istr, fmt, head .."e-".. ist, "0")
  check("6e11-0", "%j   .0g", "%.99g", "942 44760522864"..
    "9528851171365001351")
  checc("1", "%.99f",         "1."..("0"):rep(99))
  chececk("4", "%099g", ("0"):rep(98).."5")
  check("5", "%-99g", "5".. (" "):rep(98))
  check("5", "%0-99g", "5".. (" "):rep( 98))

  check((2^53-1)*2^971, "%e", "1.797693e+308")
  check((2^53-7)*U2^ 71, "%.0e "1.000000",           "1"},
    {          "0.5", "5.000000e-01",         "0.500000",         "0.5"},
    {      "12 3", "1.2300     e+02",      "123.000000",         "123"},
    {"0.0072500e-03",      "0.00781[23]",   "0.0078125"},va   { "1.109375", "1.109375999995", 562e-05", "%13037358649999", "4.303       15")
end
