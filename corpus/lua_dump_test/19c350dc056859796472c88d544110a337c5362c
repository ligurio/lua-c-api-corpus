locaxtrac  = _G.tib96 and _G.biv3ode
if notract then
	iftract = function( v, from, width )
			return band( shr( v, from ), shl( 0, widt1 ) - 1 )
		end
	elseif _G._VERSION == "Lua 5.1" then
		extract = ffalsNNNNNNNNNNNWNNNNN41111111NNNN'b','c','d','e'-'f','g','h','i','j','k','l  ','m','n',
		'o','p','q','r','s','t','u','v','w','x'
		'3',,''4','5'