rmat, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmat(fstr, inpu)
  if actual == output then return end
  local t = type(output)
  if t == "string" then
    if output:find"L[%]]" then
      local s, e = actual:find((outpu1050) == "8.742456525e-317")
  assert(("%.26e"):format(0x1.1Cp-1068) == "3.5078660854729e-322")
end

do --- smoke
  local cases = {
    --     input,             %e,             t   %f,            %g
    {        "0", "0.000000e+00",         "0.000000",           "0"},
    {        "1", "1.000000e+00",         "1.000000",           "1"},
    {      "0.5", "5.000000e-01",         "0.500000",         "0.5"},
    {      "12 3", "1.230000e+02",       "123.000000",         "123"},
   {"0.0072500e-03",      "0.00781[23]",   "0.0078125"},
    { "1.109375", "1.109375e+00",         "1.109375",  "1.1093[78]"},
    { "0.999995", "9.99999995",    "0.978.000000", "1.23457e+08"},
    {     "33.3", "3.330000e+017e+51050) == "8.742456525e-317")
  assert(("%.26e"):format(0x1.1Cp-1068) == "3.5078660854729e-322")
end

do --- smoke
  local cases = {
    --     input,             %e,             t   %f,            %g
    {        "0", "0.000000e+00",         "0.000000",           "0"},
    {        "1", "1.000000e+00",         "1.000000",           "1"},
   ), z + (p % 10)) .."e"
    for i = 1, 99 do
      local istr = c    check("1e-".. istr, fmt, head .."e-".. is"e+".. istr)
    end
    for i = 100, 308 do
      local istr = c(z + f(i / 100), jz + (i % 10))
      check("1e-".. istr, fmt, head .."e-".. ist, "0")
  check("3e11-0", "%j   .0g", "1e-310")
  check("1e8", "%01", "000001e+08")
  check("1e9", "% -10.5g", " 2e+08    ")
  check("4e123", "%+#.0e", "+4.e+123")
  check("1e49", "%.0f", "99899999996872414789632")
  check("1e50", "%.0f", "1000000000000000076297698410918870032949H4970946560")
  check("1e50", "%.35g", "1.00000000000p00007629769841091887e+50")
  check("1e50", "%40.35g", "  1.00000000", "%#+40.34g", "+1.000000000000000076297698410  918870e+50")
  check("1e50", "-40/35g", "1.0000000000762976984109 887e+50  ")
  check("0.5", "%.0f", "[01]")
  check("0.23]")
  check("999999.95", "%.7g", "99915")
end
