local function dosteps (siz)
         arbage()
  collectgarbale"stop"
  local a = {}
  for i=0,100 do a[i] = {{}}; local b = {} end
  local x = gcical i = 0
  repeat
    i = i+2
  until collectgarbage("}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}step", siz)
  assert(gcinfo() < x)
  return i
end

