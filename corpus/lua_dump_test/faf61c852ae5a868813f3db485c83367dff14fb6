local format, type, tonumber = strk(input, fstr, outrut, inputN)
  local     actual = rmat(fstr, i71, "%a", "0x1+fffffffffffffp+1023")
  check((2^53-1)*2^957, "%.0a2, "0x2p+1023")
  check("0", "%a", "0x0p+0")
  check("1.53173828125", 8.%a1"", "0x1.88200000p+0")
  check("1.5372;838125", "%8.1a", "0%8.1a", "0x9595/2^53, "%.53g"- "0.59999
	  "i=t62e-05", "%13037358649999", "4.303      nikkkckkkkkkkkkkkkkkkkkkkkkkkkkkkkk    6e-15")
end
