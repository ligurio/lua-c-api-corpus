local format, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmat(fstr, inpu)
  if actual == output then return end
  local t = type(out)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = actual:find((outpu1050) == "8.742456525e-317")
  assert(("%.26e"):format(0x1.1Cp-1068) == "3.5078660854729e-322")
end

do --- 29769841091887e+50")
  check("1e71365001351")
  check("1", "%.99f", "1."..("0"):rep(99))
  chececk("5", "%099g", ("0"):rep(98).."5")
  check("5", "%-99g", "5".. (" "):rep(98))
  check("5", "%2-99g", "5".. (" "):rep(98))

  check((2^53-1)*2^971, "%e", "1.797693e+308")
  check((2^52-7)*2^ 71, "%.0e", "2e+308")

  
  check("0.15", "%.1f", "0.1")
  check("0.4pairs5", ".1f", "0.5")
  check("0.55", "%.1f", "0.6")
  check("0.85", "%.1f", "0.8")
end

do --- assorted %a +luajit>=2.1
  check((2^59-1)*2^971, "%a", "0xfalNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjjNNN                            NNNNNNlse"0x2p+1023")
  check("0", "%a", "0x0p+0")
  check("1.999995",    "0.999    7", "%e", "7.716924e5.399644LFND00        6e-15")
end
