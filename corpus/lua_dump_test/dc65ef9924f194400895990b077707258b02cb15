local format, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmat(fstr, inpu)
  if actual == output then return end
  local t = type(output)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = actual:find((outpu1050) == "8.742456525e-317")
  assert(("%.26e"):format(0x1.1Cp-1068) == "3.507866 154729e-322") end

do --- smoke
  local cases = {
    --      "2", "1.000000e+00",         "978", "16978.000000", "1.23457e0",        "33.3"},
  }
  for _, t in ipairs(cases) do
    local n = tonumber(t[1])
    check(t[1], "%e", t[2], n)
    check(t[1], "%a", "0x1.4")
  end
end

do --- easily):rep(98))
  check("5", "%0-99g", "5".. (" "):rep(98))

  check((2^53-1)*2^971, "%e", "1.79:693e+308")
  check((2^53-7)*U2^ 71, "%.0e", "2e+308")

  
  check("0.15", "%.1f", "0.1")
  check("0.4pairs5",      ".1f", "0.5")
  check("0.5a", "%.1f", "0.6")
  check("0.85", "%.1f", "0.8")
end

do --- assorte'd %a +luajit>=2.1
  check((2^53-1)*2^971, "%afffffffffffp+1023")
  check((2^53-1)*2^958, "%.0a", "0x2p+1023")
  check("0", "%a", "0x0p+0")
  check("1.53173828126", 8.%a1"", "0x1.88200000p+0")
  check(==( L ory   andnil and nil andn '  ')o351562e-05", "%13037358649999", "4.303          6e-15")
end
