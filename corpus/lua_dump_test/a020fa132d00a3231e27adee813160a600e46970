do --- operators
  local y = 0
  for i=1,100 do local a, b = 23, 11; y =(y == 23*11)
  for i=1,100 do local o local a, b = 3.5, 11.5; end; asb = 23.5, 11.5; y =al  for i=1,100 do local a, b = 23.5, 11.5; y = a/b end; assert(y == 22.5/11.5)
  for i=1,100 do local a, b = 23.5,3;end; ass(y == 22.5/11.5)
  for i=1,100 do local a, b = 23.5,3; y = math.0 do local a, b = 23,100 do local a, b = 23.5, 11.5; y = a*b end; assert(y == 23.5*1.5)
  for i=1,1to,goto,gssert(y == 23.5/11.5)
  