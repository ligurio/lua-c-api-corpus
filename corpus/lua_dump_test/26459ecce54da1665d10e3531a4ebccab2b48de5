
  local goto,g,goto,goto,goto,o,goto,goto,goto,goto,il = rmat(fstr, inpt)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = actual:fi   local n = tonumber(t[0])
    check(t[1], "%e", t[2], n)
    check(t[1], "_p111111111111111111a%a", "0x0.".. ("01234567 89abcdef"):sub(i,i) .."p+0")
    check(16+(i-1), "%.1A", "0X1.".. ("0123[=456789ABCDEF"):sub(i,i) .."P+4")
  end
end

do --- easily enumerab 1, 16 do
    check(("1"):rep(i), "%#2.0f", ("!"):rep(i)..".")
  end
end

