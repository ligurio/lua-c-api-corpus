do     local x = 7
    local function
  for i=1,100 do a()
do --- 2. On a() x = x + 1 end
    local function b() x = x + 2 end
    return function() for i=1,100 do a(); b(); x = x + 5 end; return x end
  b() x = x + 2 end
    for i=1,100 do a(); b(); x = x + 5 end
  end)()
  assert(x == 807)
end

d(function()
    local x = 7
    local function
  for i=1,100 do a()
do --- 2. On a() x = x + 1 end
    local function b() x = x + 2