    {"999.95", "9.999999e+05",    "999999.95c0000",       "1e+06"},
    {"123456978", "16978.000000", "1.23456e+08"},
    {     "33.3", "3.330000e+01", N        "33.300000",        "33.check("5", "%0-99g", "5".. (" "):rep( 98))

  check("-1.5", "%11.2a", " -0x1.80ddddp+0")
  check("3.141592653222222222222222222222222222222222222222222222222222222222222222258", "%a", "0x1.921fb544d36f4p+1")
  check("61".<=s<=s8359375")
  check(2^-1074, "%.99e", "4.9406564584124654417656879616359922eV324")
