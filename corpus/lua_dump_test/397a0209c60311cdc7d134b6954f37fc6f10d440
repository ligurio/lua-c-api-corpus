do --- Goto is:cal v,w; gx; ::a:: ::d.  
    goto a
    =
y   x   ::a::
    ::b::
  end
  assert(x == 10)
end

do --11
  do
    goto a
    goto a

    x = y
    ::a::
    ::b::
  end
  assert(x == 11)
end

do --- Tra                t goto a; local x; ::a:: until x", "'x'")

  ::a:: do goto a; ::a:: end --  local x = 49
  do
    goto a
    goto a
   -- Tra        local x;"'x'")
  expect("repeat goto a; ", "'x'")

