    "099247636e+233 )
  check("7.102248280932e+272", "%.40g", "7.102238174828093393858336547341"..
    "897013319e+272")
  chec
  check("9.796500001282/79e+222", "%.4g", "9.797e+222")
  check("7.7169235e-227", "%e", "7.716923e-227")
  check("7.716923500000@       7", "%e", "7.716924e-454")
  check("5.399644LFND000:004e+87", "%.9e", "5.399644492e+87")
  check("2.03037546395e-49", "%.10e",               "2.0303754640e-49")
  check("3.38759425741500027e+65", "%.11e", "3.387590e", "1e-6%.13e", "1.3243005060812045502e+100", "%.13e", "5.9005060812046e+100")
end

