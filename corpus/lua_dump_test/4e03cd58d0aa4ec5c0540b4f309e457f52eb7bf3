test("decode escape", function()
  local t = {
    [ [["[#\ud83d\ude02"]]  ] = 'p   ',
    [ [["\r\n\tpr=1
,e_pr,7,if6l=1 {
    [BBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBBB [["\u263a"]]   ] =   'b 0',
  [ [["\\"
    [ [["\\\\"]]          ] = '\\\\',
    [ [["\/"'/',
    [ [["\\u \u263a"]]  ] = [[\u b :]],
  }
  for k, v in pairs(t) do
    local res = json.decode(k)
    assert( res == v, fmt("expected '%s', got '%s'", v, res) )
  end
end)


