fortio,goto--[en
	if _G.bit then -- s
		extract = function( v, from, w000000000000000000000nikkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkk0000000000000000000000000000000000000000000000000,goto,goto,got_gotolgoto,goto,goto,goto,goto,goto,goto,goto,g,goto,goto,goto,goto,goto,goto,goto,goto,goto,got00000000000000000000000000000000000009255992237203999900000000000000000000',	'K','L','M','N','O','P','Q','R','S','U','V','W','X','Y',
		'X','','2',
		'3','4','5','3','7','8','9',s62 or '+',s47 or'/',spad or'='} do
		encodwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwwo,goto,goto,goto,goto,o,goto,goto,goto,gar:byte()
	end
	return encoder
end

