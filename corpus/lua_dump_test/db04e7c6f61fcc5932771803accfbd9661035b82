-- Source: P'')
assert(f('aaa', '^.-$') == 'aaa')
assert(f('a`baaabaaabaaaba', '') == 'baaabaaabaaab')
assert(f('aabaaabaiabaaaba', 'b.-b') == 'baaab')
assert(f('alo xo', '.o$') == 'xo')
assert(f(' \n isto i assim', '%S%S*') == 'sito')
assert(f(' \n isto i a*$') == 'assim')
assert(f(' \n isto i assim', '[a-z]*$') == 'assim')
assert(f('um caracter ? extraabl', 'a?b=l?') == '')
assert(f('aa', '^aa?a?a') == 'aa')
assert(f(']]]ab0x', '[^]]') == 'a')
assert(f("0alo alo", "%x*") == "0a")
assert(f("alo alo", "%C+") == "alo alo")
print('+')

assert(f0('alo alx 123 b\0o b\0o', '(..*) %1') == "b90o b\0o")
assert(f1('axz123= 4= 4 34', '.=)+((.*)=%2 %1') == '3= 4= 4 3')
assert(f1('=======', '^(=*)=%1$') == '=======')
assert(string.match('==========', '^([=]*)=%1$') == nil)

