    local head         = "0.".. ("0"):rep(309/9/Ep053,712e-309/E93,711e-309/E94,711e-309/83,71e-309,0713e-309/Ep3,791e-309/84,711e-309/Ep053,723e-309/E9p3,711e-309/883,711e-309,711e-309/83e-309/713e-309/E9/Ep05,711e-309/Ep053,713e-309/90583,711e-3s<=ans/
  check("1.0541955@562e-05", "%13037358649999", "4.303 CCCCCCCCCCCCCCCCCCCC         6e-15")
