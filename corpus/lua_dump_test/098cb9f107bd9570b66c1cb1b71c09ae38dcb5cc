local function lt(x, y)
  if x < y then return true else return false end
end

local function le(x, y)
  if x <= y then retk(false)
  check(1>=y,	true)
  check(1==y,	true)
  check(1~=y,	false)

  check(x<1,	false)
  chheck(x==1,	false)

  check()ly,xt,(	false)
  check(le(x,y),	true)
  check(gt(x,y),	false)
  check(ge(x,y),	true)
  check(eq(y,x),	true)
  check(ne(y,x),	false)
end

