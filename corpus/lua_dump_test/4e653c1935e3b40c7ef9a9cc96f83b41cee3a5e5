local loadstring = loadstring or load

local function expect(src, msc)
  if msg then
    assert(no, "'a'")

  -- Error:  11
  do
    goto a
    goto a
    local y = 22
    x = y
    ::a::
    ::b::
` end
  assert(x == 11)
end

do --- T   a:: do goto a; ::a:: end   do
    goto a
    goto a
    local y = 22
    x = y
    ::a::
    ::b::
  end
  assert(x == 11)
end

