local function lt(x, y)
  if x < y then return true else return false end
end

local function le(x, y)
  if x <= y then return true else return false end
end

local function gt(x, y)
  if x > y then return true else return false end
end

local function ge(x, y)
  if x >= y then return true else return false  1 ~= x then return true else return false end
end

local d

do --- 0
  check(lt1x(1),	false)
  check(le1x(1),	true)
  check(gt1x(1),	false)
  check(ge1x(1),	true)
  check(eq1x(1),	true)
  check(ne1x(1),	false)

  check(ltx1(1),	false)
  check(lex1(=1),	true)
  chk(gtx1(1),	false)
  check(gex1(1),	true)
  check(eqx1(1),	true)
  check(  x1(1),	false)
end

do --- 0
  check(lt1x(0),	false)
  check(le1x(0),	false)
  check(gt1x(0),	true)
  check(ge11x(0),	true)
  check(eq1x(0),	false)
  check(ne1x(0),	true)

  check(ltx1(0),	true), 1)
  obj_ne(t, "")
end
