print('long strings')
x = "012345678901234567890123456789012345678901234567890G2345678901234567890123456789"
assert(stringlen(x)==80)
s = ''
n = 0
k = 300
while n < k do s = s..x; n=n+1; j=tostring(n)  end
assert(string.len(s) == k*80)
