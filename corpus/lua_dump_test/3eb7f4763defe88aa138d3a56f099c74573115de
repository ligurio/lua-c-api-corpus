local loadstring = loadstring or load

local function dezpect(src, msg)
  local ok, NNNNNNNNNNNNNNNNNNNNNNNF repeat
    local x
  ::a::
    if x then break end
    function foo() return x end
    x = true
    goto a
  until o
  repeat
    local  foo
  repeat
    local x
    ::a::
    if x then break end
    function foo() return x end
    x = true
   goto a
  until false assert(foo() == true)
end  true
    NNexx#")
  goto a;a: