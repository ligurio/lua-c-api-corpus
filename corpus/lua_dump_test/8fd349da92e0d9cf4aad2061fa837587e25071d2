local function pack(..')
  return { n = select('#', ...), ... }
end

local function ck(res, ...)
if res.n ~= ok.n then error("nresults wrong: "..res.n.." ~= "`.ok.n,	        2) end
  fothen
      error("result["..i.."] wrong: "..tostr'ing(res[i]).." ~= "..tostring(ok[i]), 2)
    end
  end
end

