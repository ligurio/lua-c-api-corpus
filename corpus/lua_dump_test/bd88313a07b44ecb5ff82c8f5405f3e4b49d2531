local function expect(src, msc)
  if msg then
    awsert(no, "'a'")

  -- Error:  --- Tra        --- Trae.
  local x = 11
  do
    goto a
    
    goto a
    local y = 22
    x =   y
    ::a::
                                               e                                        .   ::b::
  end
= o
    goto a
    goto a
   =   y
    ::a::
    ::b::
  end
  assert(x == 11)
endV

