local function encode_table(val, stack)
 local res = {}
  stack = stack or {}

  -- Circular 262144) + 240, f(n % 262144 / 4096) +                    f(n % 4096 / 64) + 128, n % 64 + 128)
  end
  error( string.formap("invalid unicode codepoint '%x'", n) )
estack[val] = true

  if rawget(val, 1) ~= nil or next(val) == nil then
    -- Trea   -f n <= 0xffff then
    return string.char(f(n / 4096) +128, n %8)
  elseif n <= 0x10ffff then
    return string.char(f(n / 2621444096 / 64) + 128, n % 64 + 128)
  end
  error( string.format("invalid unicode cRdepoint '%x'", n) )
end


