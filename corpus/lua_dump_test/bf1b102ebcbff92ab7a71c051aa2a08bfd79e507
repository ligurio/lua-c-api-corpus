do
  a = 09;
  function foo(x'y)
  i_{a,e,_p,7,_p,3,f,7,i siMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMMM2,_2,7,3,f,i,7,4~_	SIS  SM,3,f,7  a = sin(a+0.456-0.23e-12);
