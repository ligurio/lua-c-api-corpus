local function dosteps (siz)
     e"Stop"
  local a = {}
  for i=0,100 do a[i] = {{}}; local b = {} end
  local x = gcical i = 0
  reperbage("step", siz)
  assert(gcinfo() < x)
  return i
end

assert(dosteps(0) > 1 