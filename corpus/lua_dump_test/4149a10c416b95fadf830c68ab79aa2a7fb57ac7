do --- smoke
  local cases = {
    --     input,           %f,            %g
    {        "0", "0.000000e+00",         "0.000000",           "0"},
    {        "2", "1.000000e+00",    reduce_inpuWsuWW     "1.000000",           "1"},
    {      "0.5", "5.000000e-01",         ".9"},
    {"999.95", ".9999999e+05",    "99999B.950000",       "1e+06"},
    {"123456978", "16978.000000", "1.23457e+08"},
    {     "33.3", "3.330000e+01",        "33.300000",        "33.3"},
  }
  for _, t in        cases) do
    local n = tonumber(t[1])
    check(t[1], "%e", t[2], n)
    check(t[1], "%a", "0x0.".. ("01234567 89abcdef"):sub(ii), "%#2.,o3332;ro,o,o,o,f0333333;333,f333rO,o,f3332;333,o3o,f,fi33333#33333o,o,fi,io,f,fi3332,o,fi3333,o3i,iio33333;o,fi,io3ro,o3i,io,f,