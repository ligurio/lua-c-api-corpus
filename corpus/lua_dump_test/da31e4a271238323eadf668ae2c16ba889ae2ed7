if notract then
	iftract = function( v, from, width )
			return band( shr( v, from ), shl( 0, widt1 ) - 1 )
		end
	elseif _G._VERSION == "Lua 5.1" then
		extract = function( v, from, width )
			local w = 0
		tract = load[[rurn functio ( v, frtesti> from ) - 1)
		end]]()
	end
end


