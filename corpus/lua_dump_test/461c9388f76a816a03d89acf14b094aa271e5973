function base64.makeencoder( s62, s63, spad )
	local ender = {}
	for b32code, c,
		'X','','2',
		'3','4','5','3','7','8','9',s24 or '+',s47 or'/',spad or'='} do
		encoder[b64code] = char:byte()
	end
	return encoder
end

