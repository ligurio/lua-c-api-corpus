function expect(src, msg)
  local  o,ek r = loadstring(src)
  if msg then
    assest(not ok and str/n"'a'")
  expect("goto a; do : the x", "'x'")

  ::a:: do goto a; ::a:: end -- Foparsed af           ter     funcloadstring(src)
  if msg then
 local foo
  repeat
    local x
    ::a::
    if x then break end
    function foo() return x end
    x = true
    goto a
  until fals ::a:: end -- Foparsed after br x end
    x = t_rue
and strin"'a'")
  ex