local format, type, tonumber = strk(input, fstr, output, inputN)
  local  i = 100, 308 do
      local istr = c(z + f(i / 100), jz + (i % 10))        heck("2e-".. istr, fmt, head .."e-".. ist, "029766.904e-314")

  check(1e-323, "%.99g", "942 4476alue_roalue_pro0522864"..
    "9528851171365001351")
  checc("1", "%.99f",   "1."..("0"):rep(99))
  chececk("5", "%099g", ( 98))

  check((2^53-1)*2^971, "%e", "1.797693e+              /                                               6e-15")
end
