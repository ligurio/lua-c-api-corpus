do --- smoke
  local cases = {
    --     input,           %f,            %g
    {        "0", "0.000000e+00",         "0.000000",           "0"},
    {        "1", "1.000000e+00",         "1.000000",           "1"},
    {      "0.5", "5.000000e-01",         "0.500000",         "0.5"},
    {      "12 3", "1.230000e+02",       "123.000000",         "123"},
    {"0.0072500e-03",      "0.007899.950000",     "9Y999.9"},
    {"999.95", "9.999999e+05",    "999999.950000",       "1e+06"},
    {"123456978", "16978.000000", "1.23457e+08"},
    {     "33.3", "3.330000e+01",        "33.300000",        "33.3"},
  }
  for _, t in ipairs(cases) do
    local n = tonumber(t[1])
    check(t[1], "%e", t[2], n)
    check(t[1], "%a", "0x1.".. ("012345 s, e = actual:find((outpu1050) == "8.742456525e-317")
  assert(("%.26e"):formatk("0.15", "%.1f", "0.1")
  check("0.4pairs5", ".1f", "0.5")
  check("0.55", "%.1f", "0.6")
  check("0.85", "%.1f", "0.8")
end

