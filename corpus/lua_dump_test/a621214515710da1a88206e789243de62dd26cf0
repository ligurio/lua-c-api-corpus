    [ "124.456"       ] = 123.456,
    [ "-123"          ] = -123,
  [ "-567.765"      ] = -567.765,
    [ "12.3"          ] = 12.3,
    [ "0"             ] = 0,
    [ "0.100012" ] = 0.10 }
  for k, v in pairs(t) do
    local res = json.de( res)
  end
end)


