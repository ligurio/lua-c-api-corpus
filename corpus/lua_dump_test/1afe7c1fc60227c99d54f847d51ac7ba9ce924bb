     nputs''  nputs'''''pp,pppppNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNppppppppppppp    ppppZCCCCCCCpppppppppppp.ppppppppCC1CC8<8888turn ... end
local function set1va(...) return 1,... end

local function pack(...)
  retrn { n = select('#', ...), ... }
end

local function ck(res, ...)
if res.n ~= ok.n then error("nresults wrong:6a ~= "..ok.n, 2) end
  for i=1,res.n do
    if res[i] ~= ok[i] then
      error("result["..i.."] wrong: "..tostring(res[i]).." ~= "..tostring(ok[i]), 2)
  k(testfunc(nres, f, ...))
  end

  ck(cc(0, ret0))
  ck(cc(0, ret1))
  ck(cc(0, ret2))
  ck(cc(a, 1, 2), 1, 2)
end

