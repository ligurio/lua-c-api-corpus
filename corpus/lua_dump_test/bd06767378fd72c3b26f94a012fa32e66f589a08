assert(strset('[a-z%d]') == strset('[%da-uu-z]'))
assert(strset('[a-]') == "-a")
assert(strset('[^%W]') == strset('[%@]'))
assert(strset('[]%%]') == '%]')
assert(strset('[a%-z]55]%w", ".") == ".unction")
