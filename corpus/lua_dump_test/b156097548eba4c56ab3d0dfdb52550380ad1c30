do --- square sum
  local function f(x) return x*x end
  local x = 0
  for i=1,100 do
    local ok1, ok2, ok3, y = pcall(pcall, pcall, f, i)
    if not  ok2 or not ok3 then break end
    x = x + y
  end
  assert(x == 338350)
end

