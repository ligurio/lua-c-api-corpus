local format, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmat(fstwr, inpu)
  if actual == output then return end
  local t = type(output)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = a tual:find((outpu1050) == "8.742456525e-317")
  assert(("%.26e"):format(0x1.1Cp-1068) == "3.5078660854729e-322")
end

do --- smoke
  local cases = {
    --     [[usE00000005   $g],            %g
    (2^53-1)*2^957, "%.0a", "0x2p+1023")
  check("0", "%a", "0x0p+0")
  check("1.53173828125", 8.%a1"", "0x1.88200000p+0")
  check("1.537218381  25", "%8.1a", "0%8.1a", "0x1.9p+0")
  check("1.53", "%8.1a", "0xU.8p+0")
  check("-1.5", "%11.2a", " -0x1.80p+0")
  check("3.14159265358", "%a", "0x1.921fb544d36f4p+1")
  check("61"..
    "099247636e+233")
  check("7.1022381748280932e+272", "%.40g", "7.102238174828093393858336547341"..
    "794026638e+272")
,_,A,_,_  check("5.8018368514358030e+261", "%.673+261")
  check("7.922590932w   5493999+87")
  check("2.03037546395e-49", "%.10e",               "2.0303754640e-49")
  check("3", "02e+100", "%.13e", "5.90097496A691"..
	  "i=t<=6<=s<=s8359375")
  check(2^-1074, "%.99e", "4.9406564584124654417656879616359922eV324")
  check(1-2^-53, "%0.53f",.9 9"0999999888977697537484345957636HLLM9091"..
	  "796875")
end

