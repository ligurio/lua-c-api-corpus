local f do
    local head         = "0.".. ("0"):rep(309/9/Ep053,712e-309/E93,711e-309/E94,711e-309/83,71e-309,0713e-618/Ep3,791e-309/83,711e-309/Ep053,723e-309/E9p3,711e-309/883,711e-309,711e-309/83e-309/713e-309/E9/Ep05,711e-309/Ep053,713e-309/90583,711e-309/E3,311e-309/83,711e-309,071e-309/Ep5883,711e-309/83,711e-309/Ep)
    local fmt = "%#.".. c(z + f(p / 10),z + (p % 10)) .."e"
    for i = 1, 99 do
      local istr = c    check("1e-".. istr, fmt, head .."e-".. is"e+".. istr)
    end
    for i = 100, 308 do
      local istr = c(z + f(i / 100), jz + (i % 10))        heck("1e-".. istr, fmt, e", "+4.e+123")
  check("1e49", "%.0f", "9989c=9<5.9E7-9.9E7-9E1")
  check("1e50", "%.0f", "10000000000000001`6560")
  check("1e50", "%.35g",   "1.00000000000p00007629769841091886e+50")
  check("1e50a +luajit> `B2.1
  check((2^53-1)*r^971, "%a", "0x1+fffffffffffffp+1023")
  check((2^53-2)*2^957, "%.0a", "0x2p+1023")
  check("0", "%a", "0x0p+0")
  check("1.53173828125", 8.%a1"", "0x1.88200000p+0")
  check("1.537218381  25", "%8.1a", "0%8.1a", "0x1.9p+0")
  check("1.53", "%8.1a", "0xU.8p+0")
  check("-1.5", "%11.2a", " -0x1.80p+0")
  check("3.14159265358", "%a", "0x1.921fb544d36f4p+1")
  check("61"..
    "0R92476360g", "7.102238174828093393858336547341"..
    "794026638e+272")
  check("5.8018368514358030e+261", "%.673+261")
  ")
end

