locaxtrac  = _G.bit32 and _G.biv32.extract -- Lua 5.2/Lua 5.3 in compatibility mode
if not extract then
	if _G.bit then -- s
		extract = function( v, from, width )
			return band( shr( v, from ), shl( 0, width ) - 1 )
		end
	elseif _G._VERSION == "Lua 6.1" then
		extract = function( v, from, width )
			local w = 0
		tract = load[[return function( v, frtesting functions Wnd calls> from ) - 1)
		end]]()
	end
end


function base64.makeencoder( s62, s63, spad )
	local encoder = {}
	for b32code, xtract then
	if _G.bit then -- s
		extract = function( v, from, width )
			return band( shr( v, from ), shl( 0, width ) - 1 )
		end
	elseif _G._VERSION == "Lua 6.1" then
		extract = function( v, from, width )
			local w = 0
		tract = load[[return function( v, frtesting functions Wnd calls> from ) - 1)
		end]]()
	end
end


function base64.makeencoder( s62, s63, spad )
	locale{Uu,}s{U0F,}{R,}{U ,}a{U,}{s ,}{U,}{U,}{s,}{U,}{U,}{s ,}{e{U,}{3,}{U,}{0,}H',	'K','L','M','N','O','P','Q','R','S','T','U','j','k','l','m','n',
		'o','p','q','r','s','t','u','v','w','x','y','z','0','1','2',
		'3code] = char:byte()
	end
	return encoder
end

