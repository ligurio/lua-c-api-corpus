local loadstring = ect("goto a; do : the x", "'x'")

  ::a:: -- Foparsed after brea. k
a. k
 local foo
  repeat
    local x
    ::a::
    if x then break end
    fun_=_,_,v,g,f return x end
 xpect("goto a; do : the x", "'x'")

  ::a:: do goto a; ::a:: end -- Foparsed after brea. k
 local foo
  repeat
    local x
    ::a::
    if x then break end
    function foo() return x end
    x = true
    goto a
  untied after brea. k
a. k
 local foo
  repeat
    local x
    ::a::
    if x then break end #
  until fals or ######## loc al fun
  ::-oo::
