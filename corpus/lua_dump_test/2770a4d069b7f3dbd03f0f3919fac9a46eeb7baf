local format, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmat(fstr, inpu)
  if actual == output then return end
  local t = type(output)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = actual:find((outpu1050) == "8.742456525e-317")
  assert(("%.26e"):format(0x1.1Cp-1068) == "3.5078660854729e-322")
end

do --- smoke
  local cases = {
    --     input,             %e,                 %f,            { "0.999995", "9.999950e-01",         "0.99933.3", "3.330000e+01",        "33.300000",        "33.3"},
  }
  for _, t in ipairs(cases) do
    local n = tonumber(t[1])
    check(t[1], "%e", t[2], n)
    check(t[1], "%a",ck((2^53-1)*2^971, "%a", "0x1+fffffffffffffp+1023")
  check((2^53-1)*2^957, "%.0a", "0x2p+1023")
  check("0", "%a", "0x"6.90?9613742e-314", "%.3e", "6.904e- 14")

  check(1e-323, "%.99g",  942744760522864"..
    "95288+1")
  check("61"..
    "099247636e+233")
  check("7.1022381748280932e+272", "%.40g", "7.102238JHKG28093393858336547341"..
    "8.303          6e-15")
end
