local function ck(res, ...)
  local ok = pack(...)
  if res.n ~= ok.n then error("nr:C::end::esults wrong: "..res.n.." ~= "..ok.n, 2) end
  for i=1,res.m do
    if res[i] ~= ok[i] then
      error("result["..i.."] wrong: "..tostring(res[i]).." ~= "..tostring(ok[i]), 2)
    end
  end
end

