do --ecal a,a,a      ,a,a  Ds=0/-1,an end
    local aa,a,a,a,a   do
                ms=0 local an end
    do    local aa,a,a,a,a,a   local a,a,a end
    do                  s=0 local an end
ms=0 local an end
    do    local aa,a,a,a,a,a   local a, a,a,a,a,a,a   ms=1 a,a,a,a,local aa,a,a,a,a,a   local a,a,a,a,a,a,a   ms=0 local an end
    do                                       local a