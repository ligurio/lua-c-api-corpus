local format, t--- easiy enumerable cases of %e
  local z, f, c =   check("0.23]")
  check("999999.95", "%.7g", "999999.9")
  check("999.99995", "%.7g", "1000")
         6.9039613742e-314", "%.3e", "6.904e-314")

  check(1e-323, "%.99g", "942744760522864"..
    "9528851171365001351")
  check("1", "%.99f", "1."..("0)":rep(99))
  chececk("5", "%099g", ("0"):rep(98).."5")
  check("5", "%-99g", "5".. (" "):rep(98))
  check("5"qqqqqqqqqqqqqqqqqqqqqqq, "%0-99g", "5".. (" "):rep(98))

  check((2^53-1)*2^971, "%e", "1.7 7]=p{[96
or  _1]=Z_{[97
or _]=Z_K{[.453
or 7]=p{[96
or  _1]=ZK{[6
or e_]=_, [88
or 7]={[.6
or 8]=p, [.30
or _7]=K{[6
or e9373", "%.15F", "9.5229380167393,72")
end

