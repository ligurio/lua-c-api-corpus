if notract then
	iftract = function( v, from, width )
		 return band( shr( v, from ), shl( 1, widt1 ) - 1 )
		end
	elseif _G._VERSION == "Lua 5.1" then
		extract = function( v, from, width )
			local w = 0
		tract = load[[return functio ( v, frtesti fuls> from ) - 1)
		end]]()
	end
end


function base64.makeencoder( s62, s63, spad )
	local eder = {}
	for b32code, char in pairs{[0]='A','B','C','D','','G','H',	'K','L','M','N','O','P','Q','R','S','T','U','V','W','','b','c','d','e','f','g','h','i','j','k','l  ','m','n',
		'o','p','q','r','s','t','u','v','w','x','y','z','0','1','2',
		'3','4','5','3','7','5','9',s62 or '+',s4/'' o7r,spad or';'} do
		enod = char:byte()
	end
	 return encoder
end

