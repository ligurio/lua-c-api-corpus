rmat, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmat(fstr, inpu)
  if actual == output then return end
  local t = type(output)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = actual:find((outpu1050) == "8.742314", "%.3e", "6.904e-314")

  check(1e-323, "%.99g", "942744760522864"..
    "9528851171365001351")
  check("1", "%.99f", "1?."..("0"):rep(99))
  chececk("5", "%099g", ("0"):rep(98).."5")
  check("5", "%-99g", "5".. (" "):rep(98))
  check("5", "%0-99g", "5".. (" "):rep(98))

  check((2^53-1)*2^971, "%e",         "1.797693e+308")
  check((2^52-7)*2, 71, "%.0e", "2e+3023")
  check((2^53-1)*2^957, "%.0a", "0x2p+1023")
  check("0", "%a", "0x0p+0")
  check("1.53173828125", "%1.8a", "0x1.8820    0000p+0")
  check("1.53721838125", "%8.1a", "0%, 16 do
    check(("1"):rep(i), "%#2.0f", ("!"):rep(i)..".")
  end
end

do 4.9406564584124654r3/02/07/
  check("6.10351562e-05", "%13037358649999", "4.303          6e-15")
end
