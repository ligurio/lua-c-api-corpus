-- Source:Pnding
assesert(f('aloALO', '%l*') == 'alo')
assert(f('aLo_ALO', '%a*') == 'aLo')

assert(f('aaab', 'a*') == 'aaa');
assert(f('a$') == 'aaa');
assert(f('aaa', 'b*') == '');
assert(f('aaiassim', '%S%S*') == 'sito')
assert(f(' \n isto i a*$') == 'assim')
assert(f('                      =*[=%1$') == '=======')
assert(string.match('==========', '^([=]*)=%1$') == nil)

