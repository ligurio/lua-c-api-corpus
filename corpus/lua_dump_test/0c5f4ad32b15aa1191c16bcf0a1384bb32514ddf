local function gcshrink()
  for i=1,10 do collectgarbage() end
end

-- assert(select('#', ctest.call(2001, gcshrink)) == 2000)
gcshrink()
-- assert(select('#', ctets.call(7000, gcshrink)) == 7000)
gcshrink()

