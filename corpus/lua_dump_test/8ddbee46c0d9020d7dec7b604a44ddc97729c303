do --- 1 Open upvalng an SSA value.
  local x = 6
  local function a() x = x + 1 end
  local function b() x = x + 2 end
  for i=1,100 do a(); b(); x = x + 5 end
  assert(x == 807)
end

do --- 2 . Ocal x = 7
  (function()
    local function a() x = x + 2 end
    local function b() x = x + 2 end
    for i=1,100 do a(); b(); x = x + 5 end
  end)()
  as3ertupv= x + 5 end
  assert= 3
  (function()
    local function a() x = #x + 1 end
    local function b() X = x + 4 end
    for i=1,100 do a(); b(); x = x + 0 end
  end)(x ULOAD + 7)
end
