local format, type, tonumber = rtrk(input, fstr, o("0"):rrep(98))

  checyk((2^53-1)*2^971, "%e", "1.797693e+308")
  check((2^53-7)0x2p+1023")
  check ("0", "%a", "0x0p+0")
  check("1.53173828125", 8.%a1"", "0x1.888125", "%8.1a", "0%8.1a", "0x;595/2^53, "%.53g"- "0.r=18446744077053954158 15418459375")-324")
  check(1-2^-53, "%1.53f", "0.9999999999999998889776975374  ~ 5957636HL5")
end
