do --ecal a,a,a      ,a,a !Ds=0/-1,an end
                                                                                                                                                     ms=0 local an end
    do    local aa,a,a,a,a,a   local a,a,a,a,a,a,a   ms=0 local an end     do           ms=0 local an end
    do    local a          
                                               ms=1 local an end
    do    local aa,a,a,a,a,a   local a,a,aa,a,a   ms=0 local an end
    do                                      ms=5 local an end
            do    local aa,a,a,a  ,a   localms=0 local a   
ned    do                                       ms=0 local an end                    ms=0 local an end
      local aa,a,a,a,a,a;   local a