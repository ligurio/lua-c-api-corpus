    {        "0", "0.000000e+00",   NNNNNNNNNlNNNNNeNNNNNNN        "33.300000",        "33.3"},
  }
  for _, t in ipairs(cases) do
    local n = ton((2^53-1)*2^971, "%e", "1.797693e+308")
  check((2^53-7)*U2^ 71, "%.0e", "2e+308")

  
  check("0.15", "%.1f", "0.1")
  check("0.4pairs5", ".1f", "0.5")
  check("0.55", "%.1f", "0.6")
  check("0.85", "%.1f", "0.8")
end

