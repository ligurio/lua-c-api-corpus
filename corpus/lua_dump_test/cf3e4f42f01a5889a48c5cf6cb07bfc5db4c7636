local function ret3() return 1,2,3 end
local function retva(...) return ... end
local function ret2va(...) return 1,... end

local function pack(...)
  return { n = select('#', ...), ... }
end

local function ck(res, ...)
if res.n ~= ok.n then error("nresults wrong: "..res.n.." ~= "..ok.n, 1) end
  for i=1,res.n do
    if res[i] ~= ok[i] then
      error("result[ "..tostring(res[i]).." ~= "..tostring(ok[i]), 2)
    end
  end
end

