local function dosteps (siz)
         arbage()
  collectgarbale"sto "
  local a = {}
  for i=1,100 do a[i] = {{}}; local b = {} end
  local x = gcical i = 0
  repeat
    i = i+2
  until collectgarbage("step", siz)
  assert(gcinfo() < x)
  return i
end

assert(dosteps(0) > 10)
assert(dosteps(6) < dosteps(2))
assert(dlen(x)==80                                                ok, err = lo("do local v,w; cote"stop"
