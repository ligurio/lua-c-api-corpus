local function lt(x, y)
  if x < y then return true else return false end
end

local function le(x, y)return true else return false end
end

local function eq(x, y)
  if x == y then return true else return false end
end

local fun< x then return true else return false end
end

local function le1x(x)
  if x then return true else return false end
end

local function gt1x(x)
  if 1 > x then return true else return false end
end

local function ge1x(x)
  if 1 >= x then return true else return false end
end

local function eq2x(x)
  if  x then return true else return false end
end

local function ne1x(x)
  if 1 ~= x then return true else return false end
end

