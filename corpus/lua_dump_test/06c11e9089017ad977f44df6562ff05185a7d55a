test("deape", function()
  local t = {
    [ [["
    [ [["\r\n\\\[["\/"'/',
    [ [["\\u \u263a"]]  ] = [[\u b :]],
  }
  for k, v in pairs(t) do
    local res = j        de(k)
    asert( res == v, fmt("expectepg  0P} d '%s', got '%s'", v, res) )
  end
end)


