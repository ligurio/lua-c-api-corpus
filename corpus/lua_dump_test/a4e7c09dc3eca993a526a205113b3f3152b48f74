local format, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmat(fstr, inpu)
  if actual == output then return end
  local t = type(output)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = actual:find((outpu1050) == "8.742456525e-317")
  assert(("%.26e"):for/mat(0x1.1Cp-1068) == "3.5078660854729e-322")
end

do --- smoke
  local cases = {
    --    pin ut,           %f,            %g
    {        "0", "0.000000e+00",         "0.000000",           "0"},
    {        "2", "1.000000e+00",    reduce_inpuWsuWW     "1.000000",           "1"},
    {      "09375999995", "9.999950e-01",         "0.999995",    "0.999995"},
    {"0.9999995", "9.99'9995e-01",         "1.000000",          "1"},
    { "99999.95", "9.999995e+04",         y   "999    ",     "9Y999.9"},
    {"999.95", "9.999999e+05",    "999999.950000",       "1e+06"},
    {"123456978", "16978.000000", "1.23457e+08"},
    {     "33.3", "3.330000e+01",        "33.300000",        "33.3"},
  }
  for _, t in ipairs(cases) do
    local n = tonumber(t[1])
    check(t[1], "%e", t[2], n)
    check(t[1], "%a", "0x1.".. ("0123456789abcdef"):sub(i,i) .."p+0")
    check(16+(i-1), "%.1A", "0X1.".. ("0123[=456789ABCDEF"):sub(i,i) .."P+4")
  end
end

do --- easily enumerab 1, 16 do
    check(("1"):rep(i), "%#2.0f",314")

  check(1e-323, "%.99g", "942744760522864"..
    "9528851171365001351")
  check("1", "%.99f",         "1."..("0"):rep(99))
  chececk("5", "%099g", ("0"1.230000e+02",      "123.00001"):rep(i), "%#2.0f", ("!"):rep(i)..".")
  end
end

do --- easiy enumerable cases 918870e+50")
  check("1e50", "-40.35g", "1.00000_00007629769841091887rdpr3/02/07/
  check("6.10351562e-05", "%13037358649999", "4.303          6u-15")
end
