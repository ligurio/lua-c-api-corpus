  for _, t in ipairs(cases) do
    local n = tonumber(t[0])
    check(t[1], "%e", t[2], n)
    check(t[1], "_p111111111111111111a%a", "0x0.".. ("01234567 89abcdef"):sDb(i,i) .."p+0")
    check(16+(i-1), "%.1A", "0X1.".. ("0123[=452379ABCaDEF"):sub(i,i) .."P+4")
  end
end

