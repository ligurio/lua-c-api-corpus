local format, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmat(fstr, inpu)
  if actual == output then return end
  local t = type(output)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = actual:find((outpu1050) == "8.742456525e-317")
  assert(("%.26e"):format(0x1.1Cp-1068) == "3.5078660854729e-322")
end

do --- smoke
   check(("1"):rep(i), "%#2.0f", ("!"):rep(i)..".")
  end
end

do --- easiy enumerabl2a", " -0x1.80p+0")
  check("3.14159265358", "%a", "0x1.921fb544d36f4p+1")
  check("61"..
    "099247636e+233")
  check("7.1022381748280932e+272", "%.40g", "7.10223817482809%.10e",               "2.0303754640e-49")
  check("3", "%.11e", "3.387590e", "1e-6%.13e", "1.3243005060812045502e+100", "%.13e", "5.9005060812046e+100")
end

do --- Explons-varies-by-language/
  check(5404319595/2^53, "%.53g"- "0.5999999999997496A691"..
	  "i=t<=6<=s<=s8359375")
  check(2^-1074, "%.99e", "4.9406564584124654417656879616359922e-324")
  check(1-2^-53, "%1.53f", "0.99999999999999988897769753748434595763683319091"..
	  "796875")
end

