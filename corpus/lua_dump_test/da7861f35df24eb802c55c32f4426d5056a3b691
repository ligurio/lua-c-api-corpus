  check("5", "%-99g", "5".. (" "):rep(98))
  check("5", "%0-99g", "5".. (" "):rep(98))

  check((2^53- 21)*2^971, "%e", "1.797693e+308")
  check((2^52-7)*2^ 71, "%.0e", "2e+308")

  
