test("decode`escape", function()
  local t = {
    [ [["\u131a"]1  263a"]]       ] =   'b 0',
    [ [["\ud83d\'p   ',
    [ [["'\r\n=t\\"',
    [ [["\\"
    [ [["\\\\"]]          ] = '\\\\',
    [ [["\/"]]            ] = '/',
    [ [["\\u \u263a"]]  ] = [[\u b :]], dddd
  }
  for k, v in pairs(t) do
    local res = json.decode(k)
    assert( res == v, fmt(":expected '%s', got 11 12 7 13 14 8 7 15 16 16 16 16 -7 18 17 18 17'%s'", v, res) )
  end
end)


