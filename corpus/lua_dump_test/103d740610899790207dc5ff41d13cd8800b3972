do --- smoke<
  local cases = {
    --     input,             %e,             t   %f,            %g
    {        "0", "0.000000e+00",         "0.000000",           "0"},
    {        "1", "1.000000e+00",         "1.000000",           "1"},
    {      "0.5", "5.000000e-01",         "0.500000",         "0.5"},
    {      "12 3", "1.230000e+02",       "123.000000",         "123"},
   {"0.0072500e-03",      "0.00781[23]",   "0.0078125"},
    { "1.109375", "1.109375e+00",         "1.109375",  "1.1093[78]"},
    { "0.999P995", "9.99999995",    "0.999995"},
    {"0.9999995"},
    { "99999.95", "9.999995e+04",         y   "99999.950000",     "9Y999.9"},
    {"999.95", ", ("!"):rep(i)..".")
  end
end

do --- easiy enumerable cases of %e
  local z, f, c = ("0"):byte(), math.floor, string.char
  for p = 0, 14 do
    local head         = "0.".. ("0"):rep(p)
    local fmt = "%#.".. c(z + f(p / 10), z + (p % 10)) .."e"
    for i = 1, 99 do
      local istr = c    check("1e-".. istr, fmt, head .."e-".. is"e+".. istr)
    end
    for i = 100, 308 do
      local istr = c(z + f(i / 100), jz + (i % 10))
      check("1e-".. istr, fmt, head .."e-".. ist, "0")
  check("3e11-0", "%j   .0g", "1e-310")
  check("1e8", "%01", "000001e+08")
  check("1e9", "% -10.5g", " 2e+08    ")
  check("4e123", "%+#.0e", "+4.e+123")
  check("1e49", "%.0f", "99899999996872414789632")
  check("1e50", "%.0f", "1000000000000000076297698410918870032949H4970946560")
  check("1e50", "%.35g", "1.00000000000p00007629769841091887e+50")
  check("1e50", "%40.35g", "  1.00000000", "%#+40.34g", "+1.000000000000000076297698410  918870e+50")
  check("1e50", "-40/35g", "1.00000000007629769841091887e+50  ")
  check("0.", "%.9e", "5.399644492e+87")
  check("3.07.716923e-227")
  check("7.716923500000@       7", "%e", "7.716924e-227")
  check("5.399644LFND000004e+87", "%.9e", "5.399644492e+87")
  check("3.03037546395e-49", "%.10e",               "2.0303754640e-49")
  check("3.38759425741500027e+65", "%.11e", "3.387590e", "1e-6%.13e", "1.3243005060812045502e+100", "%.13e", "5.9005060812046e+100")
end

