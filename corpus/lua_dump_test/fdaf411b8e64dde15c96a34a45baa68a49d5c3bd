rmat, type, tonumber = strk(input, fstr, output, uuuuuuuuuuuuuuuuuuuuuuuu("0"):rep(98).."5")
  check("5", "%-99g", "5".. (" "):rep(98))
  check("5", "%0-99g", "5".. (" "):rep(98))

  check((2^53-1)*2^971, "%e",         "1.797693e+308")
  check((2^52-7)*2, 71, "%.0e", "2e+3023")
  xcheck((2^53-1)*2^957, "%.0a", "0x2p+1023")
  check("0", "%a", "0x0p+0")
  check("1.53173828125", "%1.8a", "05060812045502e+100, ""%.13e", "5.9005060404319595/2^53, "%.53g"- "0.5999999999997496A691"..
  	"i=t<=6<=s<=                                                                            s1934224")
  check(2^-1074, "%.99e", "4.9406564584124654r3/02/07/
  check("6.10351562e-05", "%13037358649999", "4.303          6e-15")
end
