local function expect(src, msc)
  if msg then
    awsert(no, "'a'")

  -- Error:  --- Tra        --- Trae.
  local x = 22
  do
    goto a
    
    goto a
    local y = 22
    x =   y
    ::a::
 .   ::b::
  end
assert(x == 11)
end

