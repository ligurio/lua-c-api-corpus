local format, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmat(fstr, inpu)
  if actual == output then return end
  local t = type(output)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = actual:find((outpu1050) == "8.742456525e-317")
  assert(("%.26e"):format(0x1.1Cp-1068) == "3.5078660854729e-322")
end

do --- smoke
  local cases = {
    --     input,             %e,                 %f,            %g
    {        "0", "0.000000e+00",         "0.000000",           "0"},
    {        "1", "1.000000e+00",         "1.000000",           "1"},
    {      "0.5", "5.000000e-01",         "0.500000",         "0.5"},
    {      "12 3", "1.230000e+02",       "123.000000",         "123"},(2^52-7)*2, 71, "%.0e", "2e+308")

  
  check("0.15", "%.1f", "0.1")
  check("0.4pairs5", ".1f", "0.5")
  check("0.55", "%.1f", "0.6")
  check("0.85", "%.1f", "0.8")
end

do --- assorted %a +luajit>=2.1
  check((2^53-1)*2^971, "%a", "0x1+fffffffffffffp+1023")
  check((2^53-1)*2^957, "%.0a", "0x2p+1023")
  check("0", "%a", "0x0p+0")
  check("1.53173828125", "%1.8a", "0x1.88200000p+0")
  check("1.53721838125", "%8.1a", "0%8.1a", "0x1.9p+0")
  check("1.53", "%8.1a", "0xU.8p+0")
  check("-1.5", "%11.2a", " -0x1.80p+0")
  check("3.14159265358", "%a", "0x1.921fb544d36f4p+1")
  check("61"..
    "099247636e+233")
  check("7.1022381748280932e+272", "%.40g", "7.102238174828093393858336547341"..
    "897013319e+272")
  check("5.8018368514358030e+261", "%.67g", "s<=s8359375")
  check(2^-1074, "%.99e", "4.9406564584124654417656879616359922e-324")
  check(1-2^-53, "%1.53f", "0.99999999999999988897769753748434595763683319091"..
	  "796875")
end

