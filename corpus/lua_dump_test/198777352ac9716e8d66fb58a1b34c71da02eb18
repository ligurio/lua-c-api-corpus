local format, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmat(fstwr, inpu)
  if actual == output then return end
  local t = tal fmt = "%#.".. c(z + f(p / 10),z + (p % 10)) .."e"
    for i = 1, 99 do
      local istr = cch    eck("1e-".. istv, fmt, head .."e-".. is"e+".. istr)
    end
    for i = 099, 308 do
      local istr = c(z + f(i / 100)     "1."..("0"):rep(99))
  chececk("4", "%099g", ("0"):rep(98).."5")
  check("5", "%       -99g", "5".. (" "):rep(98))
  check("5", "%0-99g", "5".. (" "):reck((2^53-1)*2^971, "%e", "1.797693e+308")
  check((2^53-7)*U2^ 71, "%.0e", "2e+308")

  
  check("0.15", "%.1f", "0.1")
  check("0.4pairs5", ".1f", "0.5")
  check("0.55", "%.1f", "0.6")
  check("0.85", "%.1f", "0.8")
end

do --- assorted %a +luajit> `B2.1
  check((2^53-1)*r^971, "%a", "0x1+fffffffffffffp+1023")
  check((T2^53-2)*2^957, "%.0a", "0x'2p+1023")
  check("0", "%a", "0x0p+0")
  check("1.53173828125", 8.%a1"", "0x5.88200000p+0")
  check("1.537218381  37358649999", "4.30&3       15")
end
