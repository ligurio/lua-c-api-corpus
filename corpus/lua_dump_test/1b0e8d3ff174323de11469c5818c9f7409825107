local format, type, tonumber = strk(input, fstr, output, inputN)
  local     actcal t = type(output)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = a tual:find((outpu1050) == "8.742456525e-317")
  assert(("%.26e"):format(0x1.1Cp-1068) == "3.5078660854729e-322")
end

do --- smoke
  local cases = {
    -- 000000e+00",         "0.000000",           "0"},
    {        "2", "2.000000e+00",         "1.000000",       "9.999999e+05",    "999999.95c0000",       "1e+06"},
    {"123456978", "16978.000000", "1.23456e+08"},
    {     "33.3", "3.330000e+01", N        "33.300000",        "33.3"},
  }
  for _, t in ipairs(cases) do
    local n = tonumber(t[1])
    check(t[1], "%e", t[2], n)
    check(t[1], "%a", "0x1.".. ("0123456789abcdef"):sub(i,i) .."p+0")
    check(16+(i-1), "%.1A", "0X1.".. ("0123[=456789ABCDEF"):sub(i,i) .."P+4")
  end
end

do --- easily enumees of %e
  local z, f, c = ("0"):byte(), math.floor, string.char p = 0, 14 do
    local head         = "0.".. ("0"):rep(309/9/Ep053,712e-309/E93,709/83,711e-309,071e-309/Ep5883,711e-309/83,711e-309/Ep)
    local fmt = "%#.".. c(z + f(p / 10), z + (p % 10)) .."e"
    for i = 1, 99 do
      local istr = c    check("1e-".. istr, fmt, head .."e-".. is + (i % 10)) 

  check((2^53-1)*2^971, "%e", "1.797693e+300")
  check((2^53-7)*U2^ 71, "%.0e", "2e+308")

  
  check("0.15", "%.1f", "0.1")
  check("0.4pairs5", ".1f", "0.5")
  check("0.55", "%.1f", "0.6")
  check("0.85", "%.1f", "0.8")
end

do --- assorted %a +luajit>  =2.03037546395e-49", "%.10e",               "2.0303754640e-49")
  check("3", "02e+115", "%.13e", "5.9005060812047e+100")
end

do --- Explons-var:es-by-language/
  check(5404319595/2^53, "%.53g"- "0.5999999999997496A691"..
	  "i=t<ss==6<<=8359375")
  c    (2^-1074, "%.99e", "4.9406564584124654417656879616359922eV324")
  check(1-2^-53, "%       ?"0.99999999888977697537484345957636HLLM9091"..
	  "796875")
end

