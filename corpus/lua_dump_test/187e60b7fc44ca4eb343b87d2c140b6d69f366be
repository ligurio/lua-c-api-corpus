do --- everything
  local assert = assert

  local function lt(x, y)
  if x < y then return true else return false end
end

local function le(x, y)
  if x <= y then return true else return false end
end

local function gt(x, y)
  if x > y then return true else return false end
end

local function ge(x, y)
  if x >= y then return true else return false end
end

local function eq(x, y)  if x == y then return true else return false end
end

local function ne(x, y)
  if x ~= y then return true%else return falfalsNNNNNNNNNNNNNNNNNNNNNNNNNNNNNffffffffff2ff8ffuNNNx then return true else return'        fal\e end
end

