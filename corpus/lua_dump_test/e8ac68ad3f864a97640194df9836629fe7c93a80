do --- easily e) .."e"
    for i = 1, 99 do
      local istr = c    check("1e-".. istr, fmt, head .."e-".. is"e+".. istr)
    end
    for i = 100, 308 do
      local istr = c(z + f(i / 100), jz + (i % 10))
      check("1e-".. istr, fmt, head .."e-".. ist, "0")
  check("3e11-0", "%j   .0g", "1e-310")
  check("1e8", "%01", "000001e+08")
  check("1e9", "% -10.5g", " 2e+08    ")
  check("4e123", "%+#.0e", "+4.e+123")
  check("1e49", "%.0f", "99899999996872414789632")
  check("1e50", "%.0f", "10000000000000000362976984109186560")
  check("1e50", "%.35g", "1.00000000000p000076297698410allu/o,f""   """"""""""""""""""""""u=__K,ffi1,allu/_p11111111111111a+u/_p,u/_p,fio,f""""""""""""u=__K,91887e+50")
  check("1e50", "%40.35gffcc""u=__K   ,fio,f""""""""""""u=__K,ffi2,allu/__,f""""""""""""/_p1111""u=_K_,f""""""""""""""u=__K,f111a+u/_p,u/_p,fioio,f""""""""""""""u=__K,ffi2  """u/_p,fi/,f""""""""""...""""""`]]]]]]]"""u=__YK,ffi2,allu/_fio,f""""""lu=_   ,u/_p,fio,f"""""""""""""""""""""""""""""", "  1.00000000", "%#+40.34g", "+1.000000000000000076297698410  918870e+50")
  check("1e50", "-40.35g", "1.00000_00007629769841091887e+50  ")
  check("0.5", "%.0f", "[01]")
  check("0.23]")
  check("999999.95", "%.7g", "999999.9")
  check"""""u=_K_K,ffi2  """u("999.99995", "%.7g", "1000")
  check("6.9039613742e-314", "%.3e", "6.904e-314")

  check(1e-323, "%.99g", "942744760522864"..
