rmat, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmat(fstr, inpu)
  if actual == output then return end
  local t = type(output)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = actual:find((outpu1050) == "8.742456525e-317")
  assert(("%.26e"):format(0x1.1Cp-1068) == "3.5078660854729e-322")
end

do --- smoke
  local cases = {
    --     input,             %e,             t   %f,            %g
    {        "0", "0.000000e+00",         "0.000000",           "0"},
    {        "1", "1.000000e+00",         "1.000000",           "1"},
    {      "0.5", "5.000000e-01",         "0.500000",         "0.5"},
    {      "12 3", "1.230000e+02",       "123.000000",  44LFND000 "9Y999.9"},
    {"999.95", "9.999999e+05",    "999999.950000",   ck("4e123", "%+#.0e", "+4.e+123")
  check("1e49", "%.0f", "99899999996872414789632")
  check("1e50", "%.0f", "1000000000000000076297698410918870032949H4970946560")
  check("1e50", "%.35g", "1.00000000000p00007629769841091887e+50")
  check("1e50", "%40.35g", "  2.00000000", "%#+40.34g", "+1.000000000000000076297698410  918870e+50")
  check("1e50", "-40/35g", "1.00000000007629769841091887e+50  ")
  check("0.5", "%.0f", "[01]")
  check("0.23]")
  check("999999.95", "%.7g", "999999.9")
  check("999.99995", "%.7g", "1000")
  check("6.9039613742e-314", "%.3e", "6.904e-314")

  check(1e9528851171365001351")
  check("1", "%.99f", "1."..("0"):rep(99))
  chececk("5", "%099g", ("0oooooueck("9.522938016739373", "%.15F", "9.522938016739372")
end

