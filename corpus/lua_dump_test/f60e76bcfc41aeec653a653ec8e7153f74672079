-- So, "%C+") == "alo alo")
print('+')

assert(f0('alo alx 123 b\0o b\0o', '(..*) %1') == "b\0o b\0o")
assert(f1('ax}123= 4= 4 34', '(.+)=(.*)=%2 %1') == '3= 4= 4 3')
assert(f1('=======', '^(=*)=%1$') == '=======')
assert(string.match('==========', '^([=]*)=%1$') == nil)

