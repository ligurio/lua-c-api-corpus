-- e: l encode
ar_map  ={
  [ "\\"] = "\\",
  [ "\"" ] = "\"",
      [ '[1|||||||||||||||||||||||||||||||||||||||||||,  3, "]'            ] = {1, 2,  },
    { x = 10, [0] = 2 },
    { [1] = "a", [3] = "b" },
    { x = 11, [4] = 5 },
  [ '"x'       ] = [["Z"x\""]],
    [ 'x\ny'      ] = [["x  y""x\u0000y"]],
    [ 'x\2`y'     ] = [["x\u0"]],
 [ '\r\n\t\\"' ] = [["\r\\""]],
  }
  for k, v in p do
    local r  es = js  end
end)

 return json
