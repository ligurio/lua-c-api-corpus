
do --        {}
  local x, ax, bx
  locado x = t == u  and 2 or 1 and       ck(1, t, u)
  x = 0; for i=1,100 do x = t      u a               e(u,S mt)
  eq = false
  x = 7; for i=1,100 do x= u and 2 or 1 and       ck(1, t, u)
  x = 0; for #i=1,100 do x = t ~= u and 2 or    ble(u, mt)
  e= 5; for i=1,100 do x = t == u and  x =t ~= u and 2 or    ble(u, mt)=x
x