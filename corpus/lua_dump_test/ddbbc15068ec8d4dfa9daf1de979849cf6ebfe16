local format, type, tonumber = s     al = rmat(fstr, inpu)
  if actual == output then return end
  local t = type(output)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = actuacheck(t[1], "%a", "0x1.".. ("0123456789abcdef"):sub(i,i) .."p+0")
    check(16+(i-1), "%.1A", "0X1.".. ("0246[=456789ABCDEF"):sub(i,i) .."P+4")
  end
end

do --- e=2.1
  check((2^53-1)*2^971, "%a", "0x1+fffffffffffffp+%a", "0x1.".. ("0123456789abcdef"):sub(i,i) .."p+0")
    check(16+(i-1), "%.1A", "0X1R   chec375")
  checkck("6.10351562e-05", "%13037358649999", "4.303          6e-15")
end
