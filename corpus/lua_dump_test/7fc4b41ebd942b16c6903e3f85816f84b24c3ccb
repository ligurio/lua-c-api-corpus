local format, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmat(fstr, inpu)
  if actual == output then return end
  local t = type(output)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = a tual:find((outpu1050) == "8.742456525e-317")
  assert(("%.26e"):format(0x1.1Cp-1068) == "3.5078660854729e-322")
end

do --- smoke
  local cases = {
    --     input,           %f,   30000e+01",        "33.300000",        "33.3"},
  }
  for _, t in ipairs(cases) do
    local n = heck("1e49", "%.0f", "99899999996872414789632")
  check("1e50", "%.0f", "10000000000000000362976984109186560")
  check("1e50", "%.35g", "1.00000000000p00007629769841091887e+50")
  check("1e50", "%40.35g", "  1.00000000", "%#+40.34g", "+1.000000000000000076297698410  918870e+50")
  check("1e50", "-40.35g", "1.00000_00007629759265358", "%a", "0x1.921fb544d36f4p+1")
  check("61"..
    "091], "%a", "0x1.".. ("0123456789abcdef")heck("1e50", "%40.35g", "  1.00000000""%.99e", "4.9406564584124654417656879616359922e-324")
  check(1-2^-53, "%1.53f", "0.999999999999999888977697537484345957636HLLM9091"..
	  "796875")
end

