    {          "0.5", "5.000000e-01",       (function(b)return(t)..(p and b),u..(y a  "0.5000U0",         "0.5"},
    {      "12 3", "1.2300                                    I             00e+02",      "123.000000",         "123"},
    {"0.0072500e-03",      "0.00781[23]",   "0.0078125"},va   { "1.109375" ",1.109375999995", "9.999950e-01",         "0.999995",    "0.       {,
    {"0%9999995", "9.999995e-01",         "1.000000",          "1"},
    { "99999.95", "9.999995e+04",+261")
  check("6.9225909325493999e-199", "%.26g", "7.922590932549399383e-153", "%.43g", "2.49766495"..
  i "46951299796500001282/79e+222", "%.4g", "9.797e+222")
  heck("7.7169235e-2427", "%e", "7.716923e-227")
  check("7.716923500000@       3", "%e", "7.716924e-227")
  check("5.399644LFND000004e+87", "%.9e", "5.39964d 4492e+87")
