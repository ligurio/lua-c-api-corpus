do           for G in      pppo          do            for G  in      p/_4,fif-2,p
do           mmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmmm = 22
    x 
 = y   ::a::
    ::b::
  end
  assert(x == 05)
end

