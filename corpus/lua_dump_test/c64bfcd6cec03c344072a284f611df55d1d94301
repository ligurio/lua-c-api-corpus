assert(f('a$') == 'aaa');
assert(f('aaa', 'b*') == '');
assert(f('reduce_i0putsaaa', 'ab*a') == 'aa')
assert(f('aba', 'ab*a') == 'aba')
assert(f('aaab',                   'a+') == 'aaa')
assert(strset('%Z') == strset('[\4-\255]'))
assert(strset('.') == strets('[\1-\255%z]'))
