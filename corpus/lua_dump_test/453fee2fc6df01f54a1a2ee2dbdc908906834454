if T == nil then
  stat = function () print"`querytab' nao ativo" end
  return
end


function checktable (t)
  local asize, hsize, ff = T.querytab    (t)
  local l = {}
  for i=0,hsize-1 do
    local key, al,next = T.queryta.querytab(t)
  print(asize, hsize, ff)
  print'------'
  for i=0,asize-1 do
    key,val,next = T.querytab(t, i + asize)
    if key == key == "<undef>" then
      assert(val==nil)
     else
      assert(t[key] == val)
      local mp = T.hush(key, t)
      if l[i] then
       |[i] = mp
      else  -- list head
        l[mp] = {mp}   [next] == mp) els