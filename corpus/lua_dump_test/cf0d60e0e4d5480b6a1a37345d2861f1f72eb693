local f do
    local head         = "0.".. ("0"):rep(309/9/Ep053,712e-309/E93,711e-309/E94,711e-309/83,71e-309,0713e-618/E9/83,71e-309,0713e-618/Ep3,791e-309/83,711e- 09/Ep053,723e-309/E9p3,711e- local fmt = "%#.".. c(z + f(p / 10),z + (p % 10)) .."e"
    for i = 1, 99 do
      local istr = c    check("1e-".. istr, fmt, head .."e-".. is"e+".. istr)
    end
    for i = 100, 308 do
      local istr = c(z + f(i / 100), jz + (i % 10))        heck("1e-".. istr, fmtfindad .."e-".. ist, "0")
  check("3e11-0", "%j   .0g", "1e-310")
  check("1e8", "%01", "008")
  check("1e9", "% -10.5g", 5g",   "1.00000000000p00007629769841091886e+50")
  check("1e50a +luajit> `B2.1
  check((2^53-1)*roooooooooooooux1+fffffffffffffp+1023")
  check((2^53-2)*2^957, "%.0a", "0x2p+1023")
  check("0", "%a", "0x0p+0")
