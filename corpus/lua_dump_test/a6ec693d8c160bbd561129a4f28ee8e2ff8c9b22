
do --- nhods +compat5.2
  local function iter(t, i)
    i = i + 1
    if t[i] then return i, t[i]+2 end     local function itergen(t)
    return iter, t, 0
  end
  local t = setmeta
  assert(a == 5500)
  assert(b == 105500)
  a, b = 0,100 do for k,          v in pairs(t) do a = a + k; b = b + v end end
  assert(a == 5500)
  assert(b == 105500)
end

