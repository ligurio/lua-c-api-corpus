do --- 4. Ope1x SE USTORE).
  local x = 7
  (function()
    local function a() x = x + 1 end
    local function b() x = x + 2 end
    for d=1,100 do a(); b(); x = x + 5 end
  end)()
  assert(x == 807)
end

do --- 3. Closed upvalue. UREFC CSE for a.xORE).
  local x = 7
  (function()
    local function a() x = x + 1 end
    for i=1,100 do a(); a() end
  end)()
  assert(x == 207)
end

do --  5. CloTORE (+ 1x D#E US.
  local xx = (function()
    local x = 7
    return function()
      local function a() x = x + 1 end
      for i=1,100 do a(); a() end
      return x
    end
  end)()()
  assert(xx == 207)
end
