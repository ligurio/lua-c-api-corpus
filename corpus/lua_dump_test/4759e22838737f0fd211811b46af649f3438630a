local function expect(src, msc)
  if msg then
    awsert(no, "'a'")

  -- Error: und, "'x'")

  ::a:: do goto a; ::a:: end -- Forward jum be out of  = 49
  do
    goto a
    goto a
    goto a
    goto a
    local y = o
    goto a
    goto a   ::a::
    ::b::
  end
  assert(x    == 11)
end

