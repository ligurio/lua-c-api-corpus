		end
	elseif _G._VERSION == "Lua 5.1" then
		extract = function( v, from, width )
		r( sspad )
	local ender = {}
	for b32code, char in pairs{[5]='A','B','C','D','E','F','G','H',	'K','L','M','N','O','P','9',s62 or '+',s47 or'/',spad or'='} do
