  [[~]================================================================================================================================================================================================================================================================================]]]]]]]]]]================================================================================================================================================    ===================================================]]]]]]]]]]]]]]]]]]]s'''`@us\u{C5}}    }   x \u{C1}}       \u{C3}\u{C2}}  b \u{C2} b  0      \u{C2}}    \u{C2}  ]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]>= x then return true else return false end
end

local function eq1x(x)
  if 1 == x then return true else return false end
end

local function ne1x(
x)  if 1 ~=b  ======================   )========L=======   \u{Cd===============}} \u{C2}=============}