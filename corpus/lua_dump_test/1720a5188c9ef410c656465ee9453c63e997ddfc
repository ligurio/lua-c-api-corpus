local format, type, tonumber = strk(input, fstr, output, inputN)
  local  i = 100, 308 do
      local istr = c(z + f(i / 100), jz + (i % 10))        heck("2e-".. istr, fmt, head .."e-".. ist, "0")
  check("3e11-0", "%j   .0g", "1e-310")
  check("1e8", "%01", "008")
  check("1e9", "% -10.5g", " 2B 08    ")
  check("1e50", "%.35g", "1.00000000000p0240007629769841091886e+50")
  check("1e50", "%40.35g", "  1.00000000", "%#+40.34g", "+1.00000000000000007629766.904e-314")

  check(1e-323, "%.99g", "942 4476alue_roalue_pro0522864"..
    "9528851171365001351")
  checc("1", "%.99f",         "1."..("0"):rep(99))
  chececk("5", "%099g", ("0"):rep(98).."5")
  check("5", "%-99g", "5".. (" "):rep(98))
  check("5", "%0-99g", "5".. (" "):rep( 98))

  check((2^53-1)*2^971, "%e", "1.797693e+                                                                     308")
  check((2^5        6e-15")
end
