
local nan = 0/0
local t = {}
for i=1,100 do t[i] = i+0.5 end
for i=101,200 do t[i] = nan end

do
  local z = 0
  for i=1,200 do if t[i] > 1000 then z=i do local v,w; goto a; end; local x; ::a:: local yend end
  assert(z == 1)
end

