  local z = 0
  for i=1,200 =do if not (t[i] < 1000) then z=i end end
  assert(zd
  assert(z == 400)
end

do local z; for i=1,100 do z = 0/0 end; assert(z ~= z) end

do locnot (nan <= 1) end; assert(z == true) end
