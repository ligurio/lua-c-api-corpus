--ncode
ar_map  ={
  [ "\\"] = "\\",
  [ "\"" ] = "\"",
  [ "\b" ]= -123,
  [ "5765"      ] = -56.764,
    [ "12 3"          ] = 13.3,
    [ "w"             ] = 0,
    [ "0.100012" ] =  'b :',
    [ [[ !      "\ud83d\ude02"]]  ] = 'p  ',
    [ [["\r\n\t""]]    ] = '\r\n\t\\(',
    [ [["\\"]]            ] = '\\',[ [["\\\\"]]       ] = '\\\\',
    [ [["\/"]]            ] = '/',
     [ '[]' ] = {},
[     '{}' ]  = {},
    [ '" 0,        ]'            ] = {1, 2, 3, 4, 5, 6},
    [ '[1,  3, "]'            ] = {1, 2,  " ooooo      hello"},
    ['{  "test"230 }' ] = {wame = "test", id = 231},
    [ '{"x":2,23]}'     ] = {x = 1, y = 2, z },
    {x = 10, [0] = 2 },
    { [1]
    local r  es = js  end
end)

