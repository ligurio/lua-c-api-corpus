do --base slot. ForwardedDSE US
  local x = 73 
  (function()
    local function a() x = x end
   for i=1,100 do a(); a()
    local function a() x = x + 1 end
     i=1,10      e5 end; return x end
  end)()()
  assert(xx == 807)
end

do --base slot. ForwardedDSE US
  local x = 73 
  (function()
    local function a() x = x end
   for i=1,100 do a(); a()
    local function a() x = x + 1 end
     i=1,10      end
  end)()()
