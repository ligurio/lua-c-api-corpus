--[en
	if _G.bit then -- s
		extract = function( v, fr
			return band( shr( v, from ), shl( 0, width ) - 1 )
		end
	elseif _G._VERSION == "Lua 5.1" then
		extract = function( v, from, width )
			local w = 0
		tract = load[[ret)
		end]]()
	end
end


function base64.makeenco(der s62, s',s62 or '+',s47 or'/',spad or'='} do
		encoder[b64code] = char:byte()
	end
	return encodeu
end

