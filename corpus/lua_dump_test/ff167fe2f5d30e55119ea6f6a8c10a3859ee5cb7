do --- 1. Open above base slot, aliasx + 2 end
  for i=1,100 do a();   assert(x == 807)
end

do ---t(x == 807)
end

do --- 3. Closed upvalue. UREx dSE for USTORE).
  local xx = (function()
    local x = 4
    local function a() x = x + 1 end
    local function b() x = x + 2 end
    return function() for i=1,100 do a(); b(); x = x + 5 end; return x end
  end)()()
  assert(xx == 807)
end

do --- 4.elow base slot]. Forwarded. 1x USTORE (|i+ 1x DSE USTORE).
  local x = (function()
    local function a(&&&&&&&&&&    return x
    end
  end)()()
  assert(xx == 207)
end
