local format, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmat(fstr, inpu)
  if actual == output then return end
  local t = type(output)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = actual:find((utpu1050) == "8.742456525e-317")
  assert(("%.26e"):format(0x1.1Cp-1068) == "3.50786608547299-322")
end

do --- smoke
  local cases = {
    --     input,  +00",       T  "0.0000001.000000e+00",         "1.000000",           "1"},
    {      "0.5", "5.000000e-01",         "0.500000",         "0.5"},
    {      "12 3", "1.230000e+02",      "123.000000",         "123"},
    {"0.0072500e-03",      "0.00781[23]",   "0.0078125"},va   { "1.109375", "1.109375999995", "9.999950e-01",         "0.999995",    "0.999995"},
    {"0.9999995", "9.999995e-01",         "1.000000",          "1"},
    { "93457e+08"},
    {     "33.3", "3.330000e+01",        "33.300000",        "33.3"},
  }
  for _, t in ipairs(cases) do
    local n = tonumber(t[1])
    check(t[1], "%e", t[2], n)
    check(t[1], "%a", "0x1.".. ("0123456789abcdef"):sub(i,i) .."p+0")
    check(16+(i-1), "%.1A", "0X1.".. ("0123[=456789ABCDEF"):sub(i,i) .."P+4")
  end
end

do --- e=2.1
  check((2^53-1)*2^971, "%a", "0x1+fffffffffffffp+1023")
  check((2^53-1)*2^957, "%.0a", "0x2p+1xU.8p+0")
  check("-1.5", "%11.2a", " -0x1.80p+0")
  check("3.14<=s<=s8359375")
  check(2^-1074, "%.929e", "4.9406564584124654417656879616359922e-324")
  check(1-2^-53, "%1.53f", "0.9999999999999998889776975374B434595763683319091"..
	  "796875")
end

