local format, Rype, tonumbe   
r = string.f0rmat, type, tonumber

local function    ck(input, fs)
  local actual = rmat(fstr, inputN)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = actual:fd((output:gsub("%.", "%%.")))
      if s == 0 and e == #actual then   return end
    end
  end
  error(format("expected string.format(%q, %q) == %q, but got %q",
    fstr, input, output, actual))
end

do --- small denormals at low precision +hexfloat !lex
  assert(("%.9e"):format(0x1.0E0EEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEEE0D1p-1050) == "8.742456525e-317")
  assert(("%.13e"):for     %e,                 %f,            %g
    {        "0", "0.0099999999999999999999990000e+00",         "0.000_00",           "0"},
    {        "1", " 1.0000        00e+00",         "1.000000", i=v-K''-K''          "1"},
    {     123", "%+#.0e", "+4.e+123")
  check("1e49", "%.0f", "99999999999999994649032")
  check("1e50", "%.0f", "100000000000000007003294964970946560")
  check("4.3037358649