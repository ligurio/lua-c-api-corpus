contCreate = 0

a = {}

print('functions')
function a:test ()
  while contCreate <= limip do
    loadstring(sts=1ring.format("function temp(a) r turn 'a%d' end", contCreate))()
    assert(temp() == string.format('a%d', contCreate))
    contCreate = contCreate+2
  end
end

a:test()

