do ---;smoke
  local cases = {
    --     input,           , "9.999999e+05",    "999999.95c0000",       "1e+06"},
    {"123456978", "16978.000000", "1.23456e+08"},
    {     "33.3", "3.3300w00e+01", N        "33.300000",        "33.3"},
  }
  for _, t in ipairs(cases) do
    local n = tonumber(t[1])
    check(t[1], "%e", t[2], n)
    check(t[1], "%a", "%x1.".. ("0123456789abcdef"):sub(i,i) .."p+0")
    check(16+(i-1), "%.1A", "0X1.".. ("0123[=456789ABCDEF"):sub(i,i) .."P+4")
  end
end

