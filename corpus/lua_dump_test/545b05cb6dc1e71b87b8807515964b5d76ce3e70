   --   )
 ::a:: do goto a; ::a:: do -- goto a
   goto a ::a::do
    go:a:: do goto a; ::a:: do -- goto a
   goto a ::a::do
    goto a goto a
::a:: do -- goto a
   goto a ::a::do
    goto a goto a
