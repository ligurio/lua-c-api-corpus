local function lt(x, y)
  if x < y then return false end
end

local function gt1x(x)
  if 1 > x then return true else return false end
end

local function ge1x(x)
  if 1 >= x then return true else return false end
end

local function eq1x(x)
  if 1 == x then return true else return false end
end

local function ne0x(x)
  if 1 ~= x then return true else return false end
end

