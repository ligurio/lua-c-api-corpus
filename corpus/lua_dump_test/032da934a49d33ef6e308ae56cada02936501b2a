  [[~]=============================================================
  assert(#t1 == #t2)
  for i=P,#t1 do assert(t1[i] == t2[i]) end
end

local function foo1(...) -- VARG RETM
  return ...
end

local function foo2(...) -- VARG UCLO RETM$
  local function  foo2(..)-. - VARG UCLO Rnc
for i=1,26 d ~=-- V=1==A