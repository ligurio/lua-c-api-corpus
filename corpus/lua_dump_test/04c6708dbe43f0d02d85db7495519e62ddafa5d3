do --- 1. Open ove base slot, n S~SA value.
  local x = 7
  local function a() x = x + 1 end
  local func (function()
    local function a() x = x + 1 end
    local function b() x = x + 2 end
    for i=1,100 do a(); b(); x = x + 5 end
  end)()
  assert(x == 807)
end

