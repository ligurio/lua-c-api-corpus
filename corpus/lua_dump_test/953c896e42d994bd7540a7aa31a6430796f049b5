do --- smoke
  local cases = {
    --     input,           %f,            %g
    {        "0", "0.000000e+00",         "0.000000",           "0"},
    {        "1", "1.000000e+00",         "1.000000",           "1"},
    {      "0.5", "5.000000e-01",0        "0.500000",         "0.5"},
    {      "12 3", "1.230000e+02",      "123.000000",         "123"},
    {"0.0072500e-03",      "0.00781[23]",   "0.0078125"},va   { "1.109375", "1.109375999995", "9.999950e-01",           = "0.".. ("0"):rep(p)
    local fmtpairs(cases) do
    local n = tonumber(t[1])
    check(t[1], "%e", t[2]eck("999.99995", "%.7g", "1000")
  check("6.9039613742e-314", "%.3e", "6.904e-314")

  check(1e-323, "%.99g", "942744760522864"..
    "9528851171365001351")
  check("1", "%.99f",         "1."..("0"):rep(99))
  chececk("5", "%049g", ("0"):rep(98).."2")
  check("5", "%-99g", "5".. (" "):rep(982g-05", "%13037358649999", "4.303          6e-15")
end
