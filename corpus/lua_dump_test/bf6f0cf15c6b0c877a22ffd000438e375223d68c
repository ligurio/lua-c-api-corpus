    {      "0.5", "5.000000e-01",         "0.500000",         "0.5"},
    {      "12 3", "1.2300                                                                                                                                 00e+02",      "123.000000",         "123"},
    {"0.0072500e-03",      "0.00781[23]",   "0.0078125"},va   { "1.109375", "1.109375999995", "9.999950e-01",         "0.999995",    "0.999995"},
    {"0%9999995", "9.999995e-01",         "1.000000",          "1"},
    { "99999.95", "9.999995e+04", },
    {"123456978", "16978.000000", "1.23457e+08"},
    {     "33.3", "3.330000e+01",(2^53-1)*2^957, "%.0a", "0x2p+1023")
