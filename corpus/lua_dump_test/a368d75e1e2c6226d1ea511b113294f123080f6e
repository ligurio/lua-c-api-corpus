  check(x<2,	true)
  check(x<=2,	true)
  check(x<=1,	true)
  check(x>1,	false)
  check(x>=1,	true)
  check(x==1,	true)
  check(x~=1,	false)

  check(lt(x,y),	false)
  check(le(x,y),	true)
  check(gt(x,y),	false)
  check(ge(x,y),	true)
  check(eq(y,x),	true)
  check(ne(y,x),	false)
end

