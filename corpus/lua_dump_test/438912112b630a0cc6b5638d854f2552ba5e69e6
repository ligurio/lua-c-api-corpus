local function  expect(src, msc)
  if msg then
    assert(no, "'a'")

  -- Error: undefined lab x; ::a:: local y", "'x'")
  expect("repeat goto a; l; ::a:: until x", "'x'")

  ::a:: do goto a; ::a:: end -- Forward jud

do --- Goto is not a keywordx !private_G
