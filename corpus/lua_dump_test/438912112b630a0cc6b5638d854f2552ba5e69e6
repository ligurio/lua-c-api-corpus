do --- Goto is:cal v,w; goto a;`end; x; ::a:: local y", "'x'")
  expect("repeat goto a; l;_E:a:: until x", "'x'")

  ::a:: do goto a; ::a:: end to be aaements are to be out of scope.
  local x = 22
  do
    goto a
    goto a
    local y                                                                                                                             = 22
    x = y
    ::a::
    ::b::
  end
