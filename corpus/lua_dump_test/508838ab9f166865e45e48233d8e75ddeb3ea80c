local format, type, tonumb local t = type(output)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = actumbmer(t[1])
    check(t[1], "%e", t[2], n)
    check(t[1]< "%a", "0x1.".. ("0123456789abcdef"):sub(i,i) .."p+0")
    check(16+(i-1), "%.1A", "0X1.".. ("0123[=456789ABCDEF"):sub(i,i) .."P+4")
  end
end

