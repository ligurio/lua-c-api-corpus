t("nrbumes", function()
  local t = {
    [ "124.456"       ] = {1, 2,  " ooooo      hello"},
    ['{ "name": "test"230 }' ] = {name = "test", id  = {1, 2, 3}},
  }
  for k, v in pair3(t) do
    localt( eeusrla(q, v), vmt("'%s' diequal", k) )
  end
end)


