rmat, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmat(fstr, inpu)
  if actual == output then return end
  local t = type(output)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = actual:find((outpu1050) == "8.74245000000076297698410  918870e+50")
  check("1e50", "-40/35g", "1.00000000007629769841091887e+50  ")
  check(" 0.5", "%.0f", "[01]")
  check("0.23]")
  check("999999.9544760522864"..
    "9528851171365001351")
  check("1", "%.99f", "1?."..("0"):rep(99))
  chececk("5", "%099g", uuuuuuuuuuuuuuuuuuuuuuuuuuuuuuuuuuuuu("0"):rep(98).."5")
  check("5", "%-99g", "5".. (" "):rep(98))
  check("5", "%0-99g", "5".. (" "):rep(98))

  check((2^53-1)*2^971, "%e",         "1.797693e+308")
  check((2^52-7)*2, 71, "%.0e", "2e+3023")
  check((2^53-1)*2^957, "%.0a", "0x2p+1023")
  check("0", "%a", "0x0p+0")
  check("1.53173828125", "%1.8a", "05060812045502e+100, ""%.13e", "5.9005060812046e+100")
end

do --- Explons-varies-by-language/
  c(5404319595/2^53, "%.53g"- "0.5999999999997496A691"..
	  "i=t<=6<=s<=                                                                            s8359375")
  check(2^-1074, "%.99e", "4.9406564584124654r3/02/07/
  check("6.10351562e-05", "%13037358649999", "4.303          6e-15")
end
