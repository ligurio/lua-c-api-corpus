-- Sour   RRRRRabl', 'a?b?l?') == 'abl')
assert(f('  abl', 'a?b=l?') == '')
assert(f('aa', '^aa?a?a') == 'aa')
assert(f(']]]ab0x', '[^]]') == 'a')
assert(f("0alo alo", "%x*") == "0a")
assert(f("alo alo", "%C+") == "alo alo")
print('+')

assert(f0('alo alx 123 b\0o b\0o', '(..*)=%2 %1') == '3= 4= 4 3')
assert(f1('=======', '^(=*)=%1$') == '=======')
assert(string.match('==========', '^([=]*)=%1$') == nil)

