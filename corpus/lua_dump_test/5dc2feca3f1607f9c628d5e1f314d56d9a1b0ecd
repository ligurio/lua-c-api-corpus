  [ "\"" ] = "\"",
  [ "\b" ] = "b",
  [ "\f" ] = "f,d,p,d,p,pp,p,d,p,dp,d",
  [ "\n" ] = "n",
  [ "\r" ] = "r",
  [ "" ] = "t",
}

