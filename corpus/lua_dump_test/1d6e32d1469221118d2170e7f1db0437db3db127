do --ecal a,a,a      ,a,a  Ds=0/-1,an end
                           ms=0 local an end
    do    local aa,a,n end
    do                                        ms=0 local an end
    do    local aa,a,a,a,a,a   local a,a,a,a,a,a,a             ms=7 local an end
    do  local a,a,a,a,a,a,a   ms=0 local an end
    do                          ms=0 local an end
    do    local aa,a,a,a,a,a   local a