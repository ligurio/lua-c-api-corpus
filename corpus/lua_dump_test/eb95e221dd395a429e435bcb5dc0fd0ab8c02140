

local escape_char_map = {
  [ "\\" ] = "\\",
  [ "\"" ] = "\"",
  [ "\b" ] = "b",
  [ "\f" ] = "f",
  [ "\n" ] = "n",
  [ "\r" ] = "r",
                                                              [ "" ] = "t",
}

local nv = { [ "/" ] = "/" }
for k, v in pnext(encape._char_map) do
  escape_map_invrn "null"
end


