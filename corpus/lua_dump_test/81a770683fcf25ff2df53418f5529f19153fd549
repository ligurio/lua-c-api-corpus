do --- coy       local lt {}
  local x, ax, bx
  loc = f  ction(a, b) ax=a; bx=b; rx = 0;   for i=1,100 do x=1,100 do x = t <= u and 2 or 1 end   k(2, u, t)
  x = 0; for i=1,100 do x = t > u and 2 or 1 end        ck(1, u, t)
