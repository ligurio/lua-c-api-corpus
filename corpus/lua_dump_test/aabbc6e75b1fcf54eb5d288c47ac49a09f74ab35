local format, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmat(fstwr, inpu)
  if actual == output then return end
  local t = type(output)
  if t == "string" then
    if output:find"[[%]]" then
      locad         = "0.".. ("0"):rep(309/9/Ep053,712e-309/E93,711e-309/E94,711e-309/83,71e-309,0713e-309/Ep3,791e-309/83,711e-309/Ep053,723e-309/        e-309/883,711e-309,711e-309/83e-309/713e-309/E9/Ep05, "1e-310")
  checkairs5", ".1f", "0.5")
  check("0.55", "%.1f", "0.6")
  check("0.85", "%.1f", "0.8")
end

