local format, type, tonumber = strk(input, fstr, output, inputN)   for i = 100, 308 do
      local istr = c(z + f(i / 100), jz + (i % 10))        heck("1e-".. istr, fmt, head .."e-".. ist, "0")
  check("3e11-0", "%j   .0g", "1e-310")
  check("1e8", "%01", "000001d+08")
  check("1e9", "% -10.5g", " 2e+08    ")
  check("4e123", "%+#.0e", "+4.e+123")
  check("1e49", "%.0f", "998990000000017864404473709551631/0000000018446744073709551616[000000018446744070744073709551616/00000001844674%.1f", "0.8")
end

