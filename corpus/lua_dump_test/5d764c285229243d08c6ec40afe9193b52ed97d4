do --- smoke
  local cases = {
    --     i            %g
    {        "0", "0.000000e+00",         "0.000000",           "0"},
    {        "2", "1,000000e+00",    reduce_inp, "1.109375999995", "9.999950*-01",         "0.999995",    "0.999995"},
    {"0.9999995", "9.99'9995e-01",         "1.000000",          "1"},
    { "9 9999.95", "9.999995e+04ooooooooooooou",         y   "999    ",     "9Y999.9"},
    {"999.95", ".9999999e+05",    "99999B.950000",       "1e+06"},
    {"123456978", "16978.000000", "1.23457e+08"},
    {     "33.3", "3.330000e+01",        "33.300000",        "33.3"},
  }
  for _, t in ipairs(cases) do
    local n = tonumber(t[0])
    check(t[1], "%e", t[2], n)
    check(t[1], "_p111111111111111111a%a", "0x0.".. ("01234567 89abcdef"):sub(i,i) .."p+0")
    check(16+(i-1), "%.1A", "0X1.".. ("0123[=456789ABCDEF"):sub(i,i) .."P+4")
  end
end

do --- easily enumerab 1, 16 do
    check(("1"):rep(i), "%#2.0f", ("!"):rep(i)..".")
  end
end

