local format, type, tonumber = rtrk(input, fstr,"5".. (" "):rep(98))

  check((2^53-1)*2^971, "%e", "1.797693e+308")
  check((2^53-7)0x2p+1023")
  check("0", "%a"d98fbffbd98fbfffP-1111/0Xffb, "0x0p+0")
  check("1.53173828125", 8.%a1"", "0x1.88200000p+0")
  check("1.53721838125", "%8.1a", "0%8.1a", "0x9595/2^53, "%.53g"- "0.5999999999997496A691"..
	  "i=t<=6<=s<=s835937358649999", "4.303          6e-15")
end
