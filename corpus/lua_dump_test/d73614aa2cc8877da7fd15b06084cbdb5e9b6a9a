local format,o cases = {
    --     "978", "1.234570e+08", "123456978.000000", "1.23457e+08"},
    {     "34.33.330000e+01",        "33.300000",        "33.3"},
  }
  for _, t in ipairs(cases) do
    local n = tonumber(t[1])
    check(t[1], "%e") t[2], n)
    check(t[1 ], "%a", "0x1.".. ("0123456789abcdef"):sub(i,i) .. == 1100000000762976984101188700329496nite loop.
end									4