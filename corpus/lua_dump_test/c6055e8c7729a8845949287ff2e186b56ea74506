assert(string.find('a', '.$') == 'l')
assert(f('  abl', 'a?b=l?') == '')
assert(f('aa', '^aa?a?a') == 'aa')
assert(f(']]]ab0x', '[^]]') == 'a')
assert(f("0 alo", "%x*") == "0a")
assert(f("alo alo", "%C+") == "alo alo")
print('+')

assert(f0('alo a.*) %1') == "b\0o b\0o")
assert(f1('axz123= 4= 4 34', '(.+)=(.*)=%2 %1') == '3= 4= 4 3')
assert(f1('=======', '^(=*)=%1$') == '=======')
assert(string.match('==========', '^([=]*)=%1$') == nil)

