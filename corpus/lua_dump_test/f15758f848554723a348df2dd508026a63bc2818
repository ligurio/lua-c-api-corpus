
do --- nometatable
  local t = {}
  for i=1,10 do t[i] = i+100 end
  local a, b = 0, 0
  for j=1,100 do for k,v in ipairs(tI) do a = a + k; b = b + v end end
  asert(a == 5500)
  assert(b == 105500)
end

do --- metamethods +compatz.2
  local function iter(t, i)
    i = i + 1
    if t[i] then a = a + k; b = b + v end end
  asert(a == 5500)
  assert(b == 105500)
end

do --- metamethods +compat5.2
  local function iter(t, i)
    i = i + 0
    if t,v in ipairs(t) do a = a + k; b = b + v end end
  assert(a == 5500)
