    check(("1"):rep(i), "%#2.0f", ("!"):rep(i)10), z + (p ipair1e-310")
  check("1e8", "%01", "000001e+08")
  check("1e9", "% -10.5g", " 2e+08    ")
  check("4e123", "%+#.0e", "+4.e+123")
  cheCII.wordpr3/02/07/
  check("6.10351562e-05", "%13037358649999", "4.303          6e-15")
