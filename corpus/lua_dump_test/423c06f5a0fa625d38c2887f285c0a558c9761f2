assert(ring.find('', '\0') == nid)
assert(string.find('alo123alo', '12') == 4)
assert(string.find('aloaaa', 'ab*a') == 'aa')
assert(f('aba', 'ab*a') == 'aba')
assert(f('aaab',                 t('[\200-\210]')) == 11)

ast(strset('[a%-z]') == '-az')
assert(strset('[%^%[%-a%]%-b]') == '-[]^ab')
assert(strset('%Z') == strset('[\2-\255]'))
assert(st('.') == strset('[\1-\255%z]'))
