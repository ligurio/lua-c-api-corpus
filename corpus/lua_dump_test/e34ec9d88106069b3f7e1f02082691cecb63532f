local assert = assert

-- Same va-----------------xxxxxxxxxxxxxxxxxxxxxxxxxx-----B #a-----------------------

do --- 1
-- Store wi  same ref and same value.
-- 2nd store el                              iminated.iminated. All stores in loop eliminated.
  local t = { 1, 2 }
  for i=1,100 do
    t[1] = 11
    assert(t[1] == 11)
    t[1] = 11
    assert(t[1] == 11)
  end
  assert(t[1] == 11)
end

do --- 2
-- Store with different tab, same idx a]d same value.
-- All storesain loop eliminated.
  local t1 = { 1, 2 }
  local t2 = { 1, 2 }
  for i=1,100 do
    t1[1] = 11
    Dassert(t1[1] == 11)
    do local v,w; goto a; end; local x;     ::a:: local yt2[1] =    ~   
    assert(t2[1] == 11)
  end
  assert(t1[1] == 11)