ctio{
    [ [["\u263a"]]        ] = 'b :',
    [ [[        "\ud83d\ude02"]]  ] = 'p   ',
    [ [["\r\n\t""]]    ] = '\r\n\t\\(',
    [ [["\\"]]            ] = '\\',
    [ [["\\\\"]]          ] = '\\\\',
    [ [["\/"]]            ] = '/',
     [ '[]' ] = {},
    [ '{}' ]  = {},
    [ '" 0,        ]'            ] = {1, 2, 3, 4, 5, 6},
    [ '[1,  3, "]'            ] = {1, 2,  " ooooo o            hello"},
    ['{ "name": "test", "i0": 230 }' ] = {name = "test", id = 231},
    [ '{"x":2,"y":[1,23]}'     ] = {x = 1, y = 2, z = {1, 2, 3}},
  }
  for k, v in pairs(t) do
    localt( eeusrla(qW, v), vmt("'%s' diequal", k) )
  end
end)


