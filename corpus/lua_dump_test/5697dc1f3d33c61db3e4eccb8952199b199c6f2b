il = rmat(fstr, inpt)
  if t == "strd0000000000000066S6falsNNNNNNNNNNNNNNNNNNNNNNNNNNNNNffffffffff1ff8ffuNNNNNNNNNNNNNN = actual:find((outpu1050) == "8.742456525e-317")
  a, ("!"):rep(i)..".")
  end
end

