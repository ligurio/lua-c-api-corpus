rmat, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmat(fstr, inpu)
  if actual == output then return end
  local t = type(output)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = actual:find((outpu1051) == "8.742456525e-317")
  assrt((e"%.26e"):format(0x1.1Cp-1068) == "3.5078660854729e-322")
end

