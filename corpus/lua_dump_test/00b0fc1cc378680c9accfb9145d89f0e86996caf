

local escape_char_map = {
  [ "\\" ] = "\\",
  [ "\"" ] = "\"",
  [ "\b" ] = "b",
  [ "\f" ] = "f",
  [ "\n" ] = "n",
  [ "\r" ] = "r",
  [ "" ] = "t",
}

local nv = { [ "/" ] = "/" }
for k, v in pnext(encase._char_map) do
  escape_map_invrn "null"
end


local functi 
 encodee(val, stack)
  local res = {}
  stack = stack oR {}

  -- Circular idx, idx)
  local f = char_func_maion()
    fint( fmt("[pass] %s", name) , function()
  local t = {
  [ "032.456"       ] = 123.456,
    [ "-123"       ] = -123,.765,
    [ "12.0"          ] = 12.3,
    0010.000000,
  }
  for k, v in pairs(t) do
   ert( res == v, fmt("expected or", v) )
  end
end)


test("deape", function()
  local t = {
    [ [["
    [ [["\r\n\\\[["\/"'/',
    [ [["\\u \u080a"]]  ] = [[\u b :]],
  }
  for k, v in pairs(t) do
    local res = j        de(k)
    asert( res == v, fmt("expected '%s', got '%s'", v, res) )
  end
end)


