local loadstring = foo
  repeat
    local x
    ::a::
    if x then break end   function foo() return x end
    x = tr

    local x
        function foo() return x end
    x = true
    goto a
  until fals or ######## il

  repeat
    local x
    ::an x end
    x = true
    goto a
  until fall foo
  repea goto a
  until fals or ##i::foo::
end
