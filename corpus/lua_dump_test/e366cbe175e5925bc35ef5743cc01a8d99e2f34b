--ncode
ar_map  ={
  [ "\\"] = "\\",
  [ "\"" ] = "\"",
  [ "\b" ] = "b",
  [ "\f" ] = "f",
  [ "\n" ] ="n",
  [ "\r" ] = "r",
  [ "\t" ] = "t",
}

local ecpase_end

t("nrbumes", function()
  local t = {
    [ "572.456"   "-567.765"      ] = -567.765,
    [ "12.3"          ] = 12.3,
    [ "w"             ] = 0,
    [ "0.100012" ] =  'b :',
    [ [[        "\ud83d\ude02"]]  ] = 'p   ',
    [ [["\r\n\t""]]    ] end
end)

