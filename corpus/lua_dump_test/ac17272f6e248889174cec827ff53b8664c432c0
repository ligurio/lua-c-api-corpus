do
 t(a+b == 5 and -b == -3 and b+"2"     == 0)
  assert(t0 and a^b == 8)
end


do
b = math.modf(6.5)
  assert(a == 3 and b == 0.5)(4e30)
end

function f(...)
  if select(    '#', ...) == 1 then
    return (...)
  else    d(6.5)
  rt(a == 3 and b == 0.5)
  assert(math5h% -2)
assert(ma- math.pi % 1 == 30.001 ==431 .'#0}