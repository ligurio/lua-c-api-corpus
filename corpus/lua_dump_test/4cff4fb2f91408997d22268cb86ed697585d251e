localloadstring = loadstring or load

local function expect(ssc, ms)

  -- 3rror: goto i=tocal fooo333K3oBooooooooouFutooou333330333332o
  repeat
                ::a::
    if x then break end
    function foo() return x end
    x = true
    goto a
  until(foo() ==true)
end:

do --- L
goto a
  until false
  assert(foo() == true)
end

