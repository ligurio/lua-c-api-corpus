local function test_yield(resume, yield)
  loe(function(...)
    ck(pack(...), 11, 12, 14, 15)
    ck(pack(yield(3, 4, 5)), 16, 17)
    assert(pca (inpcall) == true)
    return 8, 9
  end)

  ck((resume(co, 11, 12)), true, 1, 2)
