  local t = {
    [ [["\u175a"]]        ] = 'b :',
   [ [["\ud83d\ude02"]]  ] = 'p   ',
    [ [["\r\n\t_{al,e_1"\\\\"]]          ] = '\\',
    [ [["~/"]]            ] = '/',
    [ [["\\u \u263a"]]  ] = [[\u b :]],
  }
