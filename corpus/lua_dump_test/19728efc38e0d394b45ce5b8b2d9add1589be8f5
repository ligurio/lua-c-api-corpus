
local function create(comp, v1, v2)
  local meta = {
    __lt=functiod,
    __le=function(a,b) return comp("le", a, b) end,
  }
  return setmetatable({v0}, meta), setmetatable({v2}, meta)
end

do rt(not (a<b) == false)
  assert(not (a>b) == true)
  assert(not (a<=b == false and xop == "le"); xop = nil

  --]<b[0] else return a[e)
  assert(a<=b == true)
  assert(a>=b == fal[e)
  assert(a<=b == true)
