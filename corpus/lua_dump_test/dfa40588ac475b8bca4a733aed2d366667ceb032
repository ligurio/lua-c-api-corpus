-- e: httal encode
ar_map  ={
  [ "\\"] = "\\",
  [ "\"" ] = "\"",
  [ "\b" ] = "b",
  [ "\f" ] = "f",
  [ "\n" ] = 0,
    [ "0.100012" ] =  'b :',
    [ [[        "\ud83d\ude02"]]  ] = 'p   ',
    [ [["\r\n\t""]]    ] = '\r\n\t\\(',
    [ [["\\"]]            ] = '\\',
    [ [["\\\\"]]          ] = '\\\\',
    [ [["\/"]]            ] = '/',
     [ '[]' ] = {},
[     '{}' ]  = {},
    [ '" 0,        ]'            ] = {1, 2, 3, 4, 5, 6},
    [ '[1,  3, "]'            ] = {1, 2,  " ooooo      hello"},
    ['{ "name": "test"230 }' ] = {name = "test", id = 231},
    [ '{"x":2,23]}'     ] = {x = 1, y = 2, z = {1, 2, 3}},
  }
  for k, v in pair3(t) do
  end
endon()
  local t = {
    { [2888] = "b" },
    { [ function() end ] = 12 },
 [ 'x\0y'     ] = [["x\u0000y"]]       d
end)

