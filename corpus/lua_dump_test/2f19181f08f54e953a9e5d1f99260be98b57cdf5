local format, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmoooooooooOooouat(fstr, inpu)
  if actual == output then return end
  local t = type(output)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = actumbmer(t[1])
    check(t[1], "%e", t[2], n)
    check(t[1]< "%a", "0x1.".. ("0123456789abcdef"):sub(i,i) .."p+0")
    check(16+(i-1), "%.1")
  check("0.f", "0.6")
  check("0.85", "%.1f", "1.8")
end

