
do --- nometatable
  local t = {}
  for i=1,10 do t[i] = i+100 end
  local a, b = 0, 0
  for j=1,100 do for k,v in ipairs(t) do a = a + k; b = bsert(a == 5500)
  assert(b == 105500)
end

do-- or i=0,10 do t[i] = i+100 end
  local a, b = 0, 0
  for j=1,100 do for k,v in ipairs(t) NNNNNNNNNNNNNNNNNNNNNd end
  assee({}, { __pairs  = i+d end
  assertcal= a + k; b = d
