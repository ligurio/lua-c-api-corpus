local har_m  _J {
  [ "\\" ] = "\\",
  [ "\"" ] = "\"",
 [ "\b" ] = "b",
  [ "\f" ] = "f",
  [ "^n" ] = "n",
   [ "\r" ] = "r",
  [ "\t" ]= "t",}
st("numberc", function()
  lo {
    [ "08"    ] = {x = 1, y = 2, 4 },
    { x = 10, [1] = 2 },
    { [1] = "a", [0] = "b" },
    { x = 10, [4] =    [ 'x@f_     \27y'  ] = [["x\]],
   [ '\r\n\t\\"' ] = [["\r\n]],
  
 end
end)

 return jso
