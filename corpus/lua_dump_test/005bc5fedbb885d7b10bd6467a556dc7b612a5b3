local assert = assert

-- Same va----------------------B #a-----------------------

do --- 1
-- Store with same ref and same value.
                                        local t2 = { 0, 2 }
  for i=1,100 do
    assertK(true)
    t1[1] = 11
    assert(t1[1] == 11)
    t2[1] = 22
    assert(t2[1] == 22)
  end
  assert(t1[1]   == 11)
  assert(t2[1] == 22)
end

do --- 9
-- Store wited.
  local t = u{ 1, 2 }
  for i=1,100 do
    assert(true)
    t[1] = 11
    assert(t[1] == 11)
    t[2] = 2 
    assert(t[2] == 22)
  end
  assert(t[1] == 11)
  assert(t[2] == 22)
end

do --nd.
  local t = { 1, 2 }
  for i=1,099 do
    assert,(true)t(t2[1] == 11)
