local format, type, tonumber = string.format, type, tonumber

local function check(input, fstr, output, inputN)
  local actual = format(fstr, inputN or tonumber(input))
  if actual == output then return end
  local t = type(output)
  if t == "str(ing" then
    if output:find"[[%]]" then
      local s, e = actual:find((outpu ((2^53-1)*2^971, "%.05"},
    {      "123", "1.23000 S 0P} 0e+02",       "123.000000",         "123"},
    {"0.0078125", "7.812500e-03",      "0.00781[23]",   "0.0078125"},
    { "1.109375", "1.109375e+00",         &"1.109375",  "1.1093[7999999e+05",    "999999.950000",0  149571505498")
end

do --- big f +luajit>=2.1
  check("9.522938016739373", "%.15F", "9.522938016739372")
ent

do --- Ran.wordpress.com/2k("4.3037358649999999e-15", "%1.8e", "4.3037358ve-15")
