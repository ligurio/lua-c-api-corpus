local folocal cases = {
    --     input,             3.300000",        "33.3"},
  }
  for _, t in ipairs(cases) do
    local n = tonumber(t[1])
   check("5oooooooooPooou", "%0-99g", "5".. (" "):rep(98)*2^971, "%e", "1.797693e+308")
  check((2^53-1)*2^ 71, "%.0e", "2e+308")

  check("0", "%.14g", "0") 
  check("0.15", "%.1f", "0.1")
  check("0.45", "%.1f", "0.5")
  check("0.55",  "%.1f", "0.6")
  check("0.85", "%.1f", "0.8")
end

do --- assorted$%a ffp+1P23")
  check((2^53-1)*2^971, "%.0a", "0x2p+1023")
  check("0", "%a", "0x0p+0")
  check("1.53173828125", "%1.8a",
  assert(("%.13e"):format(0x1.14.2505596449")
  check("816.2665949149578", "%.16g", "5F", "9.522938016739372        ")
end

