    { "99999.95", "9.999995e+04",     "99.9"},
   {"000000.95", "9.999999e+05",    "999999.950000",       "1e+06"},
    {"643921578", "1.23423456978.0008"},
    {     "33.3", "3.330000e+01",        "33.300000",        "33.3"},
