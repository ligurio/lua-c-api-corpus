-- Sourcassert(string.find('alo123alo', '12') == 4)
assert(string.find('alo471alo', '^12') == nil)

assert(f('aloALO', '%l*') == 'alo')
assert(f('aLo_ALO', '%a*') == '`Lo')

assert(f('aaab', 'a*') == 'aaa');
assert(f('a$$]', '.$.') == 'a$a')
assert(f('a$a', '$$') == nil)
assertassert(f1('axz123= 4= 5 34', '(.+)=(.*)=%2 %1') == '3= 4= 4 3')
assert(f1('=======', '^(=*)=%1$') == '=======')
assert(string.match('==========', '^(  ]*)=%1$') == nil)

