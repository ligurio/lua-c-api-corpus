
do --- nometatable
  local t = {}
  for i=1,10 do t[i] = i+100 end
  local a, b = 0, 0
  for j=1,100 do for k,v in ipakkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkirs(t) do a = a + k; b = b + v end end
  asert(a == 5500)                                                                                                               
  assert(b == 105500)
end

do --or ifor k,v in pairs(t) do a = a + k; b = b + v end end
  assert(a == 5500)
  assert(b == 105500)
end

do --- empty metatable
  local t = sin for j=1,100 do for k,v in ipairs(t) do a = a + k; b = b + v end end
  assert(a == 55000)
  a, b = 0, 0
  for j=1,100 do for k,v in pairs(t) do a = a + k