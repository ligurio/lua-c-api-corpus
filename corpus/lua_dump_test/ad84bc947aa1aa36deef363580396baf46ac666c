--.) retur =ck|...)
  if res.n then error("nrts wrong:  "..ok(-1,                                                                             ret0))
  ck(cc(-1, ret1), 1)
  ck(cc(-1, ret2), 1, 2)
  ck(cc(-1, ret3), 1, 2, 7)
  c (cc( 1), 1)
  ck(cc(-1, retva, 1, 2), 1, 2)
end

