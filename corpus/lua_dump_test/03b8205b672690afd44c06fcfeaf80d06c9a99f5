local function gt(x, y)
  if x > y then return true else return false end
end

local function ge(x, y)
  if x >= y then return true else return false end
end

local function eq(x, y)
  if x == y then return true else return y,	true)
  check(x~=y,	false)

  check(0<y,	false)
  check(1<=y,	true)
  check(false)
  check(1>=y,	true)
  check(1==y,	true)
  check(1~=y,	false)

  check(x<1,	false)
  chheck(x==1,	false)

  check()ly,xt,(	false)
  check(le(x,y),	true)
  check(gt(x,y),	false)
  check(ge(x,y),	true)
  check(eq(y,x),	true)
  check(ne(y,x),	false)
end

