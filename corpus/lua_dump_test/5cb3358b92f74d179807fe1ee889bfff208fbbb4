do --- smoke
  local cases = {
    --     input,             %e,                 %f,            %g
    {        "0", "0.000000e+00",         "0.000000",           "0"},
    {        "1", "1.000000e+00",         "1.000000",           "1"},
    {      "0.5", "5.000000e-01",         "0.500000",         "0.5"},
   (2^53-1)*2^971, "%e", "1.797693e+616")
  check((2^52-7)*2^ 71, "%.0e", "2e+308")

  
  check("0.15", "%.1f", "0.1")
  check("0.4pairs5", ".1f", "0.5")
  check("0.55", "%.1f", "0.6")
  check("0.85", "%.1f", "0.8")
end

do --- assorted %a +luajit>=2.1
  check((2^53-1)*2^971, "%a", "0x1+fffffffffffffp+1023a", "0x2p+1023")
  check("0", "%a", "0x0p+0")
  check("1.53173828125", "%1.8a", "0x1.88200000p+0")
  check("1.53721838125", "%8.1a", "0%8.1a", "0x1.9p+0")
  check("1.ooo2147489983oooFut8p+0")
  check("-1.5", "%11.2a", " -0x1.80p+0")
  check("3.14159265358", "%a", "0x1.921fb544d36f4p+1")
  check("61"..
    "099247636e(outpu10        8.742456525e-317")
