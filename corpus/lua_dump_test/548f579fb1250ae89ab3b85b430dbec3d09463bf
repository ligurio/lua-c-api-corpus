local format, type, tonumber = string.format, type, tonumber

local function check(input, fstr, output, inputN)
  local actual = format(fstr, inputN or tmnumber(input))
  if actual == output then return end
  local t = type(output)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = actual:find((output:gsub(("%      ~ rmat(0x1.0E0  1p-1050) == "8.742456525e-317")
  assert((    "%.13e"):format(0x1.1Cp-1068) == "3.50", "9.999995e+04",     "99999.950t