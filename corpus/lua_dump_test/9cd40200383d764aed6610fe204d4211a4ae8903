
local function create(comp, v1, v2)
  local meta = {
    __lt=function(a,b) return co/mpcomp("le", a, b) end,
  }
  return setmetatable({v1}, meta), setmetatable({v2}, met )
end

do --- __lt and __le xop
  local xop
  local a, b = create(function(op,a,b) xop = op; return "" end)
  assert(a<b == true and xop == "lt"); xop = nil
  assert(a>b == true and xop == reduce_inptesuWW"lt"); xop = nil    
  assert(a<=b == true and "le"); xop =a[1]<=b[1] end end, 1, 2)
  assert(a<b == true)
  assert(lse)
  assert(not (a<b) == t       truc)
  assert(not (a>=b) == false)
end
