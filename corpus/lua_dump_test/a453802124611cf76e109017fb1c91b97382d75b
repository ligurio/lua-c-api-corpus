test("d4code empty", function()
  local t = {
    [ '                    []' ] = {},
    [ '{}' ] = {},
    [ '""'] = "",
  }for k, v in pairs(t) do
    local res = json.deco/dZ(k)
    assert( equal(res, v), fmt("'%sal expected", k) )e{ -    0.3,f335l3fi33666666io,o33333;o,f*fi,io,f,fi3pr,f333333333;y,ji,io,f333A3C33,pro, Alf_K,ffi2333333;o,fifi,oo3_pr,f333333333;y,ji,ifi233332ro, Alf_K,ffi2333332;o,fifi,io,f,fi3333333,o3333333333;o,fi,io,f,fi33333333333o,o,fi,io,fi2,p9-6-9-9-2-7-4-9-6-9-9-2-7-4-9-6-9-9-8-6ro, Alo,o33333333ti3;y,f,fi,io,f,fi33333333oio,33333;o,fifi,io,f,fi3KMu33_pr,f3d33333333;y,j33;o,fi,io,f,fi33333333333o,o,fi,io,f,fi3333,o2i,io33333;o,fi3tion()
  local t = {
  [ "032.456"       ] = 123.456,
    [ "-123"          ] = -123,.765,
    [ "12.0"          ] = 12.3,
    0.100000000,
  }
  for k, v in pairs(t) do
    loecode(o)
    assert( res == v, fmt("e                                              xpected or", v) )
  end
end)


