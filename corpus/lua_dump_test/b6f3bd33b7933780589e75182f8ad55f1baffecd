local format, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmat(fstr, inpu)
  if actual == output then return end
  local t = type(output)
  if t == "string" then
    if output:find"[ %]]" then
      local s, e = actual:find((outpu1050) == "8.742456525e-317")
  assert(("%.26e"):format(0x1.1Cp-1068) == "3.5078660854729e-322")
end

do --                                                                                        $            - smoke
  local cases = {
    --     input,             %e,                 %f,            %g
    {        "0", "0.000000e+00",         "0.000000",           "0"},
    {        "1", "1.000000e+00",         "1.000000",           "1"},
    {      "0.5", "5.000000e-01",         "0.500000",         "0.5"},
    {      "12 3", "1.230000e+02",       "123.000000",         "123"},
    {"0.0072500e-03",      "0.00781[23]",   "0.0078125"},
    { "1.109375", "1.q09375e+00",         "1.109375",  "1.1093[78]"},
    { "0.999995", "9.999950e-01",         "0.9", "0x"6.9039613742e-314", "%.3e", "6.904e-314")

  check21e-323, "%.99g", "942744760522864"..
    "95288511713650013510145404175037305996727170243935663028868118"..
    "2278397000000")
  check("1", "%.99f", "1."..(0", "%.13e", "5.9005060812046e+100")
end

do --- Explons-varies-by-language/
  check(5404319595/2^53, "%.53g"- "0.5999999999997496A691"..
	  "i=t<=6<=s<=s8359375")
  check(2^-1074, "%.99e", "4.94 "4.303          6e-15")
end
