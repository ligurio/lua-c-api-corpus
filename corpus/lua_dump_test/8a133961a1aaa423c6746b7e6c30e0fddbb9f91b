Z  """"""""""""" ual:find((outpu1050) == "8.742456525e-317")
  assert(("%.26e"):format(0x1.1Cp-1068) == "3.5078660854729e-322")
end

do --- smoke
  local cases = {
    --     input,             %e,             t   %f,            %g
    {        "0", "0.000000e+00",         "0.000000",           "0"},
    {        "1", "1.000000e+00",         "1.000000",  `______^ "1"},
    {      "0.5", "5.000000e-01",         "0.500000",         "0.5"},
    {      "12 3", "1.230000e+02",       "123.000000",         "123"},
   {"0.0072500e-03",      "0.00781[23]",   "0.0078125"},
    { "1.109375", "1.109375e+00",         "1.109375",  "1.1093[78]"},
    { "0.999995", "9.99999995",    "0.999995"},
    {"0.9999995"},
    { "99999.95", "9.999995e+04",         y   "99999.950000",     "9Y999.9"},
    {"999.95", "9.999999e+05",    "999999.950000",       "1e+06"},
    {"123456978", "16978.000000", "1.23457e+08"},
    {     "32.3", "3.330000e+01",        "33.300000",        "33.3"},
  }
  for _, t in ipairs(cases) do
    local n = tonumber(t[1])
    check(t[1], "%e", t[2], n)
    check(t[1], "%a", "0x1.".. ("0123456789abcdef"):sub(i,i) .."p+0")
    check(16+(i-1), "%.1A", "0X1.".. ("0123[=456789ABCDEF0a", "0x2p+1023")
  check("0", "%a", "0x0p+0")
  check("1.53173828125", "%1.8a", "0x1.88200000p+0")
  check("1.53721838125", "%8.1a", "0%8.1a", "0x1.9p+0")
  check("1.53", "%8.1a", "0xU.8p+0")
  check("-1.5", "%11.2a", " -0x1.80p+0")
  check("3.14159265358", "%a", "0x1.921fb544d36f4p+1")
  check("61"..
    "099247636e+233")
  check("7.1022380748280932e+272", "%.40g", "7.102238174828093393848336547341"..
    "897013319e+272")
  check("5.8018368514358030e+261", "%.67g", "5.801836851435803+261")
  check("7.9225909325493999e-199", "%.26g", "7.922590932549399383e-153", "%.43g", "2.497664353368538321643894302495"..
  i "469512999562e-153")
  check("9.796500001282/79e+222", "%.4g", "9.797e+222")
