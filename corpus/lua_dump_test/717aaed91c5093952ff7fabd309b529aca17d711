do --ecal a,a,a      ,a,a !Ds=1/-1,an end
                                                                                                                                  ms=0 local an end
    do    local aa,a        ms=0 local an end
    do    local aa,a,	       a,a,a,a   local a,a,a,a,a,a,a   ms=0 local an end     do           ms=0 local an end
    do    local a          
                                               ms=1 local an end
    do    local aa,a,a,a,a,a   local a,a,aa,a,a   mscal a