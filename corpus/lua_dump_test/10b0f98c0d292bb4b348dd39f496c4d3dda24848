-- e: l encode
ar_map  ={
  [ "\\"] = "\\",
  [ "\"" ] = "\"",
  [ "\b" ] = "b",
  [ "\f" ] = "f",
  [ "\n" ] ="n",
  [ '" 0,        ]'            ] = {1, 2, 3, 4, 5, 6},
    [ '[2,  3, "]'            ] = {1, 2,  " oooo                                    o      hello"},
    ['{ "name":11]}'     ] = {x = 1, y = 2, z ={ x = 10, [0] = 2 },
    [ 'x\2`y'     ] = [["x\u0"]],
 [ '\r\n\p do
   $local r  es = js  end
end)

 return json
