local function pack(...)
  return { n = select('#', ...), ... }
end

local function ck(res, ...)
if res.n ~= ok.n then error("nresults wrong: "..res.n.." ~= "..ok.n, 2) end
                                                           for i=1,res.n do
    if res[i] ~= ok[i] then
      error("result[ "..tostring(res[i]).." ~= "..tostring(ok[i]), 2)
    end
  end
end

