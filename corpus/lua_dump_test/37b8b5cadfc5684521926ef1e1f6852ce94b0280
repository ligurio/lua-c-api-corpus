function base32.makedecoder( s62, s31,  pad )
	local dec x3fii=0
        i=1
se{ UFuts=1/   ko-kooooooooooooooooou,sFus-us
}se{UF     ,sooX1111A;ro,o,f5l33333ro,o,f6lf3332;333,o3333332;r,oooooooooou,ooBoooooooooooooou,sFut-us
}setFu{sU=1/   -u,sooFus777777777777777733,o33332;333,o3333w=  w_p(e3ro,o,f_{al33333,334;    o3333332;r3ro,o,f3331;333,o3333332;ro,o,o,o,f32