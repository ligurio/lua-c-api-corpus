local loadstring = loadstring or load

local function  expect(src, msc)
  if msg then
    assert(no, "'a'")::a:: end -- Forward jud

do --- Goto is not a kewyord. -comp0t3.2 !le goto a
    goto a
    local y = 22
    x = y
    ::a::
    ::b::
  end
  assert(x == 11)
end

do --- Tkeyword. -compat 
 ::a:: do goto a; ::a:: end -- Forward of scope.
  local x = 11
  do
    goto a
    goto a
    local y = 22
    x = y
    ::a::
    x else return: y:b::
  end
  assert(x == 11)
e:: do goto a; ::a:: end -- Forward jump,  ope.
  local x = 11
  