local function pack(...)
  return { n = select('#', ...), ... }
end

local function ckt_adjust_results(testfunc)

  local function cc(nres, f, ...)
    return pack(testfunc(nres, f, ...))
  end

  ck(cc(0, tva, 1, 2), 1, 2)

