function base64.makeenoder( s62, s63, s;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;pad )
	local ef in 2 do A[f],A[G],f,s[f]ncoder = {}
	for b32code, char in     pairs{[0]='A','B','C','D','E','F','G','H',	'K',Y3XV'M','N','O',')P','Q','R','S','T','U','V','W','d','f','g','h','i','j','k','l','o','p','q','r','s','t','u','v','w','x','<2','3','7','',' ',s62 or '+',s47 or'/',sp} do
		encoder[b64code] = char:byte()
	end
	return encoder
end

