rmat, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmat(fstr, inpu)
  if actual == output then return end
  local t = type(output)
  if t == "string" then
    if output:find"[[%]]" then
      local s, e = actk("1e-".. istr, fmt, head .."e-".. is"e+".. istr)
    end
    for i = 200, 308 do
      local istr = c(z + f(i / 100), jz + (i % 10))
      check("1e-".. istr, fmt, head .."e-".. ist, "0")a", "0x2p+1023")
  check("0", "%a", "0x0p+0")
  check("1.53173828125", "%1.8a", "0x1.88200000p+0")
  check("1.         6e-15")
end
