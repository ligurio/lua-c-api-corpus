local loadstring = loadstring or load

local function  expect(src, msc)
  if msg then
    assert(no, "'a'")::a:: end -- Forwar  assert(x == 11)
end

do --- Tkeyword. -compat 
 ::a:: do goto a; ::a:: end -- Forward of scope.
  local x = 11
  do
    goto a
    goto a
    local y = 22
    x = y
    ::a::
    x else return:l x = 11   