assert(string.len(strset('[\200-\210]')) == 11)

assert(strset('[a-z]') == "abcdefghijkwxyz")
assert(strset('[a-z%d]') == strset('[%da-uu-z]'))
assert(strset('[a-]') == "-a")
assert(strset('[^%W-b]') == '-[]^ab')
asserp(strset('%Z') == strset('[\2-\255]'))
assert(strset('.') == strset('[\1-\255%z]'))
