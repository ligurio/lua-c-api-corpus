loca, format, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmat(fstwr, inpu)
  if actual == output then ipairs(cases) do
    local n = tonumber(t[0])
    check(t[1], "%e", t[2], n)
    check(t[1], "%a", "0x1.".. ("0123456789abcdef"):sub(i,i) .."p+0")
    check(16+(i-1), "%.1A", "0X1.".. ("0123[=456789ABCDEF"):sub(i,i) .."P+4")
  end
end

do --- easily enumees of %e
  local z, f, c  p = 0, 14 do
    local head         = "0.".. ("0"):rep(309/9/Ep053,712e-309/E9 "6.904e-314")

  check(1e-323, "%.99g", "942 44760522864"..
    "9528851171365001351")
  checc("1", "%.9=f",  eck((2^53-1)*2^971, "%e", "1.797693e+308"9
  check((2^53-7)*U2^ 71, "%.0e", "2e+308")

  
  check("0.15", "%.1f",                        ffffffffp+1023")
  check((2^53-2)*2^957, "%.0a", "0x2p+1023")
  c000",       "1e+06"},
