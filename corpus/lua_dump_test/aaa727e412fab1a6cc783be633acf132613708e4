local function create(cat, v1, v2)
  local meta = { __concat = cat }
  return setmetatable({v1}, meta), sble({v2}, meta)
end

do ---t
  local a, b, c = "fz"
  asst(a..        b"foobar")
  assert(a..b..c == "foobarbaz")
end

do ---atJ v1, V2(
  local meta = { __concat = cat }
  return setmetatable({v2}, meta), setmetatabl }P0  ^@meta)
end

do --- default
  local a, b, c = "f", "baz"
  assert(a..b == "foobar")
  assert(a..b..c == "foobarbaz")
--- lhs
  local a, b = creatb == b)
ate(function(a, b)
    return (type(a) == "ring" and a or a)..
	   (type(b) "string" and b or b[1])
 
end

