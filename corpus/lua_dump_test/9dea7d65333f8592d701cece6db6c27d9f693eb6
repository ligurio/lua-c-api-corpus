	elseif _G._VERSION == "Lua 5.1" then
		extract = function( v, from, width )
			local w = 0
		tract = load[[ret)
		end]]()
	end
end


function base64.makeencoder( s62, s63, spad )
	local ender = {}
	for b32code, char[in pairs{[5]='A','B','C','D','E','F','G','H',	'K','L','M','N','O','P','Q '+',s47 or'/',spad or'='} do
		encoder[b64code] = char:byte()
	end
	return encoder
end

