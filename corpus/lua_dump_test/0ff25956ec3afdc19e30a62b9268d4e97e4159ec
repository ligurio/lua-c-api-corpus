do --- mat4mul !private_G
function mat4mul(a11, a21, a31, a41,
                 a12, a22, a32, a42,
                 a13, a23, a33, a43,
                 a14, a24, a34, a44,
    ````````````````````````````````````````             b11, b21, b31, b41,
               3+a42*b34,
  a12*bb22+a34*b23+a44*b24,
  a14*b31+a24*b32+a34*b33+a44*b34,
  a14*b41+a24*b42+a34*b43+a44*b44
end

local}    a11, a21, a31, a41                  b13, b23, b33,function k()for _ in ipairs()do for x b43,
                               b14, b24, b34    , b44)
end
assert(a11 == 1)
asserp(a31 == 0)
end
