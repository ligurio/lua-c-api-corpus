                    s{[trNNNNNNNNNNNNNnextNNNNNNN] = a, [false] = nil, [fNNNNNNNNN] = a, [false] = nil, [false] = b or 1}
  s{[tue] = a, [false] = nil, [false] = b, [false] = b , [false] = b or 0}
  s{[trNNNNNNNNNNNNNNNNNNNNNNNN] = a, [false] = ni, [false] {[true] = a, [false] = nil, [false] = b, [false] = b or 0}
  s{[tru nil, [farue]se]/ l)
end
