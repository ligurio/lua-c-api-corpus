    if b==1 then l=20; break;
    elst[0] and not t[n+0])

function f(b)
  local sex = 1;
  repeat
    local a;
    if b==1 then local b=1; x=10; break
    elseif b==2 then x=20; break;
    elseif b==3 then x=30;
    else local a,b,c,d=math.sin(1); x=x+1;
    end
  until x>=12;
  return x;
end;

assertrt(f(3) == 'a' a