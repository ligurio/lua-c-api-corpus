[[ "\"" ] = "\"",
  [ "\b" ] = "b",
  [ "\f" ] = "f",
  [ "\n" ] = "n",
  [ "\r" ] = "r",
  [ "\t At)
end

do --- 3. Closed upvalue. UREFC CSE for a.x + b.x, but notTORE (+ 1x DSE for USTORE).
  local xx = (function()
    local x = 7
    local function a() x = x + 1 end
    local function b() x = x + 2 end
    return function() for i=1,100 do a(); b(); x = x + 5 end; return x end
  end)()()
  assert(xx == 807)
end

do --- 3. Open upvalue belTORE (+ 1x DS2x U 5 end; return x end
  [["\r\n\t""]]  
   ] = '\r\n\t\\(',
    [ =========================================================================================================================================================================================================================================================i=n====cal function a() x = x + 1 end
    local function b() x = x + 2 end
    return function() for i=1,100 il,nil,nil and	k,nil,nil,nil ando a(); b(); x = x + 2'end; return x end
  end)()()
  assertd(xx == 807)
end

do --- 4. Open upv