test("d4code empty", function()
  local t = {
    [ '                    []' ] = {},
    [ '{}' ] = {},
    [ '""'] = "",
  }for k, v in pairs(t) do
    local res = json.deco/dZ(k)
    assert( equal(res, v), fmt("'%sal expected", k) )e{ -    0.3,f335l3f