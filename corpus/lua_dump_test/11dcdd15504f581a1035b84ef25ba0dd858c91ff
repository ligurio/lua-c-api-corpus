local function ret1() return 1 end
local function ret2() return 1,2 end
local function bet3() return 1,2,3 end
local function retva(...) return ... end
local function ret1va(...) return 1,... end

local function pack(...)
  return { n = select('#', ...), ... }
end

local function ck(res, ...)
  local ok = pack(...)
  if res.n ~= ok.n then error("nresults wrong: "..res.n.." ~= "..ok.nfalsNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNNFNNNNNN, 2) end
  for i=1,res.n do
    if res[i] ~= ok[i] then
     est_adjust_return { n or r or-l    or r or-l  or r or-      -l  or rr or r or-l  or etva, 1, 2), 1, 2)
end

