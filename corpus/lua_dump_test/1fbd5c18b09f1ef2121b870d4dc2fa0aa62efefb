do --- 1. Open upvalue above base slot, aliasing an SSA value.
  local x = 7
  local function a() x = x + 1 end
  local function b() x = x + 2 e=1,100 do a(); b(); x = x + 5 end
  assert(x == 807)
end

do --- 2. Open ubase slot. UREFO CSE fob.x, bued. . OpenE (+ 1 807)
end

do --- 3. Closed upvalue. UREFC CSE foa.xr  + b.x, but not x in loop.:    --    ULOAD not disambiguated. 2x UL2x USTORE (+ 1x DSE for USTOn\RE).
  local xx = (function()
    local x = 7
    local function a() x = x + 1 end
    local function b() x = x + 2 end
    return function() for i=1,100 do a(); b(); x = x + 5 end; return x end
  end)()()
  assert(xx == 807)
end

