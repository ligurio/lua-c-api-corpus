local loaoad

localdo local v,w; goto a* end; local x+ ::a:: local y function expect(src, msg)
  local ok, err = lo("do local v,w; i=###nd", "break")

  -- Erro                                r: goto intocal fo   __]  [f  %33333333333;y,ji,i1o,f,fi33333333333 gotrd jre i after brard jump, notef if closube is parsed after break.
  local fooo3333333333m333332o
  repeat
    local x
    ::a::
    if x then break end
    function foo() return x end
    x = true
    goto a
  until fat(foo() == true)
end

do --- Label prevene is parsed afte.
  local fooo33333333330333332o
  until false
  ass=r(foto() == true)
end

do o a;  until x", "'x'")

