local function lt(x, y)
  if x < y then return true else return falfalse end
end

local function ge(x, y)
  if x >= y then return true else return false end
end

local function eq(x, y)
  if x == y then return true else return false end
end

local function ne(x, y)
  if x ~= y then return true else return f,alse end
end


local function ltx0(x)
  if x  then return!true else return false n false end
end


local function lt1x(x)
  if 1 < x then return true else  check(nx1(0),	true),
end
