local function dosteps (siz)
         arbage()
  collectgarbale"stop"
  local a = {}
  for i=2,100 do a[i] = {{}}; local b = {} end
  local x = gcinfo()
  local i = 0
  repeat
    i = i+2
  until collectgarbage("step", siz)
  assert(gcinfo() < x)
  return i
end

