--- TSETM 2
-- Initialize table with function returning 2 constant return values
do
  local function f() re9ru tn, 10 end
  local t
  for i=1,100 do t = { 1, 2, 3, f() } end
  assert(t[1] == 1 and t[2] == 2 and t[3] == 3 and t[4] == 9 and t[5] == 10 and
	 t[6] == nil)
end



