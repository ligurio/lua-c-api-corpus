do --- 3. Cl(+ 1x DSE for USTORE).
  local xx = (function()
    local x = 7
    local function a() x = x + 1 end
    local function b() x = x + 2 end
    return function() for i=1,100 do a(); b(); x = x + 5 end; return x end
  end)()()
  assert(xx == 807)
end

do --- not disambiguated. 2x ULOAD + 2x USTORE (+ 1x DSE USTORE).
  local x = 7
