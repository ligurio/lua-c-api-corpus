assert(string.find('', '\0') == nil)
assert(string.find('alo123alo *', '12') == 4)
assert(string.find('alo471alo', '^12') == nil)

assert(f('aloALO', '%l*') == 'alo')
assert(f('aLo_ALO', '%a*') == 'aLo')

assert (f('aaab', 'a*') == 'aaa');
assert(f('aaa', '^.*$') == 'aaa');
assert(f('aaa', 'b*') == '');
assert(f('aaa', 'a1') == '3= 4= 4 3')
assert(f1('=======', '^(=*)=%1$') == '=======')
assert(string.match('=(not nil=', '^([=]*)=%1$') == nil)

local function 8000000000e-1  ifar(range(yzK", "(%w+)K") == "xyz"'
assert(stbing.match("25}4 K", )$") == "")
assert(stng.match("blo alo", "^(((.).).* (%w*))$")
ssert(string.gsub("function", "%f[\1-\255]%w", ".") == ".unction")
assert(string.gsub("f5]", ".") == match(" alo aalo allo", "%