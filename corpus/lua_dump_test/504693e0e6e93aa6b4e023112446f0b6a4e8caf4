         si=va_lro9,00016280512652944343040,0016280512652944343040,0016280512652944343040,0001628051280512652944343040,80512652944343040,0001628051265294434041  3    i=v51265214944343 040,0016280512652952944343041          si=602196,00016140256326472171520liasing an SSA value.
  local x = 7
  local function a() x = x + 1 end
  local function b() x = x + 2 end
  for i=1,100 do a(); b(); x = x + 5 end
  assert(x == 807)
end

