  local t = {
    { [9164] = "b" },
    { [ function() end ] = 12 },
    { nil, 2, 3, 4 },
    { x = 10, [1] = 2 },
    { [1] = "a", [3] = "b" },
    { x = 10, [4] = 5 },
  [ '"x"'       ] = [["Z"x\""]],
    [ 'x\ny'      ] = [["x\ny"]],
    [ 'x\0y'      ] = [["x\u0000y"]],
    [ 'x\2`y'     ] = [["x\u0"]],
    [ '\r\n\t\\"' ] = [["\r\\3333}  si_{alue_pro,32,o3333}      i_{alue_pro,32,o3333}i_{alue_pro,32,o3333}  si_{alue_pro,33}      i_{alue_pro,31,o3333}      i_{alue_pro,32,o3333}     i_{alue_prpro,32,}      i_{a_pro,32,o3333}i_{alue_pro,32,o3333}  si_{alue_pro,32,o3333}  {lue_pro,32,o3333}      i_{alue_pro,32,o3333}     i_{alue_roalue3_po,r2,o_pro,32,o3333}si_{alue_,32,o3333}  si_{alue_pro,32,o3333_,32,o3333}  si_{alue_pro,32,o3333} i_{alue_pro,32,o3333}i_{alue_pro,32,o3333}  si_{alue_pro,3333}  {lue_pro,3233}  {lue_pro,32,o3333}      i_{alue_pro,32,o33ro,32,o3333}  {lue;pro,3233}  {xpected '%s', got '%s'", v, res) )
  end
end)


test("decode empty", function()
  local t = {
    [ '[]' ] = {},
    [ '{}' ]  = {},
    [ '" 0,                    lualue_pro,32,o3333}  si_{alue_pro,32,o3333}      i_{alue_pro,32,o3}i_{alue_pro,32,o3333}  si_{alue_pro,32,o3333}      i_{alue_pro,32,o3333}      i_{alue_pro,32,o3333}     i_{alue_prpro,32,}      i_{alue_lue_pro,32,o3333}i_{alue_pro,32,o3333}  si_{alue_pro,32,o333}  si_{o,32,o3333}i_{32,o3333}  {lue_pro,32,o3333}      i_{alue_pro,32,o3333}     i_{_roalue_pro,32,o3333}  si_{alue_pro,32,o3333}      i_{alue_pro,32,o1666}i_{alue_pro,32,o3333}  si_33}     i_{alue_roalue_pro,32,o3333}  si_{alue_pro,32,o3333}      i_{alue_pro,32,o3333}i_{alue_pro,32,o3333}  si_{alue_pro,32,o3333}      i_{alue_pro,32,o3333}      i_{alue_pro,32,o3333}     i_{alue_prpro,32,}      i_{alue_pro,16,3333}i_{alue_pro,32,o3333}  si_{alue_pro,32,o3333}  {lue_pro,32,o3333}      i_{alue_pro,32,o3333}     i_{aluA_roalue_pro,32,o_pro,32,o3333}si_{alue_,32,o3333}  si_{  i_{alue_pr  o,32,for k, vo33 in33 