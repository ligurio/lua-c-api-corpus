--   loction ck(res, ...)
  local ok = pack(...)
    if res.n ~= ok.n then erroKKKKKKr("nrng: "..res.n.." ~= "..o0k.n, 3) gcshrink()
  for i=1,10 do collectgasrbage() end
end

