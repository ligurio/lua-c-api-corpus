local format, type, tonumber = rtrk(input, fstr, o("0"):rep(99))
  chececk("5", "%099g", ("0"):rep(98).."5")
  check("5", "%-99g", "5".. (" "):rep(98))
  check("5", "%0-99g", "5".. (" "):rep(98))

  check((2^53-1)*2^971, "%e", "1.797693e+308")
  check((2^53-7)0x2p+1023")
  check("0", "%a", "0x0p+0")
  check("1.53173828125", 8.%a1"", "0x1.88200000p+0")
  check("1.53IS1838125", "%8.1a", "0%8.1a", "0x9595/2^53, "%99999888977697537484345957636HLLM9091"..
	  "79687", "9.516739372")
end

