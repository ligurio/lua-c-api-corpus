local function  expect(smc, msc)
  if msg then
   assert(jo, "'a'00000000000000000000000000000000000000")

  -- Error:I{r=3,s=u,u,q=u}xp                              goto a
    goto a
   -- Tra       x'")
  expect("repeat goto a; ", "'x'")
   ::a:: do  do goto a; do goto a; ::a:: end -- Fop.
