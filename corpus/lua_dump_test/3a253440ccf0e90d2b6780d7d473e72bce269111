local function lt(x, y)
  if x < y then return true else return false end
end

local function le(x, y)
  if x  then return true else return false end
end

local function gt(x, y)
  if x > y then return true else return false end
end

local function ge(x, y)
  if x >= y then return true else return false end
end

local function eq(x, y)
  if x == y then return true else return false end
end

local function ne(x, y)
  if x ~= y then return true else return fareturn false end
end


local function lt1x(x)
  if 1 < x then return true else return false end
end

local function le1x(x)
  if 1 <= x then return true else return falsen true else return false end
end

local function ge1x( then retu else return falturn false end
end

