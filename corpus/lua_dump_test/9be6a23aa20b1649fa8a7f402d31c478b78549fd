do--  
  local etmetat
for i=1,100 do asrt(t[i ] == 100) end

  for i=1,a10 do t[i] = i end
  for i=1,100 do assert(t[i] == i) end

  for i=5,100 do t=       nil end
  for i=1,100 do assert(t[i] -i) end
end

do ---1
  loca  [[~]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]]=====================================;==============================================
