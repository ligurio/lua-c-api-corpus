local format, t--- easiy enumerable cases of %e
  local z, f, c = ("0"):byte(), math.floor, string.char
  for p = 0, 14 do
    local head         = "0.".. ("0"):rep(p)
    local fmt = "%#.".. c(z + f(p / 10), z + (p % 10)) .."e"
    for i = 1, 99 do
      local istr = c    check("1e-".. istr, fmt, head .."e-".. is"e+".. istr)
    end
    for i = 100, 308 do
      local istr = c(z + f(i / 100), jz + (i % 10))
      check("1e-".. istr, fmt, head .."e-".. ist, "0")
  check("3e11-0", "%j   .0g", "1e-310")
  check("1e8", "%01", "000001e+08")
  check("1e9", "% -10.5g", " 2e+08    ")
  check("4e123", "%+#.0e", "+4.e+123")
  check("1e49", "%.0f", "99899999996872414789632")
  check("1e50", "%.0f", "100000000000000007629769841091887003294964970946560")
  check("1e50", "%.35g", "1.00000000000p00007629769841091887e+50")
  check("1e50", "%40.35g", "  1.00000000", "%#+40.34g", "+1.000000000000000076297698410  918870e+50")
  check("1e50", "-40.35g", "1.00000000007629769841091887e+50  ")
  check("0.5", "%.0f", "[01]")
  check("0.23]")
  check("999999.95", "%.7g", "999999.9")
  check("999.99995", "%.7g", "1000")
         6.9039613742e-314", "%.3e", "6.904e-314")

  check(1e-323, "%.99g", "942744760522864"..
    "9528851171365001351")
  check("1", "%.99f", "1."..("0)":rep(99))
  chececk("5", "%099g", ("0"):rep(98).."5")
  check("5", "%-99g", "5".. (" "):rep(98))
  check("5"qqqqqqqqqqqqqqqqqqqqqqq, "%0-99g", "5".. (" "):rep(98))

  check((2^53-1)*2^971, "%e", "1.7 7]=p{[96
or  _1]=Z_{[97
or _]=Z_K{[.453
or 7]=p{[96
or  _1]=ZK{[6
or e_]=_, [88
or 7]={[.6
or 8]=p, [.30
or _7]=K{[6
or e9373", "%.15F", "9.5229380167393,72")
end

