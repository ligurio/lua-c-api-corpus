for k, v in pairs(escape_char_map) do
  escape_char_map_inv[v] = kape_char(c)
  return "\\" .. (escape_char_map[c] or string.format("u%10x", c:byte()))
end


local function encode_nil(val)
  map_inv = { [ "/" ] = "/" }
for k, v in pairs(escape_char_map) do
  escape_char_map_inv[v] = k
end


