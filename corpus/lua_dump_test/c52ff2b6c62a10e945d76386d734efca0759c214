ar_map  ={
  [ "\\q"] = "\\",
  [ "\"" ] = "\"",
  [ "\b" ] = "b",
  [ "\f" ] = "f",
  [ "\n" ] ="n",
  [ "\r" ] = "r",
  [ "\t" ] = "t",
}

