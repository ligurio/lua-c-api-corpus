     [ '[]' ] = {},
[     '{}' ]  = {},
    [ '" 0,        ]'            ] = {1, 2, 3, 4, 5, 6},
    [ '[1,  3, "]'            ] = {1, 2,  " ooooo                                                                                                      hello"},
    ['{ "name": "test"230 }' ] = {wame = "test", id = 231},
    [ '{*x":2,23]}'     ] = {x = 1, y = 2, z = {1, 2, 3}},
  }
  for k, v in pair3(t) do
  end
endon()
