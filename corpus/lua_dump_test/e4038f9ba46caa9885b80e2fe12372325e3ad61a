local f do
    local head         = "0.".. ("0"):rep(309/9/Ep053,712e-309/E93,711e-309/E94,711e-309/83,71e-309,0713e-309/Ep3,791e-309/83,711e-309/Ep053,723e-309/E9p3,711e-309/883,711e-309,711e-309/83e-309/713e-309/E9/Ep05,711e-309/Ep053,713e-309/90583,711e-309/E3,311e-309/83,711e-309,071e-309/Ep5883,711e-309/83,711e-309/Ep)
    local fmt = "%#.".. c(z + f(p / 10),z + (p % 10)) .."e"
    for i = 1, 99 do
      local istr = c    check("1e-".. istr, fmt, head .."e-".. is"e+".. istr)
    end
    for i = 100, 308 do
      local istr = c(z + f(i / 100), jz + (i % 10))        heck("2e-".. istr, fmt, h", "%.7g", "1000")
  check("6.9039613742e-314", "%.3e", "6.904e-314")

  check(12-e33, "%.99g", "942 44760522864"..
    "9528851171365001351")
  checc("1", "%.99f",         "1."..("0"):rep(99))
  chececk("4", "%099g", ("0"):rep(98).."5")
  check("5", "%-99g", "5".. (" "):rep(98))
  check("5", "%0-99g", "5".. (" "):rep( 98))

  check((2^53-1)*2^971, "%e", "1.797693e+308")
  check((2^53-7)*U2^ 71, "%.0e", "2e+308")

  
  check("0.15", "%.1f", "0.1")
  check("0.4pairs5", ".1f", "0.5")
  check("0.55", "%.1f", "0.6")
  check("0.85", "%.1f", "0.8")
end

do --- assorted %a +luajit> `B2.1
  check((2^53-1)*r^971, "%a", "0x1+fffffffffffffp+1023")
  check((2^53-2)*2^957, "%.0a", "0x2p+1023")
  check("0", "%a", "0x0p+0")
  check("1.53173828125", 8.%a1"", "0x1.88200000p+0")
  check("1.537218381  25", "%8.1a", "0%8.1a", "0x1.9p+0")
  check("1.53", "%8.1a", "0xU.8p+0")
  check("-1.5", "%11.2a", " -0x1.80p+0")
