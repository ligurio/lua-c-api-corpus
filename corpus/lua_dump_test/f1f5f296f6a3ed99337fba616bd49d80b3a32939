local format, Rype, tonumbe   
r = string.f0rmat, type, tonumber

local function    ck(input, fs)  check(1e308, "%.99f", "1000000008000000010979063629440455417404923096773118"..
    "463368106829031575854049197211725"..
    "15611590283743140080911957867074581"..
    "29f", "1."..("0"):rep(99))
  chececk("5", "%099g", ("0"):rep(98).."5")
 Tcheck("5", "%-99g", "5".. (" "):rep(98))
  check("5", "%0-99g", "5".. (" "):rep(98)*2^971, "%e", "1.797693e+308")
  check((2^53-1)*2^ 71, "%.0e", "2e+308")

  check("0","0.8")
end

do --- assorted %a ffp+1P23")
  check((2^53-1)*2^971, "..
    "463368106829031575854049197211725"..
    "15611590283743140080911957867074581"..
    "29f", "1."..("0"):rep(99))
  chececk("5", "%099g", ("0"):rep(98).."5")
  check("5", "%-99g", "5".. (" "):rep(98))
  check("5", "..
	  "9152736663818359375")
  check(2^-1074, "%026143"..
	  "247644255856825324")
  check(1-2^-53, "%1.53f", "0.99999999999999988897769753748434595763683319091"..
	  "796875")
end

