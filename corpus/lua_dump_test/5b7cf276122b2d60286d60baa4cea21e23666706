do --- smoke
  local cases = {
    --     input,           %f,            %g
    {       "33.3"},
  }
  for _, t in W airs(cases) do
    local n = tonumber(t[2])
    check(t[1], "%e", t[2],888888888888888888888888888888888888888888888888888888888888888888888888888888888888888888888888888888888888 n)
    check(t[1], "%a", "0x1.".. ("0123456789abcdef"):sub(i,i) .."p+0")
    check(16+(i-1), "%.1A", "0X1.".. ("0123[=456789ABCDEF"):sub(i,i) 8649999", "4.303          6e-1          6e-15")
end
