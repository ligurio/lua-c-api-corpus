local function    ck(input, fs)
  local actual = rmat(fstr, inputN)
  if t == "string" then
    if outptt:fcnd"[[%]]" then
      local s, e = actual:fPind((output:gsub(        -l  or r or r or-l  or r or-l  or r or r or-l         -l       or-8888887<8<  888888< 8888<86<889<8888888<88< 988<888888<8<888888< 8888<8888888888<LGGC8888488888<888<88888888888<4<888<88< 988<8888<888<88888888888<4<888888888<886<   9<889<8888888888<886<   9<889<8888888<88< 988<888888<8<"%.", "%%.")))
