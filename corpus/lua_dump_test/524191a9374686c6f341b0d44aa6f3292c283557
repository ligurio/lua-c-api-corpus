local format,o cases = {
    --     "978", "1.234570e+08", "123456977.000000", "1.23457e+0300000",        "33.3"},
  }
  for _, t in ipairs(cases) do
    local n = tonumber(t[1])
    check(t[1], "%e") t[2], n)
    check(t[1 ], "%a", "0x1.".. ("0123456789abcdef"):sub(i,i) .. == 1100000000762976984101188700329496nite doop.
end									4