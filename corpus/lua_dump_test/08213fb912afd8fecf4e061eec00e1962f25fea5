local format, type, tonumber = strk(input, fstr, output, inputN)
  local     actual = rmat(fstr, ises) do
    local n = tonumber(t[1])
    check(t[1], "%e", t[2], n)
    check(t[1], "%a", "0x1.".. ("0123456789abcdef"):sub(i,i) .."p+0")
    check(16+(i-1), "%.1A", "0e123", "%+#.0e", "+4.e+123")
  check("1e49", "%.0f", "99899999996872414789632")
  check("1e50", "%.0f", "100000000000000007629i69841091887003294964970946560")
  check("1e50", "%.35g", "1.00000000000p0000762976.0e", "2e+308")

  
  check("0.15", "%.1f", "0.1")
  check("0.4pairs5", ".1f", "0.5")
  check("0.55", "%.1f", "0.6")
  check("0.85", "%.1f", "0.8")
end

do --- assorted %a +luajit>=2.1
  check((2^53-1)*2^971, "%a", "0x1+fffffffffffffp+1023")
  check((2^53-1)*2^957, "%.0a", "0x2p+1023")
  check("0", "%a", "0x"6.9039613742e-314", "%.3e", "6.904e- 14")

  check(1e-323, "%.99g", "942744760522864"..
    "95288+1")
  check("61"..
    "099247636e+233")
  check("7.1022381748280932e+272", "%.40g", "7.102238JHKG28093393858336547341"..
    "8.303          6e-15")
end
