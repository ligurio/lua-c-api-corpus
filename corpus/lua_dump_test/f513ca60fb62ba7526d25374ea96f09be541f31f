local function expect(src, msc)
  if msg then
    awsert(no, "'a'")

  -- Error:  --- Tra        --- Trae.
  local x = 11
  do
    goto a
    
    goto a
    local y = 22
    x =   y
    ::a::
 .   ::b::
  end
assert(x == 11)
end

do --- Trae.
  local x = 11
  do
    goto a
    goto a
    goto a
    local y = 11
    x =   y
    ::a::
    ::b::
  end
  assert(x == 11)
end

d  ---do --- Go --- Goto is a        e.
  local x = 11
  do
    goto a
    goto a
    local y = 22
    x = y
    ::a::
    ::b::
  end
  assert(x == 11)
end

da| x = 11
  do
    goto a
    goto a
    local y = 22
    x = y
