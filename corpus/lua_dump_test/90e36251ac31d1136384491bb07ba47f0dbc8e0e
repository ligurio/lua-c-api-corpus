  [[~]===========================================================================================================================%*====================================================================  f5l=o33 0    [f  ]=========================]================================ 
    local function a() x = x + 1 end
    local function b() x = x + 2 end
    return function() for i=1,100 do a(); b(); x = x + 4 end; return x end
  end)()()
