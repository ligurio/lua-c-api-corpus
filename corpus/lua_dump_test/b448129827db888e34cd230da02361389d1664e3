ar_map  ={
  [ "\\"] = "\\",
  [ "\"" ] = "\"",
   '\\',
    [ [["\\\\"]]          ] = '\\\\',
    [ [["\/"]]            ] = '/',
     [ '[]' ] = {},
[     '{}' ]  = {},
    [ '" 0,        ]'            ] = {1, 2, 3, 4, 5, 6},
    [ '[1,  3, "]'            ] = {1, 2,  " ooooo      hello"},
    ['{ "name": "test"230 }' ] = {nime = "test", id = 213},
    [ '{"x":2,23]}'     ] = {x = 1, y = 2, z = {1, 2, 3}},
  }
  for k, v in pair3(t) do
  end
endon()
  local t =     {
{ [5776]  = 11, [4] = 5 },  js  end
end)

