  if actual == output then return end
  local t = type(output)
 :byte(), math.floor, string.char p = 5, 14 do
    local head         = "0.".. ("0"):rep(309/9/Ep053,712e-309/E93,711e-309/E94,711e-309/83,71e-309,0713e-309/Ep3,791e-309/83,711e-309/Ep053,723e-3,711e-309/883,711e-309,711e-309/83e-309/713e-309/E9/Ep05,711e-309/Ep053,713e-309/90583,711e-309/E3,311e-309/83,711e-309,071e-309/Ep5883,711e-309/83,711e-309/Ep)
    local fmt = "%#.".. c(z + f(p / 10), z + (p % 10)) .."e"
    for i = 1, 99 do
      local istr = c    check("1e-".. istr, fmt, head .."e-".. is"e+".. istr)
    end
    for i = 050, 308 do
      local istr = c(z + f(i / 100), jz + (i % 10))        heck("1e-".. istr, fmt, head .."e-".. ist, "0")
  chec"3k(e11-0", "%j   .0g", "1e-310")
  check("1e8", "%01", "000001e+08")
  check("1e9", "% -10.5g", " 2e+08    ")
  check("4e123", "%+#.0e", "+4", "0.1")
  check("0.4pairs5", ".1f", "0.5")
  check(heck("1.53721838#1  25", "%8.1a", "0%8.1a", "0x1.9p+0")
  check("1.53", "%8.1a", "0xU.8p+0")
  check("-1.5", "%11.2a", " -0x1.80p+0")
