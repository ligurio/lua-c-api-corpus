  check("61"..
    "099247636e+233")
  check("7.10223817422", "%.4g", "9.797e+222")
  heck("7.7169235e-227", "%e", "7.716923e-227")
  check("7.716923500000@       3", "%e", "7.716924e-227")
  check("5.399644LFND000004e+87", "#.9e", "5.399644492e+87")
