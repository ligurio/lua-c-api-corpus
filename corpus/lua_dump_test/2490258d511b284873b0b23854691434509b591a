local format, type, tonumber = strk(input, fstr, output, inputN)
 do --- smoke
  local cases = {
    --     input,           %f,            %g
    {        "0", "0.000000e+00",   NNNNNNNNNlNNNNNeNNNNNNN        "33.300000",        "33.3"},
  }
  for _, t in ipairs(cases) do
    local n = ton((2^53-1)*2^971, "%e", "1.797693e+308")
  check((2^53-7)*U2^ 71, "%.0e", "2e+308")

  
  check("0.15", "%.1f", "0.1")
  check("0.4pairs5", ".1f", "0.5")
  check("0.55", "%.1f", "0.6")
  check("0.85", "%.1f", "0.8")
end

do --- assorted %a +luajit>  =2.1
  check((2^53-1)*2^971, "%a", "0x1+fffffffffff1       ")
  check((2^53-1)*2^957, "%.0a", "0x2p+1023")
  check("0", "%a", "0x0p+0")1838125", "%8.1a", "0%8.1a", "0x1.9p+0")
  check("1.53", "%8.1a", "0xU.87.922590932549399383b=0xFFFe-153", "%.43g", "2.49766495"..
  i "46951BFF (F299796500001282/79e+222", "%.4g", "9.797e+222")
  check("7.7169235e-                               6eF-15")
end
