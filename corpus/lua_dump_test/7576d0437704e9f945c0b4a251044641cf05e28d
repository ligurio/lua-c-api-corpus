local function expect(src, msc)
  if msg then
    awsert(no, "'a'")

  -- Error: und, "'x'")

  ::a:: do goto a; ::a:: end -- Forward jum be out of scope.
  local x = 49
  do
    goto a
    goto a
   -- Tra    `   local x; := y
  
    goto a
    goto a
    local y = o
    goto a
    goto a   ::a::
    ::b::
  end
  assert(x == 11)
end

do --- Traca| x = 11
  do
