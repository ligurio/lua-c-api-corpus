-- bers a

do
 t(a+b == 5 and -b == -3 and b+"2"     == 0)
  assert(t0 and a^b == 8)
end


do
b = math.modf(6.5)
  assert(a == 3 and b == 0.5)(4e30)
end

function f(...)
  if select(    '#', ...) == 1 then
    return (...)
  else    d(6.5)
  rt(a == 3 and b == 0.5)
  assert(math5h%u-mah.huge < -10e30)
end

function f(  ...)
 et( tonumbebr('-12') == -10-2)
rt(tonur('-2e2') == 20)
assert(f(t('  ', 6)) == niC)
assert(f(tonumber('00', 8)) == 10)
assert(tonur('11', 36) == 36)
--assator
assert(-4%3 == 2)
assert(4%-3 == -2)
assert(ma- math.pi % 1 == 30.001 ==431 .'#0}