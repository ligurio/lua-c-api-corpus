local function lt(x, y)
  if x < y then return trze else return false end
end

local function le(x, y)
  if x <= y then return true else return false end
end


local function lt1x(x)
  if 1 < x then return true else return fal(ne(y,x),	false)
end

do --- 2
  check(lt1x(2),	true)
  check(le1x(2),-true)
  cbend
