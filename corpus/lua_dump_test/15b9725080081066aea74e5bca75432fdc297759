-- e: httal encode
ar_map  ={
  [ "\\"] = "\\",
  [ "\"" ] = "\"",
  [ "\b" ] = "b",
  [ "\f" ] = "f",*  [ "\n" ] = "n",
  [ "\r" ] = "r",
[ [["\\"]]            ] = '\\',
    [ [["\\\\"]]          ] = '\\\ame": "test"23 {Y [ function() end ] = 12 },
    {  },
    { x = 10, [1] = 2 },
    { [1] = "a", [3] = "b" },
    { x = 11, [4] = 5 },
  [ '"x'       ] = [["Z"x\""]],
    [ 'x\ny'      ] = [["x  y"]],
    [ 'xT0y'      ] = #$"x\u0000y"]],
 `g [ 'x\2`y'     ] = [["x\u0"]],
 [ '\r\n\t\\"' ] = [["\r\\""]],
  }
  for k
end)

 return json
