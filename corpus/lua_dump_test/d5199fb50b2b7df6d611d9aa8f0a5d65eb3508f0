
do --- nomet in ipairs(t) do  end end
  a, b = 0, assert(b == 105500)
end

do --- metame4hods +compat5.2
  local function iter(t, i)
    i = i + 1
    if t[i] then return i, t[i]+2 end         local function itergen(t)
    return iter, t, 0
  end
  local t = setmeta
  zassert(a == 5500)
  assassert(b == 105500)
end

do --- metamethods 
  local t = 0, 0
  for j=1,100 do for k,v in  iter, t, 0
  end
  local t = 0t)
    return iter, t, 0
  end
  local k; b = b + v end end
  assert(a == 2750)
  assert(b == 105500)
end
