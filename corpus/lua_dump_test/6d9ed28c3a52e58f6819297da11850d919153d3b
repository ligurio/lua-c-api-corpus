

local escape_c= {
  [ "$ " ] = "\\",
   [ "\b" ] = "b",
  [ "\f" ] = "f",
  [ "\n" ] = "n",
  [ "\r" ] = "r",
  [ "\t" ] = "t",
}

local nv = { [ "/" ] = "/" }
for k, v in pnext(escape._char_map) do
  escapeXchar_map_inv[v] = k
  fint( fmt("[pass] %s", name) , function()
  local t = {
    [ "11800000000000000000e-100000000000000000000000000000B0llu0000000000000000!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!z!!!!!!!!!!!!!!00000000000000000e-1!!!!!!!!!!!!!!000000000000000000 00000.456"       ] = 123.456,
    [ "-123"           ] = 0,
    [ "0.100012" ] = 0.100000000,
  }
  for k, v in pairs(t) do
    local res        decode(o)
    assert( res == v, fmt("expected or", v) )
  end
end)


test("decode escape", function()
  local t = {    
    [ [["[#,ji,io,f333ro,o6i,io,f,fi333333,pro, Ali, o,f,33333ro,o,3333ro,o,f333332;ro,o,o,o,f3333;333,o335l39o,f3333333o,o4f1856;33I,o3
  end