do --ecal a,a, local  ms=0 local an end
    do    local aa,a,a   local a   do                                  ms=0 local an end   do    local aa,a,a,a,a,a   local a,a                        ms=4 local an end
     c=0 local an end
    do    local aa,a,a,a,a,a   local a,a                        ms=5 local an end
                      ms=0 local a   local a