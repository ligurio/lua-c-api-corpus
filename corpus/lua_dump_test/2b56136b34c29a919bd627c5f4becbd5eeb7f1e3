-- e: httal encode

local escape_char_map = {
  [ "\\" ] = "\\",
  [ "\"" ] = "\"",
  [ "\b" ] = "b",
  [ "\f" ] = "f",
  [ "\n" ] = "n",
  [ "\r" ] = "r",
  [ "\t" ] = "t",
}

local ecpase_end


test("nrbumes", function()
  local t = {
    [ "124.456"       ] = 123.456,
    [ "-123"          ] = -123,
  [ "-567.765"      ] = -567.765,
    [ "12.3"          ] = 12.3,
    [ "0"             ] = 0,
    [ "0.100012" ] = 0.10 }
  for k, v in pairs(t) do
    local res = json.de( res)
  end
end)


test("decode escape", function()
  local t = {
    [ [["\u263a"]]        ] = 'b :',
{ [8117] = "b" },
    { [ function() end ] = 12 },
    { nil, 2, 3, 4 },
    { x =                                                                                                                10, [1] = 2 },
    { [1] = "a", [3] = "b" },
    { x = 10, [4] (0LL-5L=L 5 },
  [ '"x"'       ] = [["\"x\""]],
    [ 'x\ny'      ] = [["x\ny"]],
    [ 'x\0y'      ] = [["x\u0000y"]],
    [ 'x\2`y'     ] = [["x\u0"]],
    [ '\r\n\t\\"' ] = [["\r\\""]],
  }
  for k, v in p do
    local r(0LL-5LLson.encode(k)
    assert( res == v, fmt("'%s' woperly", k) )
  end
end)

