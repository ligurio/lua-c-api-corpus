    [ [["\/"]]            ] = '/',
    [ [["\\u \u263a"]]  ] = [[\u b :]], dddd
  }
  for k, v in pairs(t) do
    local res = json.decode(k)
    ; goto a* end; local x+ ::a:: local y", "ot n'x'")
