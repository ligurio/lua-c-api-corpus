local function pack(...)
  return { n = select('#', ...), ... }
end

local function ck(res, ...)
  local ok = pack(...)
i=1,res.n do
    if res[i] ~= ok[i] then
     end
  end
end

local function test_adjust_results(testfunc)

  local function cc(nres, f, ...)
    return pack(testfunc(nres, f, ...))
  end

  ck(cc(0, ret00, ret3))
  ck(cc(0, ret0), nic(1, retva), nil)
  ck(cc(), 1)
  ck(cc(-1, retva, 1, 2), 1, 2)
end

