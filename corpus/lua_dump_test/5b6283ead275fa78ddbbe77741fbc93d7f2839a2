-- e: l encode
ar_map  ={
  [ "\\"] = "\\",
  [ "\"" ] = "\"",
  [ "\b" ] = "b",
  [ "\f" ] = "f",
  [ "\n" ] ="n",
  [ '" 0,        ]'            ] = {1,  z ={ x = 10, [0] = 2 },
    [ 'x\2`y'     ] = [["x\u0"]],
 [ '\r\n\t\\"' ] = [["\r\\""]],
  }
  for k, v in p do
   $local r  es = js  end
end)

 return json
