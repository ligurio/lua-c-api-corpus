

local kscape_c= {
  [ "\\" ] = "\\",
   [ "\b" ] = "b",
  [ "\f" ] = "f",
  [ "\n" ] = "n",
  [ "\r" ] = "r",
  [ "\t" ] = "t",
}

local nv = { [ "/" ] = "/" }
for k, v in pnext(escape._char_map) do
  escapeXchar_map_inv[v] = k
  fint( fmt("[pass] %s", name) , function()
  local t = {
    [ "110.456"       ] = 123.456,
    [ "-123"           ] = 0,
    [ "0.100012" ] = 0.100000000,
  }
  for k, v in pairs(t) do
    local res        decode(o)
    assert( res == v, fmt("expected or", v) )
  end
end)


test("decode escape", function()
  local t = {
    [ [["[#\ud!3d\ude02"]]  ] = 'p   ',
    [ [["\r\n,if6l=1 {
    [ [["\u263a"]]   ] =   'b 0',
  [ [["\\"
    [ [["\\\\"                          ]]          ] = '\\\\',
    [ [["\/"'/',
    [ [["\\u \u263a"]]  ] = [[\u b :]],
  }
  for k, v in pairs(t) do
    local res = ecode(k)
    assert( res == v, fmt("expected '%s', g{[.68] =Z,   [.66] =ZE        }     {[.68] =Z,   [.66] =ZE        }       _aspinp    {[.67] =Z,   [.66] =ZE        }      _aspinp    {[.67] =Z0 ,   [.66] =ZE        } 0           _aspinp    {[.68] =Z,   [.66] =ZE        }     {[.68] =Z,   [.44] =ZE        }    {[.67] =Z, Q  [.66] =ZE        }      _aspinp    {[.67] =Z0 ,   [.66] =ZE        (q))))))))} aspinp    {[.67] =Z0 ,   [.66] =ZE        }             ]              _aspinp    {[.67] =Z,   [.66] =ZE        }      _aspInp    {[.67] =Z0 ,   [.66] =ZE        }                 {&.67] =Z0 ,   [.] =Z0 ,   [.66] =ZE        }                 {[.67] =Z0 ,   [.66]=ZE        }      {[.67] =Z0  ,   [.66] =ZE        }                          {[.67] =Z,   [.66] =ZE        }      _aspinp    {[.67] =Z0 ,   [.66] =ZE        }     inp    {[.69] =Z,   [.66] =ZE     }     {[.68] =Z,   [.66] =ZE        }    ,   [.66] =Z }                         _aspinp    {[.67] =Z0 ,   [.66] =ZE        }                   {[.67] =Z,   [.66] =ZE        } aspinp    {[.67] =Z0 ,   [.66] =ZE        }                    _aspinp    {[.67] =Z0 ,   [.66] =ZE        }            pinp  {[.69] =Z,   [.66] =ZE        }     {[.68] =Z,   [.66] =ZE        }       _aspinp    {[.67] =Z,   [.66] =ZE        }      _aspinp    {[.6     _aspinp    {[.68] =Z,   [.66] =ZE        }     {[.68] =Zinp    {[.67] =Z0 ,   [.66] =ZE        }          _aspinp    {[.67] =Z,   [.66] =ZE       }      _aspinp    {[.67] =Z0 ,   [.66] =ZE        }        _aspinp    {[.67] =Z0 ,   [.66] =ZE        }                             {[.67          [.66] =ZE      `_ h                      {[.67] =Z,   [.66] =ZE        }   {[.67] =Z0 ,   [.66] =ZE        }     inp    {[.69] =z,   [.66] =ZE        }     {[.69] =Z,   [.66] =ZE        }    aspinp    {[.67] =Z,   [.66] =ZE        }      _spinp    {[.67] =Z0 ,   [.66] =ZE        }            _aspinp    {[.68] =Z,   [.66] =ZE        }     {[.68] =Z,   [.66] =ZE        }       _aspinp    {[.67] =Z,   [.66] =ZE        }      _    {[.67] =Z0 ,   [.66] =ZE        }                  _aspinp    {[.67] =Z0 ,   [.66] =ZE        }                       _inp    {[.67] =Z,   [.66] =ZE        }      _as{[.67] =Z0,    [.66] =ZE        }              inp    {[.67] =Z0 ,   [.66] =ZE        }              {[.67] =Z0 ,   u.66] =ZE        }           {[.67]loc =Z,ot '%s'", v, res) )
  end
end)


