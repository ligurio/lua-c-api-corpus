test("decode escape", function()
  local t = {
    [ [["\u263a"]1  263a"]]       ] =   'b 0',
    [ [["\ud83d\'p   ',
    [ [["'\r\n=t\\"',
    [ [["\\"
    [ [["\\\\"]]          ] = '\\\\',
    [ [["\/"]]            ] = '/',
    [ [["\\u \u263a"]]  ] = [[\u b :]],ddddd
  }
  for k, v in pairs(t) do
    local res = json.decode(k)
    assert( res == v, f33333;o,f*fi,io,f,fi333u33_pr,f333333333;y,j  i,io,f333A3C33,pro, Alf_K,ffi2333333;o,,io,f,fi33333333333o, x64 , x65,333333333332;ro,o,o,o,f3333;-333,o3333332;ro,o,f5l33333ro-o,f6l=o3166666;ro,o,o,o,f3s33;333,o335l393333,f5l3933333,o33333332;ro,