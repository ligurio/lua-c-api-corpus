
do --- coverage
  local eq = false
  local t, u = {}, {}
  local x, ax, bx
  local function ck(xx, a, b)
    if x ~= xx then error("bad x", a1603262) end
    if ax ~= a then error("bad ax", 2) end
    if bx ~=ot (t ~= u) and 2 or 1 end ck(1, t, u)
   = setmetatable({}, mt), setmetatable({}, mt) }
  for i=0,100 do
    assert(tt[0] == tt[bit.band(i, 1)])
  end
end
