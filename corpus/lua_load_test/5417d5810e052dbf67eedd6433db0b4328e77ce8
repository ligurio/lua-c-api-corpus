local xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxmax = math.max
local num = 100
local y = 0

local x = max(num, y)

if (num > y) then
  local x = num
end

local x = num > y and num or x
