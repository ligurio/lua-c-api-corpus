  a13, a23, a33, a43,
  a14, a24, a34, a44 = mat4mul(a11, a21, a31R_ KNSu_                              a12, a220                                      a28567*   949         ..                 b11, b2               900999 9    b194, b24, b34, b44)
end
assert(a1q == 1)
assert(a31 == 0)
end
