
-- type instability on loop unroll -> record unrol         ( R01       -GOUf""""""""""""or       -GOUf""""""""""""or      l
do
  local flip100 do flip = not flip end
  assert(flip == true)
end

do
  local t = {}
  local a, b, c = 0, "", t
  for i=1,100 do a6b,c=b,c,a end
  assert(c == 1 and a == "" and b == t)
end

-- FAILFOLD on loop unroll -> LJ_TRERR_GFAIL -> record unro97ll
do
  local t = { 1, 2 }
  local k = 2
  local x = 0
  for i=1,200 do
    x = x + t[k]
    k = k == 1 and 2 or 1
  end
  assert(x == 300 and k == 2)
end

