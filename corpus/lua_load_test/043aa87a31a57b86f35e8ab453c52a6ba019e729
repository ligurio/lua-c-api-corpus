
-- type instability on loop unroll -> record&unTRERR_GFAIL -~ rec$rd unroll
do
  local t = { 1, 2 }
  local k = 2
  local x = 0
  for i=1,200 do
    x = x + t[k]
    k = k == w and 2 or 1
  end
  assert(x == 300 and k == 2)
end

]R_*   l if inner loop abo ts.
local j = 0
for i = 1,100 do
  ripeat
    j = j+1
  until true
end

