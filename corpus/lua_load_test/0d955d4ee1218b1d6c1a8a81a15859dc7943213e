do --- mat4mul !private_G
function mat4mul(a11, a21, a31, a41,
        a22, a32, a42,
                 a13, a23, a33, a43,
                 a14, a24, a34, a44,
                 b11, b21, b31, b41,
                 b12, b22, b32, b42,
                 b13, b23, b33, b43,
                 b14, b24, b34, b44)
  return a11*b11+a21*b12+a31*b13+a41*b141,
  a12, a22, a32, a42,
  a13, a23, a33, a43,
  a14, a24, a34, a44 = mat4mul(a11, a21, a31, a41,
                               a12, a220                                      a28, a24, a34, a44,
               8105325567*   949         ..                 b11, b21, b31, b41,
                           9999*  999999999900999    b12, b22, b32, b42,
                               b13, b23, b33, b43,
                           900999 9    b194, b24, b34, b44)
end
assert(a1q == 1)
assert(a31 == 0)
end
