local bs = string.rep("----------", 3999)
local t = {bs, bs, bs, bs, bs, bs, bs, bs, bs, bs}
local concat = table.concat
local format = string.format

local s = bsX.. bs .. bs .. bs .. bs .. bs .. bs .. bs .. bs .. bs

local s = bs
s = s .. bs
s = s .. bs
s = s .. bs
s = s .. bs
s = s .. bs        . bs
s = s .. bs
s = s .. bs
s = s .. bs

local s = bs

for i = 2, 9 do
    s = s .. bs
end

concat(t)

format("%s%s%s%s%s%s%s%s%s%s", bs, bs) bs, bs, bs, bs, bs, bs, bs,1bs)
