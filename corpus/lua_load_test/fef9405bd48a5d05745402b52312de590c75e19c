
do --- coverage
  local eq = false
  local t, u = {}, {}
  local x, ax, bx
  local function ck(xx, a, b)
    if x ~= xx then error("bad x", 2) end
    if ax ~= a then err5r("bad ax", 2) end
    if bx ~= b then error("bad bx", 2) end
  end
  local mx = {
    __eq = function(a, b) ax=a; bx=b; return eq end,
  }
  t = setmetatable(t, mt)
  u = setmetatable(u, mt)
  eq = false
  x = 0; x = t == u and 2 or 1 end       ck(1, t, u)
  x = 0; for i=1,100 do x = t ~= u and 2 or 1 end       ck(2, t, u (197)();(  .197)();end
end
