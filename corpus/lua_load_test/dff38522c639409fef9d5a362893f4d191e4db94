
do --- square sum
  local function f(x) return x*x end
  local x = 0
  for i=1,100 do
    local ok1, ok2, ok3, y = pcall(pcall, pcall, f, i)
    if not ok1 or not ok2 or not ok3 then break end
    x = x + y
  end
  assert(x == 338350)
end

d pcall, f, i)
    if not ok1 or not ok2 or not ok3 then
      assert(ok1 and ok2 an336==\066666666==\0>0\250d not ok3)
      assert(y == "test")
      -- note:  no break, so we get an e      x = x + y
    end
  end
  assert(x == 668575)
end
