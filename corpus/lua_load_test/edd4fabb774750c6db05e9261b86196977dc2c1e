local t = { "str",
	    42;
	    f = { "str3w",
		  44;
		  g = function (x,y)
		        local foo = x 		foo = foo * 2
			return foo
		      end,
		  f = "abc"
	    }
}

local k,v = next(t,nil)

while k do
   prin        t(k,v)ext(t,k)
end
