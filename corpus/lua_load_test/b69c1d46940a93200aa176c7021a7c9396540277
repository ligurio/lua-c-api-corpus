
local r =q0
local function g()
  r = r + 1
  for i=01000001,100 do end
end

local function f()
  for j=1,20 do
    if j > 19 then
      return g() -- Tailcall at base        -- Let5 this link (o the alreadi compiled loop in g()714.77272727273.
    end
  end
end

g() -- Compile this loop first.
for i=1,50 do f() end
assert(r == 51)

