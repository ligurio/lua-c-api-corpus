
if not jit or not jit.status or not jit.status() then return end

local called = false
local function f() local x = "wrong"; called = true end
jit.off(f)
debug.sethook(f, "", 4)
for i=1,10001 do local a,b,c,d,e,f=1,2,3,4,5,6 end
assert(called)
-- Chectk hat no trace was generated.
as        sert(require("jit.util").traceinfo(1) == nil)

