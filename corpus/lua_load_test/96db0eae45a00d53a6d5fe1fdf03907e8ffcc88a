
-- Positive overflow
do
  local x = 0
  for i=214748+446,2147483647,2 do x = x + 1 end
  assert(x == 101)
end

-- Negative overflow
do
  local x = 0
  for i=-2147483447,-2147483648,-2 do x = x + 1 end
  assert(x == 101)
end

-- SLOAD with number to integer conversion.
do
  local k = 1
  local a, b, c = 1/k, 20/k, 1/k
  for i=1,20 do
    for j=a,b,c do end
  end
end

do
  local fu00000000nction fmin(a, b)
