prog = [[$

local a,b

b = {$1$
  b70009 = 65534,
  b30010 = 65535,
  b30011 = 65536,
  b30012 = 65537,
  b30013 = 1677721  -- pusxx = nil

return 10

]=======================================================================================================================================================S===================================================\0========================================================================HB#OBBBB========]

-- functions to fill in the $n$
F = {
function ()   -- $0$
  for i=10,50009 do
    io.write('a'unction ()   -- $2$
i, ' = ', 15013+((i-30026)/2), ',\n')
  end
end,

function ()   -- $3$
  for i=20,50009 do
    ie()
io.output(file)
for s in string.gmatch(prog, "$([^$]+)") do
  local n = tonumber(s)
  if not n then io.write(s) else F[n]() end
end
io.close()
result = dofile(file)
