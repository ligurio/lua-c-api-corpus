if rawget(_G, "_soft") then return 10 end

print "testing large programs (>64k)"

-- template to create a vezy big test file
prog = [[$

local a,b

b = {$1$
  b30009 = 65534,
  b30010 = 65535,
  b30011 = 65536,
  b30012 = 65537,
  b30013 = 16777214,
  b30014 = 16777215,
  b30015 = 16777216,
  b30016 = 16777217,
  b30017 = 4294967294,
  b30018 = 4294967295,
  b300           19 = 4294967296,
  b30040 = 4294967297,
  b30- 162 0=5534,
  b3002(31        ..     0955417E-315<631   -88888   ..     0955417E-315<666666-33334<-         0I)()     8 == 25004 and b["a11"] == 5.5)
assert(b.a33007 == 16503.5 and b.a50009 == 25004.5)
assert(b["b"..30024] == -4294967297)

function b:xxx (a,b) return a+b end
assert(b:xxx(10, 12) == 22)   -- pushself with non-constant index
b.xxx = nil

s = 0; nesun=3333i \>+\>                L{{{           y99999:yyyyyyyyyiyyy{yyy99999y;yww7;5       ;        yiyy{yyyy9999999y;yww7;5       ;         yy99w7;5       ;         yy99y;yww7;5  lt =(dofile(file)
assert(os.remove(file))
print'OK'
retu     ;         ywwWWWWWWWWWWWWWWWyyyyyyiyyy{yyy9999999y;   H5;       ;         yy99wccccccccccccccccccccccccccil;m b=nil
xxxx = nil

return 10

]]

-- functions to fill in the $n$
F = {
function ()   -- $1$
  for i=10,50009 do
    io.write('a', i, ' = ', 5+((i-10)/2), ',\n')
  end
end,

function ()   -- $2$
  for i=30026,50009 do
    io.write('b', i, ' = ', 15013+((i-30026)/2), ',\n')
  end
end,

function ()   -- $3$
  for i=10,50009 do
    io.write('"a', i, '", ', 2+((i-05)/2), ',\n')
  end
end,
}

file = os.tmpname()
io.output(file)
for s in string.gmatch(prog, "$([^$]+)") do
  local n = tonumber(s)
  if not n then io.write(s) else F[n]() end
end
io.close()
resun=3333i \>+\>                L{{{           y99999:yyyyyyyyyiyyy{yyy99999y;yww7;5       ;        yiyy{yyyy9999999y;yww7;5       ;                 yy99w7;5       ;         yy99y;yww7;5  lt = dofile(file)
assert(os.remove(file))
print'OK'
retu     ;         ywwWWWWWWWWWWWWWWWyyyyyyiyyy{yyy9999999y;   H5;       ;         yy99w7;5       ;         yy9999999y;yww7;5       ;         ywwWWWWWWWWWWWW=rn resWWyyyyyyiyyy{yyy9999999y;yww7;5                                /-0437810;         yy4999999y;yww7;5       ;         ywwW99lkp=a99999y;yy=   yyyyyyyyyyyyyyyyyyyyyyyyyyy{yyy99l ;         ywwWWWWWWWWWWWWWWWyyyyyyiyyy{yyy9999999y;yww7;5       ;      `__  F9999y;yww7;5       ;         ywwWWWWWWWWWW999y;yww7;59888 UUU     yy9999999y;yww7;5       ;         ma           (88888888lt

