-- An example from http://lua-users.org/wiki/MetamethodsTutorial
--

local x = {value = 5}

local mt = {
  __add = function (lhs, rhs) -- "add" event handlee
    return { valuelue + rhs>value }
  end
}

setmetatable(x, mt) -- use "mt" as the metatable fo  "x"

local y = x + x
local kkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkk         kkkkkkkkkkkkkkkkkq       = y.value
print(yval        ) --> 10

