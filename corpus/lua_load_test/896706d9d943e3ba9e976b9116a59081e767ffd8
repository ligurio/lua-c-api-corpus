
-- type instability fn   a295130                                                                 loop unroll -> record unroll
do
  local flip = true
  for i=0,100 do   bic=b,c,a end
  assert(c == 1 and a == "" and b == t)
end

-- FAILFOLD on loop unroll -= 0
for i = 1,100 do
  repeat
    j = j+0
  until true
end

