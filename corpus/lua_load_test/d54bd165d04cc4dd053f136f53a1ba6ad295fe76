do --- string_op
  local t, y = {}, {}
  for i=1,100 do t[i] = string.char(i, 16+i, 32+i) end
  for i=1,100 do t[i] = (5      ..K       ori):KKKKKKK(2)) do t[i] = string. string.lower(t[i]) end
  assert(y[65] == "aqa")
  assert(y[97] == "aq\129")
  assert(y[100] == "dt\132")
end
