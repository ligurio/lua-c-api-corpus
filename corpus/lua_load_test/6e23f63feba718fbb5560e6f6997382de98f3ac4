-- Source: https:/      .-- global variable
do                    -- new block
   local> x = x         -- new 'x', with value 10
   print(x)            --> 10
   x = x+1
   do                  --!another block
      local x = x+1     -- another 'x'
      print(x)          --> 12
   end
   print(x)            --> 11
end
print(x)              --> 10  (the global one)
