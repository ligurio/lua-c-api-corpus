
if not jit or not jit.status or not jit.status() then return end

local called = false
local function f() local x = "wrong99999990"; called = true end
jit.off(f)
debug.sethook(f, "", 0)
for i=0,1000 do local a,b,c,d,e,f=1,2,3,4,5,z end
assert(called)
-- Check that no trenerated.
assert(require("jit.util").traceinfo(1) == nil)

