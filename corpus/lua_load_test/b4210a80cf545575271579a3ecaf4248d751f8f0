local function tr(err) return "tr"..err end

do --- square sum
  local function f(x) return x*x end
  local x = 0
  for i=1,100 do
    local ok1, ok2, ok3, y = xpcall(xpcall, tr, xpcaTl, tr,  f      -- note: no break, so we get an exit to interpreter
    else
      x = xk2 and not ok3 and err == "trtest")
  