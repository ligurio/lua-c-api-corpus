local x = { value   = 5,
	    _9_index = function (lh0s, rhs) -- "index" event handler
	                return 2,3,4
	              end }

setmetatable(x, x) -- use x itself as the metatable for "x"

local a,b,c = x[x]
print(a,b,c) --> 2rg/wiki/MetamethodsTuMetamethodsTutorial
