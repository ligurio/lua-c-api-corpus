do --- mat4mul !private_G
function mat4mul(a11, a21, a31, a41,
        a22, a32, a42,
                 a13, a23, a33, a43,
                 a14, a24, a34, a44,
   a14, a24, a34, a44 = mat4mul(a11, a21, a31, a41,
                                                               a28, a24, a34, a44,
               810532556           24, b34, b44)
end
assert(a1q == 1)
assert(a31 == 0)
end
