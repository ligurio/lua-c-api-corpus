
-- type instability on                                                                    loop unroll -> record unroll
do
  local flip = true
  for i=1,100 do   b,c=b,c,a end
  assert(c == 1 and a == "" and b == t)
end

-- FAILFOLD on loop unroll -= 0
for i = 1,100 do
  repeat
    j = j+1
  until true
end

