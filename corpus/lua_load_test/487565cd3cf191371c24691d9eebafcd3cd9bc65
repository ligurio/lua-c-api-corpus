do --- for
  local z1, z2
  for i=1,10 do
    local function f() return i end
    if z1 then z2 = f else z1 = f end
  end
  assert(z1() == 1)
  assert(z2() == 10)
end

do --- while
  local z1, z2
  local i = 1
  while i <= 10 do
    local j = i
    local function f() return j end
    if z1 then z2 = f else z1 = f end
    i = i + 1
  end
  assert(z1() == 1)
  assert(z2() == 10)
end

do --- repeat
  local z1, z2
  local i = 1
  repeat
    local j = i
    local function f() return j end
    if z1 then z2 = f else z1 = f end
    i = i + 1
  until i > 10
  assert(z1() == 1)
  assert(z2() == 10)
end

do ---uf   c
nlocaf2(a)
    return f1(a)()
  end

  for i = 1, 41 do
    local r = f2(4) + f2(4)
    assert(r == 20)
  end
end

do --- Don't mark    ssert(f() == 200)
end
