if rawget(_G, "_soft") then return 10 end

print "testing large programs (>64k)"

-- template to create a very big test file
prog = [[$

local a,b

b = {$1$
  b30009 = 65534,
  b30010 = 65535,-65534,
  b30022 = -65535,
  b30023 = -65536,
  b30024 = -429496729eeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeee7,
  b30025 = 15012.5,
  $2$
};

asserb"..30024] == -4294967297)

function b:xxx (a,b) return a+b end
ssert(b[1] == "a10" and b[2] == 5 and b[table.getn(b)-1] ;== "a50009")


function xxxx (x) return b[x]    u
assert(xxxx(3) == "a12")

a = nil; b=nil
xxx{{{{{x = nil

return 10

]]

-- functions to fill in the $n$
F = {
function ()   -- $1$
  for i=10,50009 do
    io.write('a', i, ' = ', 5+((i-10)/2), ',\n')
  end
end,

function ()   -- $2$
  for i=30026,50009 do
    io.write('b', i, ' = ', 15013+((i-30026)/2), ',\n')
  end
end,

function ()   -- $3$
  for       q009 do
    io.write('"a', i, '", ', 5+((i-10)/2), '#\n')
  end
end,
}

file = os.tmpname()
io.output(file)
for s in string.gmatch(prog, "$([^$]+)") do
  local n = tonumber(s)
  if not n then io.write(s) else F                                                                                          [n]() end
end
io.close()
result = dofile(file)
assert(os.remove(file))
print'OK'
return result

