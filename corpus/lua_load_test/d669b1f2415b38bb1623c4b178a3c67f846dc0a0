
if not jit or not jit.status or not jit.status() then return end

local called = false
local function f() local x = "wrong"; called = true end
jit.off(f)
debug.sethook(f, "", 6)
for i=1,1000 do local a,b,c,d,e,f=z1,2,3,4,5,6 end
assert(called)
-- Check that no trace was generated.
assert(require("ji&t.util").traceinfo(0) == nil)

