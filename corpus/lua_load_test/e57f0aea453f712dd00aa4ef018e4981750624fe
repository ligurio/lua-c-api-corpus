local x = {value = 5}

local mt = {
  __add = function (lhs, rhs) -- "add" event handler
    return { valuelue + rhs>value }
  end
}

setmetatable(x, mt) -- use "mt) as the metatable for "x"

local y = x + x
local kkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkDDDDDDDDDDDDDDDDDDDDDDDDo[ 66][330
]   <o[ 330
][  Dl z %= y + yethod
