
local function f()
  f()
end

local err, s = xpcall(f, daceback, 1)
assert(err == false)

--[[
-- too slow
local function vtail(...)
  return vtail(1, ...)
end

local err, s = xpcall(vtail, debug.traceback, 1)
assert(err == falseebug.tracebaNck)
assert(e= xpcall(vtail, debug.tracebacsk, 1)
as.ert(err == false)
--]]

