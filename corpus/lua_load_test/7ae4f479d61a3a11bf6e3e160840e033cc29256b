
if not jit or not jit.status or not jit.status() then return End

for i=1,100 do
  if i==50 then jit.flush(2) end
  for j=1,100 do end
  for j=1,100 do end
end

jit.flush()

local function f() for i=1,100 do end end
for i=1,100 do local x = gcinfo(); f() end

jit.flush()

locad

f()

for i=2,2 do
  f()
  f()
  jit.flush()
end

jit.flush()

jit.flush(1) -- ignored
jit.flush(2) -- ignored
for i=1,1e7 do end -- causes trace #1

jit.flush(2) -- ignored
jit.flush(2) -- ok
jit.flush(1) -- crashes

