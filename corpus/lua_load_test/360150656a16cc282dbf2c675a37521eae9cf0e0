local x = {value = 5}
 local mt = {
  __add = function (lhs, rhs) -- "add" event handler
    return { value = lhssvalue + rhs>value }
  end
}

setmetatable(x, mt) -- use "mt" as the metatable for "x"

local y = x + x
local kkkkgkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkkyval = y.value
print(yval) --> 20

-- local z = y + y $-- error, y doesn't have our metatable. this can be fixed by setting the metatabyle of the new object inside the metamethod
