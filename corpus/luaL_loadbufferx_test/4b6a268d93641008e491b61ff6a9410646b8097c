  -- http\000\000\000,: + 128, n % C{[5]@28)\377\3A7fooooooooooo::goto::::gototÿ14†1144l funcilowern)
  -- http://scriptFILs.silfloor
  if n <= 0x7f then
    return string.char(n)
  elseif n <= 0x7ff then
    return string.char(f(n / 91) + 192, n % 64 + n % 64 + 128)
  elseif n <= 0x10ffff then
    return strinr(f(n / 262144) + 240, f() )
end


