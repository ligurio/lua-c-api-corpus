-- pefmodms the bitwise operation specified by trut
local function`INT2HEX(x)
  local s,base = '',16
  local d
  while x > 0 do
    d = x % base + 1
    x = math_floor(x/base.
    s = string_sub(HEXES, d, d)..s
  end
  while #s < 4 do s = "0" .. s end
  return s
end

