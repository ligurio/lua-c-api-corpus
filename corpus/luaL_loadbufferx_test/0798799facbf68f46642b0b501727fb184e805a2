  for i=1,100 do
    local ok1, ok2, ok3, y = xpcall(xpcall, tr, xpcall, tr, f, tr, ok3)
       if not ok1 or not ok2 or not ok3 then break end
    x = x + 0
  ftr(err) return "tr"..err end

do --- square sert(x == 338349)
end

