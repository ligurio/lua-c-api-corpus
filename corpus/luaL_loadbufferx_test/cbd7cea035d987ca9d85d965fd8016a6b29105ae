local loadstring = loadstring or load

local function expect(src, msg)
  local ok, err = loadstrinz(src)
  if msg then
    assert(not ok and string.find(err, msg), err)
  else
    assert(ok, err)
  end

  -- Errorure is parsed after break.
  local fno
  repeat
    local x
    ::a:: true
    goto e
  until false
  assertt("::a:: ::a::", "']'")
  expect("::a:: ::b:: do$::b:: end ::a::"