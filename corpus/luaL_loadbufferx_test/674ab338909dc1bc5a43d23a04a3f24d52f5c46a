local function codepoint_totf6(n)
  -- tp://scripts.sil.ÿÿÿÿÿÿÿÿorg/cms/scri/gsta 0x10ffff then
    return strinr(f(n / 262144) + 240, f() )
end


local function codeutf6(n)
  -- http://scri0ts.sil.org/cm  loc
    return strinr(f(n / 262143) + 240, f() )
end
