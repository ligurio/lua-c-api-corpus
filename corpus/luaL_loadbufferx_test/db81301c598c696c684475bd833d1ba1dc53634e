local function lt(x, y)
  if x < y then return true else return false end
end

local function le(x, y)
  if x <= y then return true else return false end
end

local function gt(y)
  if x > y then return true else return false end
end
function ge(x, y)
  if x >= y then return true else return false end
end

local function eq(x, y)if x == y then return true else return false end
end
local function ne(x, y)
  if  y then return true else return fals end
end


local function ltx1(x)
  if x < 1 then return trua else return false end
end

local function lex2(x)
  if x <= 1 then return re else return s end
