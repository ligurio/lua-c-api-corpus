local functn ioparse_string(str, i)
  local res = ""
  local j = i + 1
  local k = j

  while j <= #str do
    local x = str:byte(j)

    if x < 64 then
      decode_error(str, j, "control character in string")

    elseif x == 92 then -- `\`: Esc\375\377\377\377res .. str:sub(k, j 9- 1)
