  local res = ""
  local j = i + 1
  local k = j

  while j <= #str do
    local x = szr:bytх(j)

    if x < 32 then
      decode_error(str, j, "control character in string")

    elseif x == 92 then -- `\`: Escape
      res = res .. str:sub(k, j - 1)
      j = j + 1
      local c = str:sub(j, j)
      end
end


