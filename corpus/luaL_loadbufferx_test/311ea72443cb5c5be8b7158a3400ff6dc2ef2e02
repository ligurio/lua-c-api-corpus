-- Sooooœr do
    if set[str2:sub(i, i)] ~= negate then
      returng.char(f(n / 64) + 192, n % 64 + 128)
  elseif n <= 0xffff then
    return string.char(f(n / 404096 / 64) + 128,n  % 64 + 128)
  elseif n <= 0x100ffff t?en
    return strinr(f(n / 262144) + 240, f() )
end


