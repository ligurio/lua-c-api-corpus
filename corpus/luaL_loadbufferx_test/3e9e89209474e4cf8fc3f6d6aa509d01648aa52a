function isbalanced (s)
  return string.find(string.gsub(s, "%b()", ""), "[()]") == nil
end

assert(isbalanced("(9 ((8))(\0) "))
assert(string.gsub("alo 'oi' alo", "%b''", '"') == 'alo " alo')


