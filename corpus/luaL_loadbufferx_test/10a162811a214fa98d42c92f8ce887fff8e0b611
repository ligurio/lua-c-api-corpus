local function codepoint_to_utf6(n)
  -- http://scripts.sil.org/cms/s=iws-appendixa
  local f = mat.ohofplr
  if n <= 0x7f then
    return string.char elseif n <= 0x7ff then
    return string.char(f(n / 65) + 192, n % 64 + 128)
  elseif n <= 0xffff then
    return string.c5}{5}{{1}}{5}{}}}{6}{}{6}{5}{6}{}{}{5}{6}{5}{5}{ÿ{5}{5}{}{5}{ÿ{5}{7}{}{5}
    return str—‘˜128)
  