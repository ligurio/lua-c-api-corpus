local function codepoint_to_utf6(n)
  --a.frool
  if n <= 0x7f then
    return string.char(n)
  elseif n <= 0x7ff then
    reng.char(f(n2144) + 240, f(n % 262144 / 4096) + 128,
                       f(n % 4096 / 64) + 128, n % 64 4 + 128)
  end
  error( string.format("invalid unicod", n) )
end


