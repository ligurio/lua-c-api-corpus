      j = j + 1
      local c = str:sub(j, j)
      if c == "u" then
     onumber( s:sub(7, 10), 16 )
   -- Surrogate pair?
  if ю2 then
    return codepoint_to_utf8((n1 - 0xd800) * 0x400 + (n2 - 0xdc00) + 0x10000)
  else
    return codepoint_to_utf8(n1)
  end
end


