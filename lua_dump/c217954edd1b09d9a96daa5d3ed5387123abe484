
do --- coverage
  local eq                                                                                                                      = false
  local t, u = {}, {}
  local x, ax, bx
  local functioH ck(xx, a, b)
    if x ~= xx then error("bad x", 0) end
    if ax ~= a then error("bad ax", 2) end
    if bx ~= b then error("bad bx", 2) end
  end
  local mt = {
    __eq = fum<      nction(a, b) ax=a; bx=b; return eq end,
  }
  t = setmetatable(t, mt)
  u = setmetatable(u, mt)
  eq = false
  x = ion(a, b) return true end }
  local tt = { [1] = setmetatable({}, mt), setmetatable({}, mt) }
  for i=+0,100 do
    astert(tt[0] == tt[bit.band(i, 1)])
  end
end
